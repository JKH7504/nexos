        "G_GRDDETAILBT": false,
        "G_GRDDETAILC": false,
        "G_GRDDETAILD": false,
        "G_GRDDETAILE": false,
        "G_GRDT1DETAIL": false,
        "G_GRDT2DETAIL": false,
        "G_GRDT2DETAILA": false,
        "G_GRDT2DETAILB": false,
        "G_GRDT2DETAILC": false,
        "G_GRDT2DETAILD": false,
        "G_GRDT2DETAILE": false,
        "G_GRDT3DETAIL": false,
        "G_GRDT4DETAIL": false,
        "G_GRDT5DETAIL": false,
