        "G_GRDHITRATE": false
    },
    "rules": {
        "no-cond-assign": 2,
        "no-dupe-args": 2,
        "no-dupe-keys": 2,
        "no-duplicate-case": 2,
        "no-empty": 1,
        "no-ex-assign": 2,
        "no-extra-boolean-cast": 1,
        "no-extra-parens": [1, "functions"],
        "no-extra-semi": 2,
        "no-func-assign": 2,
        "no-obj-calls": 2,
        "no-unreachable": 2,
        "block-scoped-var": 1,
        "no-case-declarations": 1,
        "no-fallthrough": 1,
        "no-floating-decimal": 1,
        "no-lone-blocks": 2,
        "no-loop-func": 2,
        "no-new": 2,
        "no-octal": 2,
        "no-redeclare": [1, {"builtinGlobals": true}],
        "no-return-assign": 2,
        "no-self-assign": 2,
        "no-sequences": 2,
        "no-unmodified-loop-condition": 2,
        "no-unused-expressions": 2,
        "no-useless-concat": 1,
        "no-void": 2,
        "no-with": 2,
        "wrap-iife": [2, "inside"],
        "no-label-var": 2,
        "no-catch-shadow": 2,
        "no-shadow": 2, 
        "no-shadow-restricted-names": 2,
        "no-undef": 1,
        "no-unused-vars": [1, { "vars": "local", "args": "none" }],
        "no-use-before-define": [2, { "functions": false, "classes": false }],
        "camelcase": 1,
        "comma-dangle": [1, "never"],
        "consistent-this": 0,
        "new-parens": 2,
        "no-bitwise": 2,
        "no-nested-ternary": 2, 
        "no-new-object": 2,
        "no-unneeded-ternary": 1,
        "no-whitespace-before-property": 2,
        "operator-linebreak": [1, "before"],
        "quote-props": [1, "consistent"],
        "quotes": [1, "double"],
        "semi": [1, "always"],
        "wrap-regex": 1
    }
}