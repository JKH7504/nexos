function onSave(ajaxData) {

    var resultData = $NC.toObject(ajaxData);
    var oMsg = $NC.getOutMessage(resultData);
    if (oMsg != $ND.C_OK) {
        alert(oMsg);
        return;
    }

    var lastKeyVal = $NC.getGridLastKeyVal(G_GRDT1MASTER, {
        selectKey: [
            "BU_CD",
            "OUTBOUND_DATE",
            "OUTBOUND_NO"
        ]
    });
    window._Inquiry();
    G_GRDT1MASTER.lastKeyVal = lastKeyVal;
}

function setUserProgramPermission() {

    var enableT1 = G_GRDT1MASTER.data.getLength() > 0;
    var enableT2 = G_GRDT2MASTER.data.getLength() > 0;
    var canSave = $NC.getProgramPermission().canSave;
    var canConfirmCancel = $NC.getProgramPermission().canConfirmCancel;

    $NC.setEnable("#btnCancelOrderFw", enableT1 && canSave);
    $NC.setEnable("#btnCancelOrderBw", enableT1 && canSave);
    $NC.setEnable("#btnCancelOrderRestore", enableT2 && canConfirmCancel);
}