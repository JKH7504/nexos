<!DOCTYPE HTML>
<html lang="ko">
<head>
<!-- ${NEXOS_META} -->
<!-- ${NEXOS_LIBRARY} ${} ${../../../} -->
<!-- PROGRAM CSS/JS -->
<!-- <link rel="stylesheet" href="css/EDR04020E0.css" /> -->
<script src="js/EDR04020E0.js"></script>
</head>
<body>
    <!-- 조회 조건 -->
    <div id="ctrConditionView" class="ctrCondition">
        <table class="ctrInner">
            <tr>
                <td>
                    <div id="lblQPopup_Bu_Cd" class="lblCondition">사업부</div>
                    <input id="edtQBu_Cd" class="edtKey" type="text" style="width: 60px;" />
                    <input id="btnQBu_Cd" class="btnIcoPopup" type="button" />
                    <input id="edtQBu_Nm" class="edtDisplay" type="text" disabled="disabled" style="width: 130px;" />
                </td>
                <td>
                    <div id="lblQDefine_No" class="lblCondition">수신정의</div>
                    <select id="cboQDefine_No" class="cboKey" style="width: 230px;"></select>
                </td>
            </tr>
            <tr>
                <td>
                    <div id="lblQRecv_Date" class="lblCondition">수신일자</div>
                    <div id="divQT1_Recv_Date" class="ctrGroup">
                        <input id="dtpQRecv_Date1" class="dtpKey" type="text" style="width: 100px;" />
                        <span class="lblTerm">▶</span>
                        <input id="dtpQRecv_Date2" class="dtpKey" type="text" style="width: 100px;" />
                    </div>
                    <div id="divQT2_Recv_Date" class="ctrGroup" style="width: 220px;">
                        <input id="dtpQRecv_Date" class="dtpKey" type="text" style="width: 100px;" />
                    </div>
                </td>
                <td>
                    <div id="divViewDiv">
                        <div id="lblQView_Div" class="lblCondition">데이터구분</div>
                        <div class="ctrGroupBox" style="width: 230px;">
                            <input id="rgbQView_Div1" name="rgbQView_Div" class="rgbNormal" type="radio" value="1" />
                            전체수신내역
                            <input id="rgbQView_Div2" name="rgbQView_Div" class="rgbNormal" type="radio" value="2" />
                            오류수신내역
                        </div>
                    </div>
                </td>
            </tr>
        </table>
    </div>
    <div id="divMasterView" class="ctrNormal">
        <!-- Tab Handler -->
        <ul>
            <li id="divTab1"><a href="#divT1TabSheetView">수신처리내역</a></li>
            <li id="divTab2"><a href="#divT2TabSheetView">수신내역</a></li>
        </ul>
        <!-- 수신처리내역 -->
        <div id="divT1TabSheetView">
            <!-- split 상단-->
            <div id="divMaster">
                <table class="ctrTitleBar">
                    <tr>
                        <td id="ctrTitle_grdT1Master" class="lblTitle">수신처리내역</td>
                        <td>
                            <input id="btnRecv" class="btnImgNormal" type="button" value="수신처리" style="width: 80px;" />
                            <input id="btnErrorProcess" class="btnImgNormal" type="button" value="오류내역처리" style="width: 105px;" />
                            <input id="btnClose" class="btnImgCancel" type="button" value="종결처리" style="width: 80px;" />
                        </td>
                        <td id="ctrRows_grdT1Master" class="lblRows">0/0 Rows</td>
                    </tr>
                </table>
                <div id="grdT1Master"></div>
            </div>
            <!-- split 하단-->
            <div id="divSplitter">
                <div id="divT1Detail" class="ctrLeft" style="border-width: 0;">
                    <!-- 수신내역 -->
                    <table class="ctrTitleBar">
                        <tr>
                            <td id="ctrTitle_grdT1Detail" class="lblTitle">수신내역</td>
                            <td id="ctrRows_grdT1Detail" class="lblRows">0/0 Rows</td>
                        </tr>
                    </table>
                    <div id="grdT1Detail"></div>
                </div>
                <div id="divT1Sub" class="ctrRight" style="border-width: 0 0 0 1px; display: none;">
                    <!-- 오류내역 -->
                    <table class="ctrTitleBar">
                        <tr>
                            <td id="ctrTitle_grdT1Sub" class="lblTitle">오류내역</td>
                            <td id="ctrRows_grdT1Sub" class="lblRows">0/0 Rows</td>
                        </tr>
                    </table>
                    <div id="grdT1Sub"></div>
                </div>
            </div>
        </div>
        <!-- 수신내역 -->
        <div id="divT2TabSheetView">
            <table class="ctrTitleBar">
                <tr>
                    <td id="ctrTitle_grdT2Master" class="lblTitle">수신내역</td>
                    <td id="ctrRows_grdT2Master" class="lblRows">0/0 Rows</td>
                </tr>
            </table>
            <div id="grdT2Master"></div>
        </div>
    </div>
</body>
</html>
