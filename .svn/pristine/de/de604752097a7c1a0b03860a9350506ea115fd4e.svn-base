<!DOCTYPE HTML>
<html lang="ko">
<head>
<!-- ${NEXOS_META} -->
<!-- ${NEXOS_LIBRARY} ${} ${../../../} -->
<!-- PROGRAM CSS/JS -->
<!-- <link rel="stylesheet" href="css/CMC04100E0.css" /> -->
<script src="js/CMC04100E0.js"></script>
</head>
<body>
    <div id="ctrConditionView" class="ctrCondition">
        <table class="ctrInner">
            <tr>
                <td>
                    <div id="lblQPopup_Bu_Cd" class="lblCondition">사업부</div>
                    <input id="edtQBu_Cd" class="edtKey" type="text" style="width: 60px;" />
                    <input id="btnQBu_Cd" class="btnIcoPopup" type="button" />
                    <input id="edtQBu_Nm" class="edtDisplay" type="text" disabled="disabled" style="width: 120px;" />
                    <input id="edtQCust_Cd" class="edtHidden" type="text" disabled="disabled" />
                </td>
                <td>
                    <div id="lblQPopup_Delivery_Cd" class="lblCondition">배송처</div>
                    <input id="edtQDelivery_Cd" class="edtNormal" type="text" style="width: 60px;" />
                    <input id="btnQDelivery_Cd" class="btnIcoPopup" type="button" />
                    <input id="edtQDelivery_Nm" class="edtDisplay" type="text" disabled="disabled" style="width: 120px;" />
                </td>
                <td>
                    <div id="lblQDeal_Div" class="lblCondition">거래구분</div>
                    <div class="ctrGroupBox">
                        <input id="chkQDeal_Div1" name="chkQDeal_Div" class="chkNormal" type="checkbox" value="1" />
                        거래진행
                        <input id="chkQDeal_Div2" name="chkQDeal_Div" class="chkNormal" type="checkbox" value="2" />
                        거래종료
                        <input id="chkQDeal_Div3" name="chkQDeal_Div" class="chkNormal" type="checkbox" value="3" />
                        조회종료
                    </div>
                </td>
            </tr>
            <tr>
                <td>
                    <div id="lblQMall_Item_Cd" class="lblCondition">몰상품코드</div>
                    <input id="edtQMall_Item_Cd" class="edtNormal" type="text" style="width: 210px;" />
                </td>
                <td>
                    <div id="lblQMall_Item_Nm" class="lblCondition">몰상품명</div>
                    <input id="edtQMall_Item_Nm" class="edtNormal" type="text" style="width: 210px;" />
                </td>
                <td>
                    <div id="lblQItem_Cd" class="lblCondition">상품코드</div>
                    <input id="edtQItem_Cd" class="edtNormal" type="text" style="width: 230px;" />
                </td>
            </tr>
        </table>
        <!-- 추가 조건 -->
        <table id="ctrAdditionalConditionView" class="ctrAdditionalCondition">
            <tr>
                <td>
                    <div id="lblQItem_Nm" class="lblCondition">상품명</div>
                    <input id="edtQItem_Nm" class="edtNormal" type="text" style="width: 210px;" />
                </td>
            </tr>
        </table>
    </div>
    <div id="divMasterView" class="ctrNormal">
        <!-- Tab Handler -->
        <ul>
            <li id="divTab1"><a href="#divT1TabSheetView">몰상품등록</a></li>
            <li id="divTab2"><a href="#divT2TabSheetView">몰상품내역</a></li>
        </ul>
        <!-- 몰상품등록 -->
        <div id="divT1TabSheetView">
            <div id="divAdditionalView" class="ctrAdditional top styRight">
                <div class="ctrGroupContent">
                    <input id="btnDownloadXLSFormat" class="btnImgFile" type="button" value="엑셀포맷 다운로드" />
                    <input id="btnUploadXLS" class="btnImgFile" type="button" value="엑셀 업로드" />
                </div>
            </div>
            <div id="divSplitter" class="ctrNormal" style="border-width: 0">
                <div>
                    <table class="ctrTitleBar">
                        <tr>
                            <td id="ctrTitle_grdT1Master" class="lblTitle">몰상품 내역</td>
                            <td id="ctrRows_grdT1Master" class="lblRows">0/0 Rows</td>
                        </tr>
                    </table>
                    <div id="grdT1Master"></div>
                </div>
                <div id="divDetailView">
                    <table class="ctrTitleBar">
                        <tr>
                            <td id="ctrTitle_grdT1Detail" class="lblTitle">몰상품 구성</td>
                            <td id="ctrRows_grdT1Detail" class="lblRows">0/0 Rows</td>
                        </tr>
                    </table>
                    <div id="grdT1Detail"></div>
                </div>
            </div>
        </div>
        <!-- 몰상품내역 -->
        <div id="divT2TabSheetView">
            <div>
                <table class="ctrTitleBar">
                    <tr>
                        <td id="ctrTitle_grdT2Master" class="lblTitle">몰상품 내역</td>
                        <td id="ctrRows_grdT2Master" class="lblRows">0/0 Rows</td>
                    </tr>
                </table>
                <div id="grdT2Master"></div>
            </div>
        </div>
    </div>
</body>
</html>
