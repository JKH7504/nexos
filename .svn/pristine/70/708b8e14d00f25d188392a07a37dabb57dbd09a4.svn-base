/**
 * 프로그램 사용 권한 설정
 */
function setUserProgramPermission() {

    // 프로그램 사용 권한
    var permission = $NC.G_VAR.G_PARAMETER.P_PERMISSION;

    // 수기등록일 경우
    if ($NC.G_VAR.masterData.DATA_DIV == "00") {
        if ($NC.G_VAR.G_PARAMETER.P_POLICY_LO110 != $ND.C_YES) {
            $NC.setEnable("#btnEntryNew", false);
            $NC.setEnable("#btnStockPopup", false);
        }
    }
    // 인터페이스 수신일 경우
    else {
        // 신규 추가는 불가
        $NC.setEnable("#btnEntryNew", false);
        $NC.setEnable("#btnStockPopup", false);
        // 수정 삭제는 정책 기준
        if ($NC.G_VAR.G_PARAMETER.P_POLICY_LO113 != "2") {
            $NC.setEnable("#btnEntryDelete", false);
        } else {
            $NC.setEnable("#btnEntryDelete", permission.canDelete);
        }
    }
    $NC.setEnable("#btnEntrySave", permission.canSave);
}

/**
 * 전역 변수에 정책 값 정보 세팅
 */
function setPolicyValInfo() {

    $NC.setPolicyValInfo({
        P_CENTER_CD: $NC.G_VAR.masterData.CENTER_CD,
        P_BU_CD: $NC.G_VAR.masterData.BU_CD
    }, function() {
        if ($NC.G_VAR.policyVal.LO150 != "0000") {
            var deadlineTime = $NC.G_VAR.policyVal.LO150.substring(0, 2) + ":" + $NC.G_VAR.policyVal.LO150.substring(2);
            $NC.setValue("#lblOrderDeadline", $NC.getDisplayMsg("JS.LOB01011P0.XXX", "[발주마감시간: " + deadlineTime + "] 해당 시간 전까지만 출고예정 신규 등록 가능합니다.",
                deadlineTime));
            $NC.setVisible("#lblOrderDeadline");
        } else {
            $NC.setVisible("#lblOrderDeadline", false);
        }
    });
}