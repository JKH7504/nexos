/**
 * 저장
 */
function _Save() {

    if ($NC.isNull($NC.G_VAR.masterData.CENTER_CD)) {
        alert($NC.getDisplayMsg("JS.LOB01011P0.005", "물류센터가 지정되어 있지 않습니다. 다시 작업하십시오."));
        return;
    }

    if ($NC.isNull($NC.G_VAR.masterData.BU_CD)) {
        alert($NC.getDisplayMsg("JS.LOB01011P0.006", "사업부가 지정되어 있지 않습니다. 다시 작업하십시오."));
        return;
    }

    if ($NC.isNull($NC.G_VAR.masterData.ORDER_DATE)) {
        alert($NC.getDisplayMsg("JS.LOB01011P0.007", "먼저 예정일자를 입력하십시오."));
        $NC.setFocus("#dtpOrder_Date");
        return;
    }

    if ($NC.isNull($NC.G_VAR.masterData.DELIVERY_CD)) {
        alert($NC.getDisplayMsg("JS.LOB01011P0.002", "먼저 배송처 코드를 입력하십시오."));
        $NC.setFocus("#edtDelivery_Cd");
        return;
    }

    if ($NC.isNull($NC.G_VAR.masterData.RDELIVERY_CD)) {
        alert($NC.getDisplayMsg("JS.LOB01011P0.003", "먼저 실배송처 코드를 입력하십시오."));
        $NC.setFocus("#edtRDelivery_Cd");
        return;
    }

    if ($NC.isNull($NC.G_VAR.masterData.INOUT_CD)) {
        alert($NC.getDisplayMsg("JS.LOB01011P0.008", "먼저 출고구분을 선택하십시오."));
        $NC.setFocus("#cboInout_Cd");
        return;
    }

    if ($NC.isNull($NC.G_VAR.masterData.CAR_DIV)) {
        alert($NC.getDisplayMsg("JS.LOB01011P0.XXX", "먼저 차량구분을 선택하십시오."));
        $NC.setFocus("#cboCar_Div");
        return;
    }

    // 마지막 선택 Row Validation 체크
    if (!$NC.isGridValidLastRow(G_GRDDETAIL)) {
        return;
    }

    if (G_GRDDETAIL.data.getLength() == 0) {
        alert($NC.getDisplayMsg("JS.LOB01011P0.004", "저장할 데이터가 없습니다."));
        return;
    }

    if (!confirm($NC.getDisplayMsg("JS.LOB01011P0.009", "저장하시겠습니까?"))) {
        return;
    }

    var dsD = [];
    var dsCU = [];
    // 필터링 된 데이터라 전체 데이터를 기준으로 처리
    var dsTarget = G_GRDDETAIL.data.getItems();
    var dsDetail, rowData;
    for (var rIndex = 0, rCount = dsTarget.length; rIndex < rCount; rIndex++) {
        rowData = dsTarget[rIndex];
        if (rowData.CRUD == $ND.C_DV_CRUD_R) {
            continue;
        } else if (rowData.CRUD == $ND.C_DV_CRUD_D) {
            dsDetail = dsD;
        } else {
            dsDetail = dsCU;
        }
        dsDetail.push({
            P_CENTER_CD: $NC.G_VAR.masterData.CENTER_CD,
            P_BU_CD: $NC.G_VAR.masterData.BU_CD,
            P_ORDER_DATE: $NC.G_VAR.masterData.ORDER_DATE,
            P_ORDER_NO: $NC.G_VAR.masterData.ORDER_NO,
            P_LINE_NO: rowData.LINE_NO,
            P_OUTBOUND_STATE: rowData.OUTBOUND_STATE,
            P_BRAND_CD: rowData.BRAND_CD,
            P_ITEM_CD: rowData.ITEM_CD,
            P_ITEM_STATE: rowData.ITEM_STATE,
            P_ITEM_LOT: rowData.ITEM_LOT,
            P_QTY_IN_BOX: rowData.QTY_IN_BOX,
            P_OUT_UNIT_DIV_F: rowData.OUT_UNIT_DIV_F,
            P_ORDER_QTY: rowData.ORDER_QTY,
            P_ENTRY_QTY: rowData.ENTRY_QTY,
            P_VALID_DATE: rowData.VALID_DATE,
            P_BATCH_NO: rowData.BATCH_NO,
            P_SUPPLY_PRICE: rowData.SUPPLY_PRICE,
            P_DC_PRICE: rowData.DC_PRICE,
            P_APPLY_PRICE: rowData.APPLY_PRICE,
            P_SUPPLY_AMT: rowData.SUPPLY_AMT,
            P_VAT_YN: rowData.VAT_YN,
            P_VAT_AMT: rowData.VAT_AMT,
            P_DC_AMT: rowData.DC_AMT,
            P_TOTAL_AMT: rowData.TOTAL_AMT,
            P_ITEM_ORDER_DIV: rowData.ITEM_ORDER_DIV,
            P_BU_DATE: rowData.BU_DATE,
            P_BU_NO: rowData.BU_NO,
            P_BU_LINE_NO: rowData.BU_LINE_NO,
            P_BU_KEY: rowData.BU_KEY,
            P_BU_DATETIME: rowData.BU_DATETIME,
            P_SO_DATE: rowData.SO_DATE,
            P_SO_NO: rowData.SO_NO,
            P_SO_LINE_NO: rowData.SO_LINE_NO,
            P_SO_KEY: rowData.SO_KEY,
            P_ORG_ORDER_DATE: $NC.G_VAR.masterData.ORG_ORDER_DATE,
            P_ORG_ORDER_NO: $NC.G_VAR.masterData.ORG_ORDER_NO,
            P_ORG_BU_NO: rowData.ORG_BU_NO,
            P_CSCL_REQ_YN: rowData.CSCL_REQ_YN,
            P_REMARK1: rowData.REMARK1,
            P_SPARE1_NOTE: rowData.SPARE1_NOTE,
            P_SPARE2_NOTE: rowData.SPARE2_NOTE,
            P_SPARE3_NOTE: rowData.SPARE3_NOTE,
            P_SPARE4_NOTE: rowData.SPARE4_NOTE,
            P_SPARE5_NOTE: rowData.SPARE5_NOTE,
            P_SPARE6_NOTE: rowData.SPARE6_NOTE,
            P_SPARE7_NOTE: rowData.SPARE7_NOTE,
            P_SPARE8_NOTE: rowData.SPARE8_NOTE,
            P_SPARE9_NOTE: rowData.SPARE9_NOTE,
            P_CRUD: rowData.CRUD
        });
    }
    dsDetail = dsD.concat(dsCU);
    if ($NC.G_VAR.masterData.CRUD == $ND.C_DV_CRUD_R && dsDetail.length == 0) {
        alert($NC.getDisplayMsg("JS.LOB01011P0.010", "데이터 수정 후 저장하십시오."));
        return;
    }

    $NC.serviceCall("/LOB01010E0/save.do", {
        P_DS_MASTER: {
            P_CENTER_CD: $NC.G_VAR.masterData.CENTER_CD,
            P_BU_CD: $NC.G_VAR.masterData.BU_CD,
            P_ORDER_DATE: $NC.G_VAR.masterData.ORDER_DATE,
            P_ORDER_NO: $NC.G_VAR.masterData.ORDER_NO,
            P_INOUT_CD: $NC.G_VAR.masterData.INOUT_CD,
            P_INOUT_GRP: $NC.G_VAR.masterData.INOUT_CD_SUB,
            P_OUTBOUND_STATE: $NC.G_VAR.masterData.OUTBOUND_STATE,
            P_CUST_CD: $NC.G_VAR.masterData.CUST_CD,
            P_ORDER_DIV: $NC.G_VAR.masterData.ORDER_DIV,
            P_ERP_BATCH: $NC.G_VAR.masterData.ERP_BATCH,
            P_CARRIER_CD: $NC.G_VAR.masterData.CARRIER_CD,
            P_CAR_DIV: $NC.G_VAR.masterData.CAR_DIV,
            P_DELIVERY_CD: $NC.G_VAR.masterData.DELIVERY_CD,
            P_RDELIVERY_CD: $NC.G_VAR.masterData.RDELIVERY_CD,
            P_INOUT_DATE: $NC.G_VAR.masterData.INOUT_DATE,
            P_DIRECT_YN: $NC.G_VAR.masterData.DIRECT_YN,
            P_BU_DATE: $NC.G_VAR.masterData.BU_DATE,
            P_BU_NO: $NC.G_VAR.masterData.BU_NO,
            P_SO_DATE: $NC.G_VAR.masterData.SO_DATE,
            P_SO_NO: $NC.G_VAR.masterData.SO_NO,
            P_WB_NO: $NC.G_VAR.masterData.WB_NO,
            P_ORG_ORDER_DATE: $NC.G_VAR.masterData.ORG_ORDER_DATE,
            P_ORG_ORDER_NO: $NC.G_VAR.masterData.ORG_ORDER_NO,
            P_DATA_DIV: $NC.G_VAR.masterData.DATA_DIV,
            P_REMARK1: $NC.G_VAR.masterData.REMARK1,
            P_CRUD: $NC.G_VAR.masterData.CRUD
        },
        P_DS_DETAIL: dsDetail,
        P_PROCESS_CD: $NC.G_VAR.G_PARAMETER.P_PROCESS_CD,
        P_USER_ID: $NC.G_USERINFO.USER_ID
    }, onSave);
}

/**
 * 삭제
 */
function _Delete() {

    if (G_GRDDETAIL.data.getLength() == 0 || $NC.isNull(G_GRDDETAIL.lastRow)) {
        alert($NC.getDisplayMsg("JS.LOB01011P0.011", "삭제할 데이터가 없습니다."));
        return;
    }

    var rowData = G_GRDDETAIL.data.getItem(G_GRDDETAIL.lastRow);
    // 신규 데이터일 경우 Grid 데이터만 삭제, 그외 CRUD를 "D"로 변경
    if ($NC.deleteGridRowData(G_GRDDETAIL, rowData, true) > 0) {
        // 같은 상품이 그리드에 있을 경우, 해당 상품을 삭제 했을 경우, 재계산하여 가용재고량과 비교하기 위해 아래 로직 추가
        for (var rIndex = 0, rCount = G_GRDDETAIL.data.getLength(); rIndex < rCount; rIndex++) {
            grdDetailOnCheckPStock(rIndex, rIndex != G_GRDDETAIL.lastRow); // 마지막 Row와 같으면 출고가용량 표시
        }
    }
    // 데이터가 모두 삭제되었으면 출고가능량 정보 초기화
    else {
        grdDetailOnCheckPStock(-1); // 출고가용량정보 초기화
    }
}

/**
 * 마스터 데이터 변경시 처리
 */
function masterDataOnChange(e, args) {

    switch (args.col) {
        case "INOUT_CD":
            // 입출고그룹 속성 취득
            var ATTR01_CD = $NC.getComboData("#cboInout_Cd", -1).ATTR01_CD;
            // 입출고그룹이 다를 경우 배송처/실배송처 클리어
            if ($NC.G_VAR.masterData.INOUT_CD_SUB != ATTR01_CD) {
                $NC.setValue("#edtDelivery_Cd");
                $NC.setValue("#edtRDelivery_Cd");
                $NC.setValue("#edtDelivery_Nm");
                $NC.setValue("#edtRDelivery_Nm");
                $NC.G_VAR.masterData.INOUT_CD_SUB = ATTR01_CD;
                $NC.G_VAR.masterData.RDELIVERY_CD = "";
            }
            $NC.G_VAR.masterData.INOUT_CD = args.val;
            break;
        case "ORDER_DIV":
            $NC.G_VAR.masterData.ORDER_DIV = args.val;
            break;
        case "CAR_DIV":
            $NC.G_VAR.masterData.CAR_DIV = args.val;
            break;
        case "ORDER_DATE":
            $NC.setValueDatePicker(args.view, args.val, $NC.getDisplayMsg("JS.LOB01011P0.012", "예정일자를 정확히 입력하십시오."));
            $NC.G_VAR.masterData.ORDER_DATE = $NC.getValue(args.view);
            $NC.G_VAR.masterData.ORG_ORDER_DATE = $NC.getValue(args.view);
            break;
        case "DELIVERY_CD":
            $NP.onDeliveryChange(args.val, {
                P_CUST_CD: $NC.G_VAR.G_PARAMETER.P_CUST_CD,
                P_DELIVERY_CD: args.val,
                P_DELIVERY_DIV: $NC.G_VAR.masterData.INOUT_CD_SUB === $ND.C_INOUT_GRP_D2 ? "91" : $ND.C_ALL, // 수송출고일 경우 91-물류센터
                P_VIEW_DIV: "1"
            }, onDeliveryPopup);
            return;
        case "RDELIVERY_CD":
            $NP.onDeliveryChange(args.val, {
                P_CUST_CD: $NC.G_VAR.G_PARAMETER.P_CUST_CD,
                P_DELIVERY_CD: args.val,
                P_DELIVERY_DIV: $NC.G_VAR.masterData.INOUT_CD_SUB === $ND.C_INOUT_GRP_D2 ? "91" : $ND.C_ALL, // 수송출고일 경우 91-물류센터
                P_VIEW_DIV: "1"
            }, onRDeliveryPopup);
            return;
        case "REMARK1":
            $NC.G_VAR.masterData.REMARK1 = args.val;
            break;
    }

    if ($NC.G_VAR.masterData.CRUD == $ND.C_DV_CRUD_R) {
        $NC.G_VAR.masterData.CRUD = $ND.C_DV_CRUD_U;
    }
}

