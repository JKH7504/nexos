/**
 * 화면 초기화 - 화면 로드시 자동 호출 됨
 */
function _Initialize() {

    // 단위화면에서 사용될 일반 전역 변수 정의
    $NC.setGlobalVar({
        autoResizeView: {
            container: "#divDetailView",
            grids: [
                "#grdDetail"
            ],
            exceptHeight: $NC.getViewHeight("#ctrPopupView")
        },
        // 체크할 정책 값
        policyVal: {
            LO150: "" // [B2B]발주마감시각
        },
        // 마스터 데이터
        masterData: null
    });

    // 현재고 검색 조건.상품상태 콤보 세팅
    $NC.setInitCombo("/WC/getDataSet.do", {
        P_QUERY_ID: "WC.POP_CMCODE",
        P_QUERY_PARAMS: {
            P_COMMON_GRP: "ITEM_STATE",
            P_COMMON_CD: $ND.C_ALL,
            P_ATTR01_CD: "1",
            P_VIEW_DIV: "1" // 1:등록팝업, 2:조회팝업
        }
    }, {
        selector: "#cboQItem_State",
        codeField: "COMMON_CD",
        nameField: "COMMON_NM",
        fullNameField: "COMMON_CD_F",
        addAll: true,
        onComplete: function() {
            $NC.setValue("#cboQItem_State", $ND.C_BASE_ITEM_STATE);
        }
    });

    // 버튼 클릭 이벤트 연결
    $("#btnClose").click(onCancel); // 닫기버튼
    $("#btnDelivery_Cd").click(showDeliveryPopup); // 배송처 검색 버튼
    $("#btnRDelivery_Cd").click(showRDeliveryPopup); // 실배송처 검색 버튼
    $("#btnEntryNew").click(_New); // 그리드 행 추가 버튼
    $("#btnEntryDelete").click(_Delete); // 그리드 행 삭제버튼
    $("#btnEntrySave").click(_Save); // 저장 버튼
    $("#btnStockPopup").click(showStockOverlay);// 재고에서 선택버튼 클릭
    $("#btnStockAddItem").click(btnStockAddItemOnClick); // 재고팝업에서 확인버튼클릭
    $("#btnStockSearch").click(_Inquiry); // 재고팝업에서 조회버튼클릭
    $("#btnStockOverlayClose").click(function() { // 재고에서 선택 창 닫기
        $("#divStockOverlay").hide();
    });

    $("#btnQBrand_Cd").click(showBuBrandStockPopup); // 브랜드검색 버튼 클릭
    $("#btnQItem_Cd").click(showItemStockPopup); // 상품검색 버튼 클릭

    $NC.setEnable("#edtCenter_Cd_F", false);
    $NC.setEnable("#edtBu_Cd", false);
    $NC.setEnable("#edtOrder_No", false);
    $NC.setEnable("#edtBu_Date", false);
    $NC.setEnable("#edtBu_No", false);

    $NC.setInitDatePicker("#dtpOrder_Date"); // 예정일자

    // 그리드 초기화
    grdDetailInitialize();
    grdStockInitialize();

    // 신규/삭제/저장 버튼 툴팁 세팅
    $NC.setTooltip("#btnEntryNew", $NC.getDisplayMsg("JS.LOB01011P0.XXX", "신규"));
    $NC.setTooltip("#btnEntryDelete", $NC.getDisplayMsg("JS.LOB01011P0.XXX", "삭제"));
    $NC.setTooltip("#btnEntrySave", $NC.getDisplayMsg("JS.LOB01011P0.XXX", "저장"));
}

/**
 * 등록팝업 Open 시 호출 됨
 */
function _OnLoaded() {

    $NC.setValue("#edtCenter_Cd_F", $NC.G_VAR.G_PARAMETER.P_CENTER_CD_F);
    $NC.setValue("#edtCenter_Cd", $NC.G_VAR.G_PARAMETER.P_CENTER_CD);
    $NC.setValue("#edtBu_Cd", $NC.G_VAR.G_PARAMETER.P_BU_CD);
    $NC.setValue("#edtBu_Nm", $NC.G_VAR.G_PARAMETER.P_BU_NM);
    $NC.setValue("#edtCust_Cd", $NC.G_VAR.G_PARAMETER.P_CUST_CD);
    $NC.setValue("#dtpOrder_Date", $NC.G_VAR.G_PARAMETER.P_ORDER_DATE);

    // 신규 등록
    if ($NC.G_VAR.G_PARAMETER.P_PROCESS_CD == $ND.C_PROCESS_ORDER_CREATE) {

        var ORDER_DATE = $NC.getValue("#dtpOrder_Date");
        // 마스터 데이터 세팅
        $NC.G_VAR.masterData = {
            CENTER_CD: $NC.G_VAR.G_PARAMETER.P_CENTER_CD,
            BU_CD: $NC.G_VAR.G_PARAMETER.P_BU_CD,
            ORDER_DATE: ORDER_DATE,
            ORDER_NO: "",
            INOUT_CD: "", // INOUT_CD,
            OUTBOUND_STATE: $ND.C_STATE_ORDER,
            CUST_CD: $NC.G_VAR.G_PARAMETER.P_CUST_CD,
            ORDER_DIV: "",
            ERP_BATCH: "",
            CARRIER_CD: "",
            CAR_DIV: "",
            DELIVERY_CD: "",
            RDELIVERY_CD: "",
            INOUT_DATE: "",
            DIRECT_YN: "",
            BU_DATE: "",
            BU_NO: "",
            BU_DATETIME: "",
            SO_DATE: "",
            SO_NO: "",
            WB_NO: "",
            ORG_ORDER_DATE: ORDER_DATE,
            ORG_ORDER_NO: "",
            ORG_BU_NO: "",
            DATA_DIV: "00",
            REMARK1: "",
            CRUD: $ND.C_DV_CRUD_C
        };

        $NC.setFocus("#edtDelivery_Cd");

    }
    // 예정 -> 등록, 등록 수정
    else {
        // 마스터 데이터 세팅
        var dsMaster = $NC.G_VAR.G_PARAMETER.P_MASTER_DS;
        $NC.setValue("#dtpOrder_Date", dsMaster.ORDER_DATE);
        $NC.setValue("#edtOrder_No", dsMaster.ORDER_NO);
        $NC.setValue("#edtDelivery_Cd", dsMaster.DELIVERY_CD);
        $NC.setValue("#edtDelivery_Nm", dsMaster.DELIVERY_NM);
        $NC.setValue("#edtRDelivery_Cd", dsMaster.RDELIVERY_CD);
        $NC.setValue("#edtRDelivery_Nm", dsMaster.RDELIVERY_NM);
        // $NC.setValue("#cboInout_Cd", dsMaster.INOUT_CD);
        // $NC.setValue("#cboOrder_Div", dsMaster.ORDER_DIV);
        $NC.setValue("#edtInout_Date", dsMaster.INOUT_DATE);
        $NC.setValue("#edtBu_Date", dsMaster.BU_DATE);
        $NC.setValue("#edtBu_No", dsMaster.BU_NO);
        $NC.setValue("#edtSo_Date", dsMaster.SO_DATE);
        $NC.setValue("#edtSo_No", dsMaster.SO_NO);
        // $NC.setValue("#cboCar_Div", dsMaster.CAR_DIV);
        $NC.setValue("#edtRemark1", dsMaster.REMARK1);

        $NC.G_VAR.masterData = {
            CENTER_CD: dsMaster.CENTER_CD,
            BU_CD: dsMaster.BU_CD,
            ORDER_DATE: dsMaster.ORDER_DATE,
            ORDER_NO: dsMaster.ORDER_NO,
            INOUT_CD: dsMaster.INOUT_CD,
            OUTBOUND_STATE: $ND.C_STATE_ORDER,
            CUST_CD: dsMaster.CUST_CD,
            ORDER_DIV: dsMaster.ORDER_DIV,
            ERP_BATCH: dsMaster.ERP_BATCH,
            CARRIER_CD: dsMaster.CARRIER_CD,
            CAR_DIV: dsMaster.CAR_DIV,
            DELIVERY_CD: dsMaster.DELIVERY_CD,
            RDELIVERY_CD: dsMaster.RDELIVERY_CD,
            INOUT_DATE: dsMaster.INOUT_DATE,
            DIRECT_YN: dsMaster.DIRECT_YN,
            BU_DATE: dsMaster.BU_DATE,
            BU_NO: dsMaster.BU_NO,
            BU_DATETIME: dsMaster.BU_DATETIME,
            SO_DATE: dsMaster.SO_DATE,
            SO_NO: dsMaster.SO_NO,
            WB_NO: dsMaster.WB_NO,
            ORG_ORDER_DATE: dsMaster.ORG_ORDER_DATE,
            ORG_ORDER_NO: dsMaster.ORG_ORDER_NO,
            ORG_BU_NO: dsMaster.ORG_BU_NO,
            DATA_DIV: dsMaster.DATA_DIV,
            REMARK1: dsMaster.REMARK1,
            CRUD: $ND.C_DV_CRUD_R
        };

        // 디테일 데이터 세팅
        var dsDetail = $NC.G_VAR.G_PARAMETER.P_DETAIL_DS;
        var rowData, rIndex, rCount, newRowData;
        G_GRDDETAIL.data.beginUpdate();
        try {
            for (rIndex = 0, rCount = dsDetail.length; rIndex < rCount; rIndex++) {
                rowData = $.extend({}, dsDetail[rIndex]);

                if (Number(rowData.ORDER_QTY) < 1) {
                    continue;
                }
                newRowData = {
                    CENTER_CD: rowData.CENTER_CD,
                    BU_CD: rowData.BU_CD,
                    ORDER_DATE: rowData.ORDER_DATE,
                    ORDER_NO: rowData.ORDER_NO,
                    LINE_NO: rowData.LINE_NO,
                    OUTBOUND_STATE: $ND.C_STATE_ORDER,
                    BRAND_CD: rowData.BRAND_CD,
                    BRAND_NM: rowData.BRAND_NM,
                    ITEM_CD: rowData.ITEM_CD,
                    ITEM_BAR_CD: rowData.ITEM_BAR_CD,
                    ITEM_NM: rowData.ITEM_NM,
                    ITEM_SPEC: rowData.ITEM_SPEC,
                    ITEM_STATE: rowData.ITEM_STATE,
                    ITEM_STATE_F: rowData.ITEM_STATE_F,
                    ITEM_LOT: rowData.ITEM_LOT,
                    QTY_IN_BOX: rowData.QTY_IN_BOX,
                    OUT_UNIT_DIV_F: rowData.OUT_UNIT_DIV_F,
                    ORDER_QTY: rowData.ORDER_QTY,
                    ORDER_BOX: rowData.ORDER_BOX,
                    ORDER_EA: rowData.ORDER_EA,
                    BOX_WEIGHT: rowData.BOX_WEIGHT,
                    ORDER_WEIGHT: rowData.ORDER_WEIGHT,
                    VALID_DATE: rowData.VALID_DATE,
                    BATCH_NO: rowData.BATCH_NO,
                    MARGIN_RATE: rowData.MARGIN_RATE,
                    SUPPLY_PRICE: rowData.SUPPLY_PRICE,
                    APPLY_PRICE: rowData.APPLY_PRICE,
                    DC_PRICE: rowData.DC_PRICE,
                    SUPPLY_AMT: rowData.SUPPLY_AMT,
                    VAT_AMT: rowData.VAT_AMT,
                    DC_AMT: rowData.DC_AMT,
                    TOTAL_AMT: rowData.TOTAL_AMT,
                    VAT_YN: rowData.VAT_YN,
                    DRUG_DIV_D: rowData.DRUG_DIV_D,
                    MEDICATION_DIV_D: rowData.MEDICATION_DIV_D,
                    KEEP_DIV_D: rowData.KEEP_DIV_D,
                    DRUG_CD: rowData.DRUG_CD,
                    ITEM_ORDER_DIV: rowData.ITEM_ORDER_DIV,
                    ITEM_ORDER_DIV_F: rowData.ITEM_ORDER_DIV_F,
                    BU_DATE: rowData.BU_DATE,
                    BU_NO: rowData.BU_NO,
                    BU_LINE_NO: rowData.BU_LINE_NO,
                    BU_KEY: rowData.BU_KEY,
                    BU_DATETIME: rowData.BU_DATETIME,
                    SO_DATE: rowData.SO_DATE,
                    SO_NO: rowData.SO_NO,
                    SO_LINE_NO: rowData.SO_LINE_NO,
                    SO_KEY: rowData.SO_KEY,
                    ORG_ORDER_DATE: rowData.ORG_ORDER_DATE,
                    ORG_ORDER_NO: rowData.ORG_ORDER_NO,
                    ORG_BU_NO: rowData.ORG_BU_NO,
                    CSCL_REQ_YN: rowData.CSCL_REQ_YN,
                    REMARK1: rowData.REMARK1,
                    SPARE1_NOTE: rowData.SPARE1_NOTE,
                    SPARE2_NOTE: rowData.SPARE2_NOTE,
                    SPARE3_NOTE: rowData.SPARE3_NOTE,
                    SPARE4_NOTE: rowData.SPARE4_NOTE,
                    SPARE5_NOTE: rowData.SPARE5_NOTE,
                    SPARE6_NOTE: rowData.SPARE6_NOTE,
                    SPARE7_NOTE: rowData.SPARE7_NOTE,
                    SPARE8_NOTE: rowData.SPARE8_NOTE,
                    SPARE9_NOTE: rowData.SPARE9_NOTE,
                    SHORTAGE_YN: $ND.C_NO,
                    id: $NC.getGridNewRowId(),
                    CRUD: $ND.C_DV_CRUD_R
                };
                G_GRDDETAIL.data.addItem(newRowData);
            }
        } finally {
            G_GRDDETAIL.data.endUpdate();
        }

        // 가용재고 체크
        for (rIndex = 0, rCount = G_GRDDETAIL.data.getLength(); rIndex < rCount; rIndex++) {
            grdDetailOnCheckPStock(rIndex, true);
        }

        // 수정일 경우 수정불가 세팅 - 기본
        $NC.setEnable("#dtpOrder_Date", false);

        // 수정일 경우 수정불가 세팅 - 인터페이스
        if ($NC.G_VAR.masterData.DATA_DIV != "00") {
            $NC.setEnable("#cboInout_Cd", false);
            // $NC.setEnable("#cboOrder_Div", false);
            // $NC.setEnable("#cboCar_Div", false);
            $NC.setEnable("#edtDelivery_Cd", false);
            $NC.setEnable("#btnDelivery_Cd", false);
            // $NC.setEnable("#edtRDelivery_Cd", false);
            // $NC.setEnable("#btnRDelivery_Cd", false);
        }

        $NC.setGridSelectRow(G_GRDDETAIL, 0);
    }

    // 조회조건 - 출고구분 세팅
    $NC.setInitCombo("/WC/getDataSet.do", {
        P_QUERY_ID: "WC.POP_CMCODE",
        P_QUERY_PARAMS: {
            P_COMMON_GRP: "INOUT_CD",
            P_COMMON_CD: $ND.C_ALL,
            P_ATTR01_CD: [
                $ND.C_INOUT_GRP_D1,
                $ND.C_INOUT_GRP_D2
            ].join($ND.C_SEP_DATA),
            P_VIEW_DIV: "1" // 1:등록팝업, 2:조회팝업
        }
    }, {
        selector: "#cboInout_Cd",
        codeField: "COMMON_CD",
        nameField: "COMMON_NM",
        fullNameField: "COMMON_CD_F",
        selectOption: $NC.G_VAR.G_PARAMETER.P_PROCESS_CD == $ND.C_PROCESS_ORDER_CREATE ? "F" : null,
        onComplete: function() {
            if ($NC.G_VAR.G_PARAMETER.P_PROCESS_CD == $ND.C_PROCESS_ORDER_CREATE) {
                $NC.G_VAR.masterData.INOUT_CD = $NC.getValue("#cboInout_Cd");
            } else {
                $NC.setValue("#cboInout_Cd", $NC.G_VAR.masterData.INOUT_CD);
            }
            $NC.G_VAR.masterData.INOUT_CD_SUB = $NC.getComboData("#cboInout_Cd", -1).ATTR01_CD;
        }
    });

    // 주문구분
    $NC.setInitCombo("/WC/getDataSet.do", {
        P_QUERY_ID: "WC.POP_CMCODE",
        P_QUERY_PARAMS: {
            P_COMMON_GRP: "ORDER_DIV",
            P_COMMON_CD: $ND.C_ALL,
            P_VIEW_DIV: "1" // 1:등록팝업, 2:조회팝업
        }
    }, {
        selector: "#cboOrder_Div",
        codeField: "COMMON_CD",
        nameField: "COMMON_NM",
        fullNameField: "COMMON_CD_F",
        selectOption: $NC.G_VAR.G_PARAMETER.P_PROCESS_CD == $ND.C_PROCESS_ORDER_CREATE ? "F" : null,
        onComplete: function() {
            if ($NC.G_VAR.G_PARAMETER.P_PROCESS_CD == $ND.C_PROCESS_ORDER_CREATE) {
                $NC.G_VAR.masterData.ORDER_DIV = $NC.getValue("#cboOrder_Div");
            } else {
                $NC.setValue("#cboOrder_Div", $NC.G_VAR.masterData.ORDER_DIV);
            }
        }
    });

    // 차량구분
    $NC.setInitCombo("/WC/getDataSet.do", {
        P_QUERY_ID: "WC.POP_CMCODE",
        P_QUERY_PARAMS: {
            P_COMMON_GRP: "CAR_DIV",
            P_COMMON_CD: $ND.C_ALL,
            P_VIEW_DIV: "1" // 1:등록팝업, 2:조회팝업
        }
    }, {
        selector: "#cboCar_Div",
        codeField: "COMMON_CD",
        nameField: "COMMON_NM",
        fullNameField: "COMMON_CD_F",
        selectOption: $NC.G_VAR.G_PARAMETER.P_PROCESS_CD == $ND.C_PROCESS_ORDER_CREATE ? "F" : null,
        onComplete: function() {
            if ($NC.G_VAR.G_PARAMETER.P_PROCESS_CD == $ND.C_PROCESS_ORDER_CREATE) {
                $NC.G_VAR.masterData.CAR_DIV = $NC.getValue("#cboCar_Div");
            } else {
                $NC.setValue("#cboCar_Div", $NC.G_VAR.masterData.CAR_DIV);
            }
        }
    });

    // 검색구분에 초기값 설정
    $NC.setEnable("#rgbQView_Div2", $NC.G_VAR.G_PARAMETER.P_POLICY_LS210 == "2" && $NC.G_VAR.G_PARAMETER.P_POLICY_LO250 == "2");
    $NC.setValue("#rgbQView_Div1", true);

    // 정책값 세팅
    setPolicyValInfo();

    // 프로그램 사용 권한 설정
    setUserProgramPermission();
}

/**
