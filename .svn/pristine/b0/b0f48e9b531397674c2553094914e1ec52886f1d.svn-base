/**
 * Delete Button Event - 메인 상단 삭제 버튼 클릭시 호출 됨
 */
function _Delete() {

    if (G_GRDMASTER.data.getLength() == 0 || $NC.isNull(G_GRDMASTER.lastRow)) {
        alert($NC.getDisplayMsg("JS.EDR04050E0.007", "삭제할 데이터가 없습니다."));
        return;
    }

    if (!confirm($NC.getDisplayMsg("JS.EDR04050E0.008", "선택한 수신처리내역을 삭제하시겠습니까?"))) {
        return;
    }

    var rowData = G_GRDMASTER.data.getItem(G_GRDMASTER.lastRow);
    // ER_PROCESSING 호출
    $NC.serviceCall("/EDR04050E0/callERLOOrderDelete.do", {
        P_BU_CD: rowData.BU_CD,
        P_EDI_DIV: rowData.EDI_DIV,
        P_DEFINE_NO: rowData.DEFINE_NO,
        P_RECV_DATE: rowData.RECV_DATE,
        P_RECV_NO: rowData.RECV_NO,
        P_USER_ID: $NC.G_USERINFO.USER_ID
    }, onGetDelete);
}

