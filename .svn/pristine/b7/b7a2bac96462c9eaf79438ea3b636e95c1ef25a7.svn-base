/**
 * 배송처별 출고내역탭의 그리드 초기값 설정
 */
function grdT2MasterInitialize() {

    var options = {
        frozenColumn: 3,
        specialRow: {
            compareFn: function(specialRow, rowData, column, row, cell, colspan) {
                if (rowData.DATA_GRP.substr(rowData.DATA_GRP.length - 1) == "1") {
                    return "stySubTotal";
                }
            }
        },
        summaryRow: {
            visible: true,
            compareFn: function(field, rowData) {
                if (rowData.DATA_GRP.substr(rowData.DATA_GRP.length - 1) == "1") {
                    return false;
                }
                return true;
            }
        }
    };

    // Grid Object, DataView 생성 및 초기화
    $NC.setInitGridObject("#grdT2Master", {
        columns: grdT2MasterOnGetColumns(),
        queryId: "LOF03040Q0.RS_T2_MASTER",
        sortCol: "DELIVERY_CD",
        gridOptions: options
    });

    G_GRDT2MASTER.view.onSelectedRowsChanged.subscribe(grdT2MasterOnAfterScroll);
}

/**
 * 배송처별출고내역 탭의 그리드 행 클릭시 처리
 * 
 * @param e
 * @param args
 */
function grdT2MasterOnAfterScroll(e, args) {

    if (!$NC.isGridValidLastRow(G_GRDT2MASTER, args.rows, e)) {
        return;
    }
    var row = args.rows[0];

    // 상단 현재로우/총건수 업데이트
    $NC.setGridDisplayRows(G_GRDT2MASTER, row + 1);
}

