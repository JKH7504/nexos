<!DOCTYPE HTML>
<html lang="ko">
<head>
<!-- ${NEXOS_META} -->
<!-- ${NEXOS_LIBRARY} ${} ${../../../} -->
<!-- PROGRAM CSS/JS -->
<!-- <link rel="stylesheet" href="css/CMC02020E0.css" /> -->
<script src="js/CMC02020E0.js"></script>
</head>
<body>
    <div id="ctrConditionView" class="ctrCondition">
        <table class="ctrInner">
            <tr>
                <td>
                    <div id="lblQPopup_Carrier_Cd" class="lblCondition">운송사</div>
                    <input id="edtQCarrier_Cd" class="edtNormal" type="text" style="width: 60px;" />
                    <input id="btnQCarrier_Cd" class="btnIcoPopup" type="button" />
                    <input id="edtQCarrier_Nm" class="edtDisplay" type="text" disabled="disabled" style="width: 120px;" />
                </td>
                <td>
                    <div id="lblQDeal_Div" class="lblCondition">거래구분</div>
                    <div class="ctrGroupBox">
                        <input id="chkQDeal_Div1" name="chkQDeal_Div" class="chkNormal" type="checkbox" value="1" />
                        거래진행
                        <input id="chkQDeal_Div2" name="chkQDeal_Div" class="chkNormal" type="checkbox" value="2" />
                        거래종료
                        <input id="chkQDeal_Div3" name="chkQDeal_Div" class="chkNormal" type="checkbox" value="3" />
                        조회종료
                    </div>
                </td>
            </tr>
        </table>
    </div>
    <div id="divLeftView" class="ctrLeft">
        <table class="ctrTitleBar">
            <tr>
                <td id="ctrTitle_grdMaster" class="lblTitle">차량정보</td>
                <td>
                    <input id="btnEntryCoord" class="btnImgPopup" type="button" value="좌표일괄등록" style="width: 105px;" />
                </td>
                <td id="ctrRows_grdMaster" class="lblRows">0/0 Rows</td>
            </tr>
        </table>
        <div id="grdMaster"></div>
    </div>
    <div id="divRightView" class="ctrRight">
        <!-- Tab Handler -->
        <ul>
            <li id="divTab1"><a href="#divT1TabSheetView">기본정보</a></li>
            <li id="divTab2"><a href="#divT2TabSheetView">상세정보</a></li>
        </ul>
        <!-- 기본정보 -->
        <div id="divT1TabSheetView" class="ctrInner">
            <div class="ctrGroupRow">
                <div id="lblCar_Cd" class="lblKey alignInContent">차량코드</div>
                <input id="edtCar_Cd" class="edtKey" type="text" maxlength="30" style="width: 110px;" />
            </div>
            <div class="ctrGroupRow">
                <div id="lblCar_Nm" class="lblKey alignInContent">차량명</div>
                <input id="edtCar_Nm" class="edtKey" type="text" maxlength="50" style="width: 330px;" />
            </div>
            <div class="ctrGroupRow">
                <div id="lblCar_Div" class="lblNormal alignInContent">차량구분</div>
                <select id="cboCar_Div" class="cboNormal" style="width: 150px;"></select>
            </div>
            <div class="ctrGroupRow">
                <div id="lblPopup_Carrier_Cd" class="lblNormal alignInContent">운송사</div>
                <input id="edtCarrier_Cd" class="edtNormal" type="text" style="width: 110px;" />
