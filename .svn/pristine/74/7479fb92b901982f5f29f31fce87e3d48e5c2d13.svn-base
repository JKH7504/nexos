     <!--</if>-->
     <if test="P_CUST_CD                 != null and P_CUST_CD                 != ''">
       AND CUST_CD                  = #{P_CUST_CD}
     </if>
     <if test="P_MATERIAL_CD             != null and P_MATERIAL_CD             != ''">
       AND MATERIAL_CD              = #{P_MATERIAL_CD}
     </if>
  </delete>
  <!-- [E] - DELETE_LC110ND [가공작업디테일] -->


