    }

    if ($NC.isNull(BAY_CD)) {
        BAY_CD = $NC.G_VAR.DIV_LOC.substring(0, $NC.G_VAR.policyVal.CM123);
    }

    if ($NC.isNull(LEV_CD)) {
        LEV_CD = $NC.G_VAR.DIV_LOC.substring(0, $NC.G_VAR.policyVal.CM124);
    }

    var LOCATION_CD = $NC.getDisplayLocation(ZONE_CD, BANK_CD, BAY_CD, LEV_CD, $NC.G_VAR.policyVal.CM120);
    $NC.setInitGridVar(G_GRDDETAIL);

    G_GRDDETAIL.queryParams = {
        P_CENTER_CD: $NC.G_VAR.masterData.CENTER_CD,
        P_BU_CD: $NC.G_VAR.masterData.BU_CD,
        P_MOVE_DIV: MOVE_DIV,
        P_BRAND_CD: BRAND_CD,
        P_ITEM_CD: ITEM_CD,
        P_LOCATION_CD: LOCATION_CD,
        P_STOCK_DATE1: STOCK_DATE1,
        P_STOCK_DATE2: STOCK_DATE2,
        P_TAG_DIV: TAG_DIV,
        P_RETURN_BATCH_DIV: RETURN_BATCH_DIV,
        P_ITEM_STATE: ITEM_STATE,
        P_YEAR_DIV: YEAR_DIV,
        P_SEASON_DIV: SEASON_DIV,
        P_ITEM_DIV: ITEM_DIV,
        P_DELIVERY_CD: DELIVERY_CD
    };
    // 데이터 조회
    $NC.serviceCall("/LCC03010E0/getDataSet.do", $NC.getGridParams(G_GRDDETAIL), onGetDetail);
}

/**
 * 상품추가 버튼 클릭 이벤트 처리
 */
function _New() {

    // 마지막 선택 Row Validation 체크
    if (!$NC.isGridValidLastRow(G_GRDSUB)) {
        return;
    }

    // 마지막 선택 Row Validation 체크
    if (!$NC.isGridValidLastRow(G_GRDDETAIL)) {
        return;
    }

    // 선택 데이터 검색 -> Indexes
    var checkedRows = $NC.getGridSearchRows(G_GRDDETAIL, {
        searchKey: "CHECK_YN",
        searchVal: $ND.C_YES
    });
    if (checkedRows.length == 0) {
        alert($NC.getDisplayMsg("JS.LCC03011P1.013", "추가할 재고내역 데이터를 선택하십시오."));
        return;
    }
    var MLOCATION_CD = $NC.getValue("#edtLocation_Cd");
    if ($NC.isNull(MLOCATION_CD)) {
        alert($NC.getDisplayMsg("JS.LCC03011P1.014", "재고 추가시 먼저 이동할 로케이션을 지정하십시오."));
        $NC.setFocus("#edtLocation_Cd");
        return;
    }

    // 기등록 체크 키
    var searchKey = [
        "BRAND_CD",
        "ITEM_CD",
        "ITEM_STATE",
        "ITEM_LOT",
        "VALID_DATE",
        "BATCH_NO",
        "STOCK_DATE",
        "LOCATION_CD"
    ];

    var addedCount = 0, sameLocCount = 0, pstockQtyCount = 0;
    var rowData, newRowData, preMStockQty, newMStockQty;
    G_GRDSUB.data.beginUpdate();
    try {
        for (var rIndex = 0, rCount = checkedRows.length; rIndex < rCount; rIndex++) {
            // 선택 데이터
            rowData = G_GRDDETAIL.data.getItem(checkedRows[rIndex]);

            // 동일 로케이션
            if (rowData.LOCATION_CD == MLOCATION_CD) {
                sameLocCount++;
                continue;
            }

            // 가용량 check
            if (rowData.PSTOCK_QTY < 1) {
                pstockQtyCount++;
                continue;
            }

            // 기등록 수량 체크
            preMStockQty = $NC.getGridSumVal(G_GRDSUB, {
                searchKey: searchKey,
                searchVal: [
                    rowData.BRAND_CD,
                    rowData.ITEM_CD,
                    rowData.ITEM_STATE,
                    rowData.ITEM_LOT,
                    rowData.VALID_DATE,
                    rowData.BATCH_NO,
                    rowData.STOCK_DATE,
                    rowData.LOCATION_CD
                ],
                sumKey: "MSTOCK_QTY"
            });

            // 가용재고만큼 이동처리되어 있으면 추가하지 않음
            if (rowData.PSTOCK_QTY <= preMStockQty) {
                continue;
            }

            // 추가할 가용재고 수량 계산
            newMStockQty = rowData.PSTOCK_QTY - preMStockQty;

            // 추가할 데이터
            newRowData = {
                LINE_NO: "",
                LOCATION_CD: rowData.LOCATION_CD,
                BRAND_CD: rowData.BRAND_CD,
                BRAND_NM: rowData.BRAND_NM,
                ITEM_CD: rowData.ITEM_CD,
                ITEM_BAR_CD: rowData.ITEM_BAR_CD,
                ITEM_STATE: rowData.ITEM_STATE,
                ITEM_LOT: rowData.ITEM_LOT,
                VALID_DATE: rowData.VALID_DATE,
                STOCK_DATE: rowData.STOCK_DATE,
                BATCH_NO: rowData.BATCH_NO,
                PALLET_ID: rowData.PALLET_ID,
                STOCK_QTY: rowData.STOCK_QTY,
                PSTOCK_QTY: rowData.PSTOCK_QTY,
                MSTOCK_QTY: newMStockQty,
                ITEM_NM: rowData.ITEM_NM,
                ITEM_SPEC: rowData.ITEM_SPEC,
                QTY_IN_BOX: rowData.QTY_IN_BOX,
                MLOCATION_CD: MLOCATION_CD,
                ITEM_STATE_F: rowData.ITEM_STATE_F,
                id: $NC.getGridNewRowId(),
                CRUD: $ND.C_DV_CRUD_C
            };
            G_GRDSUB.data.addItem(newRowData);
            addedCount++;
        }
    } finally {
        G_GRDSUB.data.endUpdate();
    }

    // 선택한 행이 모두 이미 지시내역 그리드에 존재할 경우
    if (addedCount == 0) {
        if (checkedRows.length == sameLocCount) {
            alert($NC.getDisplayMsg("JS.LCC03011P1.015", "선택한 상품의 재고로케이션과 이동로케이션이 모두 같습니다.\n이동로케이션 변경 후 추가하십시오."));
        } else if (checkedRows.length == pstockQtyCount) {
            alert($NC.getDisplayMsg("JS.LCC03011P1.015", "선택한 상품의 이동가능 수량이 존재하지않습니다."));
        } else {
            alert($NC.getDisplayMsg("JS.LCC03011P1.016", "선택한 상품은 이미 추가된 재고내역입니다."));
        }
        return;
    }
    $NC.setGridSelectRow(G_GRDSUB, G_GRDSUB.data.getLength() - 1);

    // 수정 상태로 변경
    G_GRDSUB.lastRowModified = true;

    // 상태 콤보 disable
    $NC.setEnable("#cboQItem_State", false);
    // 이동구분 비활성화
    $NC.setEnable("#cboMove_Div", false);
    // 택구분 비활성화
    $NC.setEnable("#cboTag_Div", false);

    // 이동구분이 13-ASS이동일 경우 로케이션 비활성화
    if ($NC.getValue("#cboMove_Div") == "13") {
        $NC.setEnable("#edtLocation_Cd", false);
        $NC.setEnable("#btnLocation_Cd", false);
    }
}

/**
 * 저장버튼 클릭 이벤트 처리
 */
function _Save() {

    if (G_GRDSUB.data.getLength() == 0) {
        alert($NC.getDisplayMsg("JS.LCC03011P1.017", "저장할 데이터가 없습니다."));
        return;
    }

    if ($NC.isNull($NC.G_VAR.masterData.CENTER_CD)) {
        alert($NC.getDisplayMsg("JS.LCC03011P1.018", "물류센터를 입력하십시오."));
        return;
    }

    if ($NC.isNull($NC.G_VAR.masterData.BU_CD)) {
        alert($NC.getDisplayMsg("JS.LCC03011P1.019", "사업부를 입력하십시오."));
        return;
    }

    if ($NC.isNull($NC.G_VAR.masterData.MOVE_DATE)) {
        alert($NC.getDisplayMsg("JS.LCC03011P1.020", "이동일자를 입력하십시오."));
        $NC.setFocus("#dtpMove_Date");
        return;
    }

    if ($NC.isNull($NC.G_VAR.masterData.MOVE_DIV)) {
        alert($NC.getDisplayMsg("JS.LCC03011P1.020", "이동구분을 선택하십시오."));
        $NC.setFocus("#cboMove_Div");
        return;
    }

    // 마지막 선택 Row Validation 체크
    if (!$NC.isGridValidLastRow(G_GRDSUB)) {
        return;
    }

    var dsD = [];
    var dsC = [];
    var dsSub, rowData;
    var dsTarget = G_GRDSUB.data.getItems();
    for (var rIndex = 0, rCount = dsTarget.length; rIndex < rCount; rIndex++) {
        rowData = dsTarget[rIndex];
        if (rowData.CRUD == $ND.C_DV_CRUD_R) {
            continue;
        }
        if (!validateSubData(rIndex, rowData)) {
            return;
        }
        if (rowData.CRUD == $ND.C_DV_CRUD_D) {
            dsSub = dsD;
        } else {
            dsSub = dsC;
        }

        dsSub.push({
            P_CENTER_CD: $NC.G_VAR.masterData.CENTER_CD,
            P_BU_CD: $NC.G_VAR.masterData.BU_CD,
            P_MOVE_DATE: $NC.G_VAR.masterData.MOVE_DATE,
            P_MOVE_NO: $NC.G_VAR.masterData.MOVE_NO,
            P_MOVE_DIV: $NC.G_VAR.masterData.MOVE_DIV,
            P_REMARK1: $NC.G_VAR.masterData.REMARK1,
            P_LINE_NO: rowData.LINE_NO,
            P_LOCATION_CD: rowData.LOCATION_CD,
            P_BRAND_CD: rowData.BRAND_CD,
            P_ITEM_CD: rowData.ITEM_CD,
            P_ITEM_STATE: rowData.ITEM_STATE,
            P_ITEM_LOT: rowData.ITEM_LOT,
            P_VALID_DATE: $NC.isNull(rowData.VALID_DATE) ? "" : rowData.VALID_DATE,
            P_BATCH_NO: $NC.isNull(rowData.BATCH_NO) ? "" : rowData.BATCH_NO,
            P_STOCK_DATE: $NC.isNull(rowData.STOCK_DATE) ? "" : rowData.STOCK_DATE,
            P_STOCK_QTY: rowData.STOCK_QTY,
            P_MSTOCK_QTY: rowData.MSTOCK_QTY,
            P_MLOCATION_CD: rowData.MLOCATION_CD,
            P_PALLET_ID: "",
            P_CRUD: rowData.CRUD
        });
    }

    // 생성된 파렛트ID를 그대로 사용하기 위해
    // 삭제할 데이터를 마지막에 추가
    dsSub = dsC.concat(dsD);

    if ($NC.G_VAR.masterData.CRUD == $ND.C_DV_CRUD_C) {
        if (dsSub.length == 0) {
            alert($NC.getDisplayMsg("JS.LCC03011P1.021", "저장할 지시내역 정보가 없습니다."));
            return;
        }
    } else {
        if ($NC.G_VAR.masterData.CRUD != $ND.C_DV_CRUD_U && dsSub.length == 0) {
            alert($NC.getDisplayMsg("JS.LCC03011P1.022", "데이터 수정 후 저장하십시오."));
            return;
        }
    }

    $NC.serviceCall("/LCC03010E0/save.do", {
        P_DS_MASTER: {
            P_CENTER_CD: $NC.G_VAR.masterData.CENTER_CD,
            P_BU_CD: $NC.G_VAR.masterData.BU_CD,
            P_MOVE_DATE: $NC.G_VAR.masterData.MOVE_DATE,
            P_MOVE_NO: $NC.G_VAR.masterData.MOVE_NO,
            P_REMARK1: $NC.G_VAR.masterData.REMARK1,
            P_CRUD: $NC.G_VAR.masterData.CRUD
        },
        P_DS_SUB: dsSub,
        P_PROCESS_CD: $NC.G_VAR.G_PARAMETER.P_PROCESS_CD,
        P_USER_ID: $NC.G_USERINFO.USER_ID
    }, onSave);
}

/**
 * 상품삭제 버튼 클릭 이벤트 처리
 */
function _Delete() {

    if (G_GRDSUB.data.getLength() == 0 || $NC.isNull(G_GRDSUB.lastRow)) {
        alert($NC.getDisplayMsg("JS.LCC03011P1.023", "삭제할 데이터가 없습니다."));
        return;
    }

    var rowData = G_GRDSUB.data.getItem(G_GRDSUB.lastRow);
    // 신규 데이터일 경우 Grid 데이터만 삭제, 그외 CRUD를 "D"로 변경
    $NC.deleteGridRowData(G_GRDSUB, rowData, true);

    // 상태 콤보 및 파렛트ID 기준 enable
    if (G_GRDSUB.data.getLength() == 0 && $NC.G_VAR.G_PARAMETER.P_PROCESS_CD == $ND.C_PROCESS_ENTRY_NEW) {
        $NC.setEnable("#cboMove_Div");
        $NC.setEnableGroup("#divStockCondition");
        $NC.setEnable("#edtLocation_Cd");
        $NC.setEnable("#btnLocation_Cd");
    } else if (G_GRDSUB.data.getLength() == 0) {
        $NC.setEnable("#cboTag_Div");
        $NC.setEnable("#edtLocation_Cd");
        $NC.setEnable("#btnLocation_Cd");
    }

}

/**
 * 마스터 데이터 변경시 처리
 */
function masterDataOnChange(e, args) {

    switch (args.col) {
        case "LOCATION_CD":
            $NP.onLocationChange(args.val, {
                P_CENTER_CD: $NC.G_VAR.G_PARAMETER.P_CENTER_CD,
                P_ZONE_CD: "",
                P_BANK_CD: "",
                P_BAY_CD: "",
                P_LEV_CD: "",
                P_LOCATION_CD: args.val,
                // 1 - 일반, 2 - 유통가공, 3 - 보세
                P_ZONE_DIV_ATTR01_CD: getZoneDivAttr01Cd($NC.getValue("#cboQItem_State")),
                P_LOC_DIV_ATTR03_CD: $NC.G_VAR.masterData.MOVE_DIV == "13" ? "20" : "10"
            }, onLocationPopup, {
                errorMessage: $NC.getDisplayMsg("JS.LCC03011P1.024", "이동 가능한 로케이션이 검색되지 않았습니다.")
            });
            return;
        case "MOVE_DATE":
            $NC.setValueDatePicker(args.view, args.val, $NC.getDisplayMsg("JS.LCC03011P1.025", "이동일자를 정확히 입력하십시오."));
            $NC.G_VAR.masterData.MOVE_DATE = $NC.getValue("#dtpMove_Date");
            break;
        case "MOVE_DIV":
            $NC.G_VAR.masterData.MOVE_DIV = args.val;
            $NC.clearGridData(G_GRDDETAIL);
            $NC.setValue("#edtQZone_Cd");
            $NC.setValue("#edtQZone_Nm");
            $NC.setValue("#edtLocation_Cd");
            return;
        case "REMARK1":
            $NC.G_VAR.masterData.REMARK1 = args.val;
            break;
        case "TAG_DIV":
            $NC.clearGridData(G_GRDDETAIL);
            $NC.setValue("#edtQItem_Cd");
            $NC.setValue("#edtQItem_Nm");
            $NC.G_VAR.masterData.TAG_DIV = args.val;
            return;
        default:
            // 마스터 데이터 변경이 아닐 경우는 CRUD 변경하지 않기 위해 RETURN
            return;
    }

    if ($NC.G_VAR.masterData.CRUD == $ND.C_DV_CRUD_R) {
        $NC.G_VAR.masterData.CRUD = $ND.C_DV_CRUD_U;
    }
}

/**
 * Grid에서 CheckBox Formatter를 사용할 경우 CheckBox Click 이벤트 처리
 * 
 * @param e
 * @param view
 *        대상 Object
 * @param args
 *        grid, row, cell, val
 */
function _OnGridCheckBoxFormatterClick(e, view, args) {

    var grdObject = $NC.getGridObject(args.grid);
    if (!grdObject.isValid) {
        return;
    }

    var columnId = grdObject.view.getColumnId(args.cell);
    if ($NC.isNull(columnId)) {
        return;
    }

    switch (columnId) {
        case "CHECK_YN":
            $NC.onGridCheckBoxFormatterChange(grdObject, e, args, true);
            break;
    }
}

