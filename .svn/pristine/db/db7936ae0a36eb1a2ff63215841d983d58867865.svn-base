            if ($NC.isNull(STOCK_DATE2)) {
                alert($NC.getDisplayMsg("JS.LCC03011P1.011", "입고 종료일자를 입력하거나 입고 시작일자를 지워주십시오."));
                $NC.setFocus("#dtpQStock_Date2");
