    if ($NC.isNull(BRAND_CD) && $NC.isNull(ITEM_CD) && $NC.isNull(STOCK_DATE1) && $NC.isNull(ZONE_CD) && $NC.isNull(BANK_CD) && $NC.isNull(BAY_CD)
