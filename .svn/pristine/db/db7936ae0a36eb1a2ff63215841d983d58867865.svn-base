function btnUploadXLSOnClick() {

    if ($NC.isNull($NC.G_VAR.masterData.CENTER_CD)) {
        alert($NC.getDisplayMsg("JS.LCC03011P1.018", "물류센터를 입력하십시오."));
        return;
    }
    if ($NC.isNull($NC.G_VAR.masterData.BU_CD)) {
        alert($NC.getDisplayMsg("JS.LCC03011P1.019", "사업부를 입력하십시오."));
        return;
    }
    if ($NC.isNull($NC.G_VAR.masterData.MOVE_DATE)) {
        alert($NC.getDisplayMsg("JS.LCC03011P1.020", "이동일자를 입력하십시오."));
        $NC.setFocus("#dtpMove_Date");
        return;
    }
    if ($NC.isNull($NC.G_VAR.masterData.MOVE_DIV)) {
        alert($NC.getDisplayMsg("JS.LCC03011P1.020", "이동구분을 선택하십시오."));
        $NC.setFocus("#cboMove_Div");
        return;
    }

    if ($NC.isNotNull($NC.G_VAR.masterData.MOVE_NO)) {
        alert($NC.getDisplayMsg("JS.LCC03011P1.029", "엑셀 업로드는 기존 데이터에 추가할 수 없습니다. 저장 후 신규로 처리하십시오."));
        return;
    }

    if (G_GRDSUB.data.getLength() > 0) {
        if (!confirm($NC.getDisplayMsg("JS.LCC03011P1.030", "엑셀 업로드시 기존 내역 삭제 후 추가됩니다. 엑셀 업로드하시겠습니까?"))) {
            return;
        }
    }

    var COLUMN_INFO = [
        {
            P_COLUMN_NM: "LOCATION_CD",
            P_XLS_COLUMN_NM: "A"
        },
        {
            P_COLUMN_NM: "BRAND_CD",
            P_XLS_COLUMN_NM: "B"
        },
        {
            P_COLUMN_NM: "ITEM_CD",
            P_XLS_COLUMN_NM: "C"
        },
        {
            P_COLUMN_NM: "ITEM_STATE",
            P_XLS_COLUMN_NM: "D"
        },
        {
            P_COLUMN_NM: "ITEM_LOT",
            P_XLS_COLUMN_NM: "E"
        }
    ];
    if ($NC.G_VAR.policyVal.LS210 == "2") {
        COLUMN_INFO.push({
            P_COLUMN_NM: "VALID_DATE",
            P_XLS_COLUMN_NM: "F"
        });
        COLUMN_INFO.push({
            P_COLUMN_NM: "BATCH_NO",
            P_XLS_COLUMN_NM: "G"
        });
        COLUMN_INFO.push({
            P_COLUMN_NM: "TAG_DIV",
            P_XLS_COLUMN_NM: "H"
        });
        COLUMN_INFO.push({
            P_COLUMN_NM: "MLOCATION_CD",
            P_XLS_COLUMN_NM: "I"
        });
        COLUMN_INFO.push({
            P_COLUMN_NM: "MOVE_QTY",
            P_XLS_COLUMN_NM: "J"
        });
    } else {
        COLUMN_INFO.push({
            P_COLUMN_NM: "TAG_DIV",
            P_XLS_COLUMN_NM: "F"
        });
        COLUMN_INFO.push({
            P_COLUMN_NM: "MLOCATION_CD",
            P_XLS_COLUMN_NM: "G"
        });
        COLUMN_INFO.push({
            P_COLUMN_NM: "MOVE_QTY",
            P_XLS_COLUMN_NM: "H"
        });
    }

    $NC.excelFileUpload({
        P_QUERY_ID: "LCC03010E1.RS_SUB3",
        P_QUERY_PARAMS: {
            P_CENTER_CD: $NC.G_VAR.masterData.CENTER_CD,
            P_BU_CD: $NC.G_VAR.masterData.BU_CD,
            P_MOVE_DIV: $NC.getValue("#cboMove_Div"),
            P_POLICY_LS210: $NC.G_VAR.policyVal.LS210
        },
        P_SERVICE_PARAMS: {
            P_XLS_COL_CHECK_YN: $ND.C_YES,
            P_XLS_FIRST_ROW: 3,
            P_XLS_FILE_DIV: "MOVE"
        },
        P_COLUMN_INFO: COLUMN_INFO
    }, function(ajaxData, dsResultData) {

        if ($NC.isNull(dsResultData) || dsResultData.length == 0) {
            alert($NC.getDisplayMsg("JS.LCC03011P1.031", "업로드할 수 있는 대상 데이터가 없습니다. 엑셀 파일을 확인하십시오."));
            return;
        }
        // 엑셀 정상 업로드가 될 경우 재고검색 및 그리드 초기화
        $NC.clearGridData(G_GRDDETAIL);
        $NC.setEnableGroup("#divStockCondition", false);
        $NC.setEnable("#edtLocation_Cd", false);
        $NC.setEnable("#btnLocation_Cd", false);
        // 이동구분 비활성화
        $NC.setEnable("#cboMove_Div", false);

        for (var row = 0; row < dsResultData.length; row++) {
            dsResultData[row].CRUD = $ND.C_DV_CRUD_C;
        }

        $NC.setInitGridData(G_GRDSUB, dsResultData);
        $NC.setGridSelectRow(G_GRDSUB, 0);
    });
}

/**
 * 검색조건의 배송처 검색 이미지 클릭
 */
function showDeliveryPopup() {

    var CUST_CD = $NC.getValue("#edtQCust_Cd");

    $NP.showDeliveryPopup({
        queryParams: {
            P_CUST_CD: CUST_CD,
            P_DELIVERY_CD: $ND.C_ALL,
            P_DELIVERY_DIV: $ND.C_ALL,
            P_VIEW_DIV: "2"
        }
    }, onDeliveryPopup, function() {
        $NC.setFocus("#edtQDelivery_Cd", true);
    });
}

function onDeliveryPopup(resultInfo) {

    if ($NC.isNotNull(resultInfo)) {
        $NC.setValue("#edtQDelivery_Cd", resultInfo.DELIVERY_CD);
        $NC.setValue("#edtQDelivery_Nm", resultInfo.DELIVERY_NM);
    } else {
        $NC.setValue("#edtQDelivery_Cd");
        $NC.setValue("#edtQDelivery_Nm");
        $NC.setFocus("#edtQDelivery_Cd", true);
    }
    onChangingCondition();
}