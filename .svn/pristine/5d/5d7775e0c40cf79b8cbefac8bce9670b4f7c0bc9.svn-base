<!DOCTYPE HTML>
<html lang="ko">
<head>
<!-- ${NEXOS_META} -->
<!-- ${NEXOS_LIBRARY} ${} ${../../../} -->
<!-- PROGRAM CSS/JS -->
<!-- <link rel="stylesheet" href="css/LDC01020Q0.css" /> -->
<script src="js/LDC01020Q0.js"></script>
</head>
<body>
    <div id="ctrConditionView" class="ctrCondition">
        <!-- 조회 조건 -->
        <table class="ctrInner">
            <tr>
                <td>
                    <div id="lblQCenter_Cd" class="lblCondition">물류센터</div>
                    <select id="cboQCenter_Cd" class="cboKey" style="width: 150px;"></select>
                </td>
                <td>
                    <div id="lblQPopup_Bu_Cd" class="lblCondition">사업부</div>
                    <input id="edtQBu_Cd" class="edtKey" type="text" style="width: 60px;" />
                    <input id="btnQBu_Cd" class="btnIcoPopup" type="button" />
                    <input id="edtQBu_Nm" class="edtDisplay" type="text" disabled="disabled" style="width: 120px;" />
                    <input id="edtQCust_Cd" class="edtHidden" type="text" disabled="disabled" />
                </td>
                <td>
                    <div id="divQT1_Outbound_Date" class="ctrGroup">
                        <div id="lblQT1_Outbound_Date" class="lblCondition">출고일자</div>
                        <input id="dtpQOutbound_Date" class="dtpKey" type="text" style="width: 100px;" />
                    </div>
                    <div id="divQT2_Outbound_Date" class="ctrGroup">
                        <div id="lblQT2_Outbound_Date" class="lblCondition">출고일자</div>
                        <input id="dtpQOutbound_Date1" class="dtpKey" type="text" style="width: 100px;" />
                        <span class="lblTerm">▶</span>
                        <input id="dtpQOutbound_Date2" class="dtpKey" type="text" style="width: 100px;" />
                    </div>
                </td>
            </tr>
            <tr>
                <td>
                    <div id="lblQPopup_Area_Cd" class="lblCondition">운송권역</div>
                    <input id="edtQArea_Cd" class="edtNormal" type="text" style="width: 60px;" />
                    <input id="btnQArea_Cd" class="btnIcoPopup" type="button" />
                    <input id="edtQArea_Nm" class="edtDisplay" type="text" disabled="disabled" style="width: 120px;" />
                </td>
                <td>
                    <div id="lblQPopup_RDelivery_Cd" class="lblCondition">실배송처</div>
                    <input id="edtQRDelivery_Cd" class="edtNormal" type="text" style="width: 60px;" />
                    <input id="btnQRDelivery_Cd" class="btnIcoPopup" type="button" />
                    <input id="edtQRDelivery_Nm" class="edtDisplay" type="text" disabled="disabled" style="width: 120px;" />
                </td>
            </tr>
        </table>
    </div>
    <div id="divMasterView">
        <!-- Tab Handler -->
        <ul>
            <li id="divTab1"><a href="#divT1TabSheetView">배차현황</a></li>
            <li id="divTab2"><a href="#divT2TabSheetView">기간별 배차현황내역</a></li>
        </ul>
        <!--  배차현황 내역 -->
        <div id="divT1TabSheetView">
            <table class="ctrTitleBar">
                <tr>
                    <td id="ctrTitle_grdT1Master" class="lblTitle">배차현황</td>
                    <td id="ctrRows_grdT1Master" class="lblRows">0/0 Rows</td>
                </tr>
            </table>
            <div id="grdT1Master"></div>
        </div>
        <!-- 기간별 배차현황 내역 -->
        <div id="divT2TabSheetView">
            <table class="ctrTitleBar">
                <tr>
                    <td id="ctrTitle_grdT2Master" class="lblTitle">기간별배차현황내역</td>
                    <td id="ctrRows_grdT2Master" class="lblRows">0/0 Rows</td>
                </tr>
            </table>
            <div id="grdT2Master"></div>
        </div>
    </div>
</body>
</html>
