  <!-- [S] - INSERT_LO020PM [출고부가정보마스터(온라인고객)] -->
  <insert id="INSERT_LO020PM" parameterType="hashmap">
    INSERT INTO LO020PM
               (
                CENTER_CD
               ,BU_CD
               ,OUTBOUND_DATE
               ,OUTBOUND_NO
               <if test="P_REMARK1                 != null and P_REMARK1                 != ''">,REMARK1                     </if>
               <if test="P_REG_USER_ID             != null and P_REG_USER_ID             != ''">,REG_USER_ID                 </if>
               <if test="P_REG_DATETIME            != null and P_REG_DATETIME            != ''">,REG_DATETIME                </if>
               ,LAST_USER_ID
               ,LAST_DATETIME
               )
         VALUES
               (
                #{P_CENTER_CD}
               ,#{P_BU_CD}
               ,#{P_OUTBOUND_DATE}
               ,#{P_OUTBOUND_NO}
               <if test="P_REMARK1                 != null and P_REMARK1                 != ''">,#{P_REMARK1}                           </if>
               <if test="P_REG_USER_ID             != null and P_REG_USER_ID             != ''">,#{P_REG_USER_ID}                       </if>
               <if test="P_REG_DATETIME            != null and P_REG_DATETIME            != ''">
               ,DECODE(#{P_REG_DATETIME} ,'SYSDATE' ,SYSDATE ,WF.C_TO_DATETIME(#{P_REG_DATETIME}))
               </if>
               ,#{P_USER_ID}
               ,SYSDATE
               )
  </insert>
  <!-- [E] - INSERT_LO020PM [출고부가정보마스터(온라인고객)] -->


  <!-- [S] - UPDATE_LO020PM [출고부가정보마스터(온라인고객)] -->
  <update id="UPDATE_LO020PM" parameterType="hashmap">
    UPDATE LO020PM
       SET LAST_USER_ID             = #{P_USER_ID}
          ,LAST_DATETIME            = SYSDATE
          <if test="P_REMARK1                 != null">,REMARK1                  = #{P_REMARK1}                            </if>
          <if test="P_REG_USER_ID             != null">,REG_USER_ID              = #{P_REG_USER_ID}                        </if>
          <if test="P_REG_DATETIME            != null">
          ,REG_DATETIME             = DECODE(#{P_REG_DATETIME} ,'SYSDATE' ,SYSDATE ,WF.C_TO_DATETIME(#{P_REG_DATETIME}))
          </if>
     WHERE CENTER_CD                = #{P_CENTER_CD}
       AND BU_CD                    = #{P_BU_CD}
       AND OUTBOUND_DATE            = #{P_OUTBOUND_DATE}
       AND OUTBOUND_NO              = #{P_OUTBOUND_NO}
  </update>
  <!-- [E] - UPDATE_LO020PM [출고부가정보마스터(온라인고객)] -->


