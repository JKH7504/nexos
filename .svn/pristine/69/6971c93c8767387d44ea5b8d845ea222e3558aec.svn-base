<!DOCTYPE HTML>
<html lang="ko">
<head>
<!-- ${NEXOS_META} -->
<!-- ${NEXOS_LIBRARY} ${} ${../../../} -->
<!-- PROGRAM CSS -->
<!-- <link rel="stylesheet" href="css/LCC05011P0.css" /> -->
<script src="js/LCC05011P0.js"></script>
</head>
<body>
    <div id="ctrPopupView" class="ctrPopup">
        <table class="ctrInner">
            <tr>
                <td>
                    <div id="lblItem_Cd" class="lblNormal">상품코드</div>
                    <input id="edtItem_Cd" class="edtDisplay" type="text" style="width: 100px;" />
                </td>
                <td>
                    <div id="lblItem_Nm" class="lblNormal">상품명</div>
                    <input id="edtItem_Nm" class="edtDisplay" type="text" style="width: 230px;" />
                </td>
            </tr>
            <tr>
                <td>
                    <div id="lblBrand_Nm" class="lblNormal">브랜드명</div>
                    <input id="edtBrand_Nm" class="edtDisplay" type="text" style="width: 100px;" />
                </td>
                <td>
                    <div id="lblItem_State_F" class="lblNormal">상품상태</div>
                    <input id="edtItem_State_F" class="edtDisplay" type="text" style="width: 100px;" />
                </td>
            </tr>
            <tr>
                <td>
                    <div id="lblLocation_Cd" class="lblNormal">로케이션</div>
                    <input id="edtLocation_Cd" class="edtDisplay" type="text" style="width: 100px;" />
                </td>
                <td>
                    <div id="lblBatch_No" class="lblNormal">제조배치</div>
                    <input id="edtBatch_No" class="edtDisplay" type="text" style="width: 100px;" />
                </td>
            </tr>
            <tr>
                <td>
                    <div id="lblStock_Date" class="lblNormal">입고일자</div>
                    <input id="edtStock_Date" class="edtDisplay" type="text" style="width: 100px;" />
                </td>
                <td>
                    <div id="lblValid_Date" class="lblNormal">제조일자</div>
                    <input id="edtValid_Date" class="edtDisplay" type="text" style="width: 100px;" />
                </td>
            </tr>
            <tr>
                <td>
                    <div id="lblPstock_Qty" class="lblNormal">출고가능량</div>
                    <input id="edtPstock_Qty" class="edtDisplay" type="text" style="width: 100px;" />
                </td>
                <td>
                    <div id="lblRemark1" class="lblNormal">비고</div>
                    <input id="edtRemark1" class="edtDisplay" type="text" style="width: 230px;" />
                </td>
            </tr>
        </table>
    </div>
    <div id="divDetailView" class="ctrPopup">
        <table class="ctrTitleBar">
            <tr>
                <td id="ctrTitle_grdDetail" class="lblTitle">재고분할정보</td>
                <td>
                    <input id="btnNew" class="btnIcoNew" type="button" />
                    <input id="btnDelete" class="btnIcoDelete" type="button" />
                </td>
                <td id="ctrRows_grdDetail" class="lblRows">0/0 Rows</td>
            </tr>
        </table>
        <div id="grdDetail"></div>
    </div>
    <div id="ctrPopupActionBar" class="ctrPopupActionBar">
        <input id="btnSave" class="btnNormal" type="button" value="저장" style="width: 80px;" />
        <input id="btnClose" class="btnNormal" type="button" value="닫기" style="width: 80px;" />
    </div>
</body>
</html>
