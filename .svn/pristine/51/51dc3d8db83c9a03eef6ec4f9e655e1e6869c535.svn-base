<!DOCTYPE HTML>
<html lang="ko">
<head>
<!-- ${NEXOS_META} -->
<!-- ${NEXOS_LIBRARY} ${c} ${../../../} -->
<!-- PROGRAM CSS/JS -->
<!-- <link rel="stylesheet" href="css/LVC02020Q0.css" /> -->
<script src="js/LVC02020Q0.js"></script>
</head>
<body>
    <!-- 조회 조건 -->
    <div id="ctrConditionView" class="ctrCondition">
        <table class="ctrInner">
            <tr>
                <td>
                    <div id="lblQCenter_Cd" class="lblCondition">물류센터</div>
                    <select id="cboQCenter_Cd" class="cboKey" style="width: 150px;"></select>
                </td>
                <td>
                    <div id="lblPopup_Cust_Cd" class="lblCondition">고객사</div>
                    <input id="edtQCust_Cd" class="edtNormal" type="text" style="width: 60px;" />
                    <input id="btnQCust_Cd" class="btnIcoPopup" type="button" />
                    <input id="edtQCust_Nm" class="edtDisplay" type="text" disabled="disabled" style="width: 120px;" />
                </td>
            </tr>
            <tr>
                <td>
                    <div id="lblQPopup_Bu_Cd" class="lblCondition">사업부</div>
                    <input id="edtQBu_Cd" class="edtNormal" type="text" style="width: 60px;" />
                    <input id="btnQBu_Cd" class="btnIcoPopup" type="button" />
                    <input id="edtQBu_Nm" class="edtDisplay" type="text" disabled="disabled" style="width: 120px;" />
                </td>
                <td id="tdQDsp_Brand_Cd">
                    <div id="lblQPopup_Brand_Cd" class="lblCondition">브랜드</div>
                    <input id="edtQBrand_Cd" class="edtNormal" type="text" style="width: 60px;" />
                    <input id="btnQBrand_Cd" class="btnIcoPopup" type="button" />
                    <input id="edtQBrand_Nm" class="edtDisplay" type="text" disabled="disabled" style="width: 120px;" />
                </td>
            </tr>
        </table>
    </div>
    <div id="divLeftView" class="ctrLeft">
        <div id="divPutawayView" style="position: relative;">
            <table id="divTitle_Putaway" class="ctrTitleBar">
                <tr>
                    <td id="dspTitle_Putaway" class="lblTitle">로케이션적재 현황</td>
                </tr>
            </table>
            <div id="chtPiePutawayReport" class="ctrGroup"></div>
            <div id="chtBarPutawayReport" class="ctrGroup"></div>
        </div>
        <div id="divLongTermView" style="position: relative;">
            <table id="divTitle_LongTerm" class="ctrTitleBar">
                <tr>
                    <td id="dspTitle_LongTerm" class="lblTitle">장기재고 현황</td>
                </tr>
            </table>
            <div id="divAdditional_LongTerm" class="ctrAdditional bottom styRight ctrSubCondition">
                <div id="lblQLong_Term_Month" class="lblCondition" style="width: 100px;">장기재고기준</div>
                <select id="cboQLong_Term_Month" class="cboKey"></select>
            </div>
            <div id="chtPieLongTermReport" class="ctrGroup"></div>
            <div id="chtBarLongTermReport" class="ctrGroup"></div>
        </div>
    </div>
    <div id="divRightView" class="ctrRight" style="overflow: hidden;">
        <div id="divRotateView" style="position: relative;">
            <table id="divTitle_Rotate" class="ctrTitleBar">
                <tr>
                    <td id="dspTitle_Rotate" class="lblTitle">회전일수 현황</td>
                </tr>
            </table>
            <div id="divAdditional_Rotate" class="ctrAdditional bottom styRight ctrSubCondition">
                <div id="lblQHigh_Rotate_Day" class="lblCondition" style="width: 130px;">회전일기준</div>
                <select id="cboQHigh_Rotate_Day" class="cboKey"></select>
            </div>
            <div id="chtPieRotateReport" class="ctrGroup"></div>
            <div id="chtBarRotateReport" class="ctrGroup"></div>
        </div>
        <div id="divValidView" style="position: relative;">
            <table id="divTitle_Valid" class="ctrTitleBar">
                <tr>
                    <td id="dspTitle_Valid" class="lblTitle">유통기한 도래 현황</td>
                </tr>
            </table>
            <div id="divAdditional_Valid" class="ctrAdditional bottom styRight ctrSubCondition">
                <div id="lblQQImpend_Day" class="lblCondition" style="width: 130px;">임박기준</div>
                <select id="cboQImpend_Day" class="cboKey"></select>
            </div>
            <div id="chtPieValidReport" class="ctrGroup"></div>
            <div id="chtBarValidReport" class="ctrGroup"></div>
        </div>
    </div>
</body>
</html>
