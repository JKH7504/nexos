<!DOCTYPE HTML>
<html lang="ko">
<head>
<!-- ${NEXOS_META} -->
<!-- ${NEXOS_LIBRARY} ${} ${../../../} -->
<!-- PROGRAM CSS/JS -->
<!-- <link rel="stylesheet" href="css/LDC03010E0.css" /> -->
<script src="js/LDC03010E0.js"></script>
</head>
<body>
    <div id="ctrConditionView" class="ctrCondition">
        <table class="ctrInner">
            <tr>
                <td>
                    <div id="lblQCenter_Cd" class="lblCondition">물류센터</div>
                    <select id="cboQCenter_Cd" class="cboKey" style="width: 180px;"></select>
                </td>
                <td>
                    <div id="lblQPopup_Bu_Cd" class="lblCondition">사업부</div>
                    <input id="edtQBu_Cd" class="edtKey" type="text" style="width: 60px;" />
                    <input id="btnQBu_Cd" class="btnIcoPopup" type="button" />
                    <input id="edtQBu_Nm" class="edtKey" type="text" disabled="disabled" style="width: 120px;" />
                    <input id="edtQCust_Cd" class="edtHidden" type="text" disabled="disabled" />
                </td>
                <td>
                    <div id="lblOutbound_Date" class="lblCondition">출고일자</div>
                    <input id="dtpQOutbound_Date" class="dtpKey" type="text" style="width: 100px;" />
                </td>
            </tr>
            <tr>
                <td>
                    <div id="lblQOutbound_Batch" class="lblCondition">출고차수</div>
                    <select id="cboQOutbound_Batch" class="cboNormal" style="width: 180px;"></select>
                    <input id="btnQOutbound_Batch" class="btnIcoRefresh" type="button" />
                </td>
                <td colspan="2">
                    <div id="lblQPopup_RDelivery_Cd" class="lblCondition">실배송처</div>
                    <input id="edtQRDelivery_Cd" class="edtNormal" type="text" style="width: 60px;" />
                    <input id="btnQRDelivery_Cd" class="btnIcoPopup" type="button" />
                    <input id="edtQRDelivery_Nm" class="edtDisplay" type="text" disabled="disabled" style="width: 120px;" />
                </td>
            </tr>
        </table>
    </div>
    <div id="divMasterView" class="ctrNormal">
        <table class="ctrTitleBar">
            <tr>
                <td id="ctrTitle_grdMaster" class="lblTitle">수기화물대장내역</td>
                <td>
                    <input id="btnCreateData" class="btnImgNormal" type="button" value="수기화물대장 생성" style="width: 140px;">
                </td>
                <td id="ctrRows_grdMaster" class="lblRows">0/0 Rows</td>
            </tr>
        </table>
        <div id="grdMaster"></div>
    </div>
</body>
</html>
