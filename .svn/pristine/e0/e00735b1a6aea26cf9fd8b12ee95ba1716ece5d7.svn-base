<!DOCTYPE HTML>
<html lang="ko">
<head>
<!-- ${NEXOS_META} -->
<!-- ${NEXOS_LIBRARY} ${} ${../../../} -->
<!-- PROGRAM CSS/JS -->
<!-- <link rel="stylesheet" href="css/CMC03050E0.css" /> -->
<script src="js/CMC03050E0.js"></script>
</head>
<body>
    <div id="ctrConditionView" class="ctrCondition">
        <table class="ctrInner">
            <tr>
                <td id="divQCenter_Cd">
                    <div id="lblQCenter_Cd" class="lblCondition">물류센터</div>
                    <select id="cboQCenter_Cd" class="cboKey" style="width: 150px;"></select>
                </td>
                <td>
                    <div id="lblQPopup_Cust_Cd" class="lblCondition">고객사</div>
                    <input id="edtQCust_Cd" class="edtKey" type="text" style="width: 60px;" />
                    <input id="btnQCust_Cd" class="btnIcoPopup" type="button" />
                    <input id="edtQCust_Nm" class="edtDisplay" type="text" disabled="disabled" style="width: 120px;" />
                </td>
                <td>
                    <div id="lblQPopup_Area_Cd" class="lblCondition">운송권역</div>
                    <input id="edtQArea_Cd" class="edtNormal" type="text" style="width: 60px;" />
                    <input id="btnQArea_Cd" class="btnIcoPopup" type="button" />
                    <input id="edtQArea_Nm" class="edtDisplay" type="text" disabled="disabled" style="width: 120px;" />
                </td>
            </tr>
            <tr>
                <td>
                    <div id="lblQDelivery_Cd" class="lblCondition">배송처코드</div>
                    <input id="edtQDelivery_Cd" class="edtNormal" type="text" style="width: 210px;" />
                </td>
                <td>
                    <div id="lblQDelivery_Nm" class="lblCondition">배송처명</div>
                    <input id="edtQDelivery_Nm" class="edtNormal" type="text" style="width: 210px;" />
                </td>
            </tr>
        </table>
    </div>
    <div id="divMasterView" class="ctrNormal">
        <div id="divMaster">
            <table class="ctrTitleBar">
                <tr>
                    <td id="ctrTitle_grdMaster" class="lblTitle">배송처 운송권역 정보</td>
                    <td id="ctrRows_grdMaster" class="lblRows">0/0 Rows</td>
                </tr>
            </table>
            <div id="grdMaster"></div>
        </div>
        <div id="divSub">
            <div id="ctrAdditional_grdSub" class="ctrAdditional top">
                <table class="ctrInner">
                    <tr>
                        <td>
                            <div id="lblPopup_Area_Cd" class="lblNormal">운송권역</div>
                            <input id="edtArea_Cd" class="edtNormal" type="text" style="width: 60px;" />
                            <input id="btnArea_Cd" class="btnIcoPopup" type="button" />
                            <input id="edtArea_Nm" class="edtDisplay" type="text" disabled="disabled" style="width: 90px;" />
                        </td>
                        <td>
                            <div id="lblDelivery_Route" class="lblNormal" style="margin-left: 15px">운송루트</div>
                            <input id="edtDelivery_Route" class="edtNormal formatterInteger" type="text" style="width: 60px;" />
                        </td>
                        <td>
                            <div id="lblDistance_Div" class="lblNormal" style="margin-left: 15px">거리등급</div>
                            <select id="cboDistance_Div" class="cboNormal" style="width: 130px;"></select>
                            <input id="btnAddDelivery" class="btnNormal" type="button" value="배송기준등록" style="width: 100px;" />
                        </td>
                    </tr>
                    <tr>
                        <td>
                            <div id="lblPopup_Carrier_Cd" class="lblNormal">운송사</div>
                            <input id="edtCarrier_Cd" class="edtNormal" type="text" style="width: 60px;" />
                            <input id="btnCarrier_Cd" class="btnIcoPopup" type="button" />
                            <input id="edtCarrier_Nm" class="edtDisplay" type="text" disabled="disabled" style="width: 90px;" />
                        </td>
                    </tr>
                </table>
            </div>
            <table class="ctrTitleBar">
                <tr>
                    <td id="ctrTitle_grdSub" class="lblTitle">미등록 배송처</td>
                    <td id="ctrRows_grdSub" class="lblRows">0/0 Rows</td>
                </tr>
            </table>
            <div id="grdSub"></div>
        </div>
    </div>
</body>
</html>
