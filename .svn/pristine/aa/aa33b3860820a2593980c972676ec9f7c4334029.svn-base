<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  <!--
  //====================================================================================================================
  // 출고예정작업[B2C] 화면
  // Author : ASETEC
  // Date : 2023-03-31
  // Description
  // 출고예정작업[B2C] 화면 CRUD Mapper
  //
  // ※ 작성은 반드시 알파벳 순서로 작성
  // ※ Mapper 파일은 [destination name]_sqlmap.xml 으로 생성
  // ※ Mapper namespace는 destination name 으로 설정
  //====================================================================================================================
  -->
  <!--
  cache         - 해당 네임스페이스을 위한 캐시 설정
  cache-ref     - 다른 네임스페이스의 캐시 설정에 대한 참조
  resultMap     - 데이터베이스 결과데이터를 객체에 로드하는 방법을 정의하는 엘리먼트
  parameterMap  - 비권장됨! 예전에 파라미터를 매핑하기 위해 사용되었으나 현재는 사용하지 않음
  sql           - 다른 구문에서 재사용하기 위한 SQL 조각
  insert        - 매핑된 INSERT 구문.
  update        - 매핑된 UPDATE 구문.
  delete        - 매핑된 DELEETE 구문.
  select        - 매핑된 SELECT 구문.
  //====================================================================================================================
  // ※ neXos Framework는 Vo를 사용하지 않음, HashMap만 사용 함
  //====================================================================================================================
  SELECT WS.GET_PROGRAM_MAPPER('LOM01010E0' ,'LO010PM' ,NULL ,'N' ,NULL ,NULL) AS SQLMAP_TEXT
    FROM DUAL;
  -->
<mapper namespace="LOM01010E0">
  <!--
  //====================================================================================================================
  // SELECT SQLMAP
  //====================================================================================================================
  -->
  <!-- myBatis로 처리시 CURSOR를 리턴 받기 위해 resultMap 정의(컬럼 정의는 하지 않음) -->
  <!-- 네이스페이스(namespace) + _VIRTUALMAP 으로 resultMap 정의 -->
  <resultMap id="LOM01010E0_VIRTUALMAP" type="hashmap"></resultMap>

  <!-- [S] - LOM01010E0.RS_MASTER -->
  <select id="RS_MASTER" statementType="CALLABLE" parameterType="hashmap">
    {  #{O_RESULT_DS               ,mode=OUT   ,jdbcType=CURSOR     ,javaType=ResultSet  ,resultMap=LOM01010E0_VIRTUALMAP} = call LOM01010E0.RS_MASTER
      (#{P_CENTER_CD               ,mode=IN    ,jdbcType=VARCHAR    ,javaType=String    }
      ,#{P_BU_CD                   ,mode=IN    ,jdbcType=VARCHAR    ,javaType=String    }
      ,#{P_ORDER_DATE1             ,mode=IN    ,jdbcType=TIMESTAMP  ,javaType=Date      }
      ,#{P_ORDER_DATE2             ,mode=IN    ,jdbcType=TIMESTAMP  ,javaType=Date      }
      ,#{P_INOUT_CD                ,mode=IN    ,jdbcType=VARCHAR    ,javaType=String    }
      ,#{P_BU_NO                   ,mode=IN    ,jdbcType=VARCHAR    ,javaType=String    }
      ,#{P_BRAND_CD                ,mode=IN    ,jdbcType=VARCHAR    ,javaType=String    }
      ,#{P_ITEM_CD                 ,mode=IN    ,jdbcType=VARCHAR    ,javaType=String    }
      ,#{P_ITEM_NM                 ,mode=IN    ,jdbcType=VARCHAR    ,javaType=String    }
      ,#{P_DELIVERY_CD             ,mode=IN    ,jdbcType=VARCHAR    ,javaType=String    }
      ,#{P_ORDERER_NM              ,mode=IN    ,jdbcType=VARCHAR    ,javaType=String    }
      ,#{P_SHIPPER_NM              ,mode=IN    ,jdbcType=VARCHAR    ,javaType=String    })
    }
  </select>
  <!-- [E] - LOM01010E0.RS_MASTER -->


  <!-- [S] - LOM01010E0.RS_DETAIL -->
  <select id="RS_DETAIL" statementType="CALLABLE" parameterType="hashmap">
    {  #{O_RESULT_DS               ,mode=OUT   ,jdbcType=CURSOR     ,javaType=ResultSet  ,resultMap=LOM01010E0_VIRTUALMAP} = call LOM01010E0.RS_DETAIL
      (#{P_CENTER_CD               ,mode=IN    ,jdbcType=VARCHAR    ,javaType=String    }
      ,#{P_BU_CD                   ,mode=IN    ,jdbcType=VARCHAR    ,javaType=String    }
      ,#{P_ORDER_DATE              ,mode=IN    ,jdbcType=TIMESTAMP  ,javaType=Date      }
      ,#{P_ORDER_NO                ,mode=IN    ,jdbcType=VARCHAR    ,javaType=String    })
    }
  </select>
  <!-- [E] - LOM01010E0.RS_DETAIL -->


  <!--
  //====================================================================================================================
  // INSERT/UPDATE/DELETE SQLMAP
  //====================================================================================================================
  -->
  <!-- [S] - INSERT_LO010PM [출고예정부가정보마스터(온라인고객)] -->
  <insert id="INSERT_LO010PM" parameterType="hashmap">
    INSERT INTO LO010PM
               (
                CENTER_CD
               ,BU_CD
               ,ORDER_DATE
               ,ORDER_NO
               <if test="P_REMARK1                 != null and P_REMARK1                 != ''">,REMARK1                     </if>
               <if test="P_REG_USER_ID             != null and P_REG_USER_ID             != ''">,REG_USER_ID                 </if>
               <if test="P_REG_DATETIME            != null and P_REG_DATETIME            != ''">,REG_DATETIME                </if>
               ,LAST_USER_ID
               ,LAST_DATETIME
               )
         VALUES
               (
                #{P_CENTER_CD}
               ,#{P_BU_CD}
               ,#{P_ORDER_DATE}
               ,#{P_ORDER_NO}
               <if test="P_REMARK1                 != null and P_REMARK1                 != ''">,#{P_REMARK1}                            </if>
               <if test="P_REG_USER_ID             != null and P_REG_USER_ID             != ''">,#{P_REG_USER_ID}                        </if>
               <if test="P_REG_DATETIME            != null and P_REG_DATETIME            != ''">
               ,DECODE(#{P_REG_DATETIME} ,'SYSDATE' ,SYSDATE ,WF.C_TO_DATETIME(#{P_REG_DATETIME}))
               </if>
               ,#{P_USER_ID}
               ,SYSDATE
               )
  </insert>
  <!-- [E] - INSERT_LO010PM [출고예정부가정보마스터(온라인고객)] -->


  <!-- [S] - UPDATE_LO010PM [출고예정부가정보마스터(온라인고객)] -->
  <update id="UPDATE_LO010PM" parameterType="hashmap">
    UPDATE LO010PM
       SET LAST_USER_ID             = #{P_USER_ID}
          ,LAST_DATETIME            = SYSDATE
          <if test="P_REMARK1                 != null">,REMARK1                  = #{P_REMARK1}                            </if>
          <if test="P_REG_USER_ID             != null">,REG_USER_ID              = #{P_REG_USER_ID}                        </if>
          <if test="P_REG_DATETIME            != null">
          ,REG_DATETIME             = DECODE(#{P_REG_DATETIME} ,'SYSDATE' ,SYSDATE ,WF.C_TO_DATETIME(#{P_REG_DATETIME}))
          </if>
     WHERE CENTER_CD                = #{P_CENTER_CD}
       AND BU_CD                    = #{P_BU_CD}
       AND ORDER_DATE               = #{P_ORDER_DATE}
       AND ORDER_NO                 = #{P_ORDER_NO}
  </update>
  <!-- [E] - UPDATE_LO010PM [출고예정부가정보마스터(온라인고객)] -->


  <!-- [S] - DELETE_LO010PM [출고예정부가정보마스터(온라인고객)] -->
  <delete id="DELETE_LO010PM" parameterType="hashmap">
    DELETE 
      FROM LO010PM
     WHERE CENTER_CD                = #{P_CENTER_CD}
     <!--<if test="P_BU_CD                   != null and P_BU_CD                   != ''">-->
       AND BU_CD                    = #{P_BU_CD}
     <!--</if>-->
     <!--<if test="P_ORDER_DATE              != null and P_ORDER_DATE              != ''">-->
       AND ORDER_DATE               = #{P_ORDER_DATE}
     <!--</if>-->
     <!--<if test="P_ORDER_NO                != null and P_ORDER_NO                != ''">-->
       AND ORDER_NO                 = #{P_ORDER_NO}
     <!--</if>-->
  </delete>
  <!-- [E] - DELETE_LO010PM [출고예정부가정보마스터(온라인고객)] -->


</mapper>