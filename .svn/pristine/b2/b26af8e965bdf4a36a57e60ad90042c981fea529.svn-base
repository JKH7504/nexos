<!DOCTYPE HTML>
<html lang="ko">
<head>
<!-- ${NEXOS_META} -->
<!-- ${NEXOS_LIBRARY} ${} ${../../../} -->
<!-- PROGRAM CSS/JS -->
<!-- <link rel="stylesheet" href="css/RIM01012P0.css" /> -->
<script src="js/RIM01012P0.js"></script>
</head>
<body>
    <div id="ctrPopupView" class="ctrCondition">
        <table class="ctrInner">
            <tr>
                <td>
                    <div id="lblCenter_Cd_F" class="lblCondition">물류센터</div>
                    <input id="edtQCenter_Cd_F" class="edtDisplay" type="text" style="width: 160px;" />
                </td>
                <td>
                    <div id="lblPopup_Bu_Cd" class="lblCondition">사업부</div>
                    <input id="edtQBu_Cd" class="edtDisplay" type="text" style="width: 60px;" />
                    <input id="edtQBu_Nm" class="edtDisplay" type="text" disabled="disabled" style="width: 120px;" />
                </td>
                <td>
                    <div id="lblQOutbound_Date" class="lblCondition">출고일자</div>
                    <input id="dtpQOutbound_Date1" class="dtpKey" type="text" style="width: 100px;" />
                    <span class="lblTerm">▶</span>
                    <input id="dtpQOutbound_Date2" class="dtpKey" type="text" style="width: 100px;" />
                </td>
                <td class="styFillArea styRight">
                    <input id="btnSearch" class="btnImgInquiry" type="button" value="조회" style="width: 60px;" />
                </td>
            </tr>
            <tr>
                <td>
                    <div id="lblQShipper_Nm" class="lblCondition">수령자명</div>
                    <input id="edtQShipper_Nm" class="edtNormal" type="text" style="width: 160px;" />
                </td>
                <td>
                    <div id="lblQBu_Date" class="lblCondition">전표일자</div>
                    <input id="dtpQBu_Date" class="dtpNormal" type="text" style="width: 100px;" />
                </td>
                <td>
                    <div id="lblQBu_No" class="lblCondition">전표번호</div>
                    <input id="edtQBu_No" class="edtNormal" type="text" style="width: 218px;" />
                </td>
            </tr>
        </table>
    </div>
    <div id="divMasterView" class="ctrPopup">
        <div>
            <div id="ctrAdditional_grdMaster" class="ctrAdditional top">
                <table class="ctrInner">
                    <tr>
                        <td>
                            <div id="lblOrder_Date" class="lblNormal">예정일자</div>
                            <input id="dtpOrder_Date" class="dtpKey" type="text" style="width: 100px;" />
                        </td>
                        <td>
                            <div id="lblInout_Cd" class="lblNormal">반입구분</div>
                            <select id="cboInout_Cd" class="cboKey" style="width: 150px;"></select>
                        </td>
                        <td>
                            <div id="lblItem_State" class="lblNormal">상품상태</div>
                            <select id="cboItem_State" class="cboKey" style="width: 100px;"></select>
                        </td>
                    </tr>
                </table>
            </div>
            <table class="ctrTitleBar">
                <tr>
                    <td id="ctrTitle_grdMaster" class="lblTitle">출고 정보</td>
                    <td id="ctrRows_grdMaster" class="lblRows">0/0 Rows</td>
                </tr>
            </table>
            <div id="grdMaster"></div>
        </div>
        <div>
            <table class="ctrTitleBar">
                <tr>
                    <td id="ctrTitle_grdDetail" class="lblTitle">출고 상세 정보</td>
                    <td id="ctrRows_grdDetail" class="lblRows">0/0 Rows</td>
                </tr>
            </table>
            <div id="grdDetail"></div>
        </div>
    </div>
    <div id="ctrPopupActionBar" class="ctrPopupActionBar">
        <input id="btnCreateRi" class="btnNormal" type="button" value="반입예정 생성" style="width: 120px;" />
        <input id="btnClose" class="btnNormal" type="button" value="닫기" style="width: 80px;" />
    </div>
</body>
</html>