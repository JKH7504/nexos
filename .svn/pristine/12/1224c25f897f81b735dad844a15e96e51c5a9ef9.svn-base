<!DOCTYPE HTML>
<html lang="ko">
<head>
<!-- ${NEXOS_META} -->
<!-- ${NEXOS_LIBRARY} ${} ${../../../} -->
<!-- PROGRAM CSS/JS -->
<!-- <link rel="stylesheet" href="css/LDC03030E0.css" /> -->
<script src="js/LDC03030E0.js"></script>
</head>
<body>
    <!-- 조회 조건 -->
    <div id="ctrConditionView" class="ctrCondition">
        <table class="ctrInner">
            <tr>
                <td>
                    <div id="lblQCenter_Cd" class="lblCondition">물류센터</div>
                    <select id="cboQCenter_Cd" class="cboKey" style="width: 150px;"></select>
                </td>
                <td>
                    <div id="lblQPopup_Bu_Cd" class="lblCondition">사업부</div>
                    <input id="edtQBu_Cd" class="edtNormal" type="text" style="width: 60px;" />
                    <input id="btnQBu_Cd" class="btnIcoPopup" type="button" />
                    <input id="edtQBu_Nm" class="edtDisplay" type="text" disabled="disabled" style="width: 120px;" />
                    <input id="edtQCust_Cd" class="edtHidden" type="text" disabled="disabled" />
                </td>
                <td>
                    <div id="lblOutbound_Date" class="lblCondition">출고일자</div>
                    <input id="dtpQOutbound_Date" class="dtpKey" type="text" style="width: 100px;" />
                </td>
            </tr>
            <tr>
                <td>
                    <div id="lblQOutbound_Batch" class="lblCondition">출고차수</div>
                    <select id="cboQOutbound_Batch" class="cboNormal" style="width: 150px;"></select>
                    <input id="btnQOutbound_Batch" class="btnIcoRefresh" type="button" />
                </td>
            </tr>
        </table>
    </div>
    <div id="divMasterView" class="ctrNormal">
        <div id="divDetailView" style="border-width: 0;">
            <div>
                <table class="ctrTitleBar">
                    <tr>
                        <td id="ctrTitle_grdMaster" class="lblTitle">유통구분별 집계내역</td>
                        <td id="ctrRows_grdMaster" class="lblRows">0/0 Rows</td>
                    </tr>
                </table>
                <div id="grdMaster"></div>
            </div>
            <div>
                <table class="ctrTitleBar">
                    <tr>
                        <td id="ctrTitle_grdDetail" class="lblTitle">유통구분별 송장내역</td>
                        <td>
                            <input id="btnBwDistributeDiv" class="btnImgCancel" type="button" value="운송장취소" style="width: 95px;" />
                        </td>
                        <td id="ctrRows_grdDetail" class="lblRows">0/0 Rows</td>
                    </tr>
                </table>
                <div id="grdDetail"></div>
            </div>
        </div>
        <div>
            <table class="ctrTitleBar">
                <tr>
                    <td id="ctrTitle_grdSub" class="lblTitle">미배차 운송장내역</td>
                    <td>
                        <input id="btnFwDistributeDiv" class="btnImgNormal" type="button" value="배차조정" style="width: 80px;" />
                    </td>
                    <td id="ctrRows_grdSub" class="lblRows">0/0 Rows</td>
                </tr>
            </table>
            <div id="grdSub"></div>
        </div>
