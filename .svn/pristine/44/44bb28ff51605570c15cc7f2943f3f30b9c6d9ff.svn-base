<!DOCTYPE HTML>
<html lang="ko">
<head>
<!-- ${NEXOS_META} -->
<!-- ${NEXOS_LIBRARY} ${} ${../../../} -->
<!-- PROGRAM CSS/JS -->
<!-- <link rel="stylesheet" href="css/LSC01010Q0.css" /> -->
<script src="js/LSC01010Q0.js"></script>
</head>
<body>
    <div id="ctrConditionView" class="ctrCondition">
        <!-- 조회 조건 -->
        <table class="ctrInner">
            <tr>
                <td>
                    <div id="lblQCenter_Cd" class="lblCondition">물류센터</div>
                    <select id="cboQCenter_Cd" class="cboKey" style="width: 150px;"></select>
                </td>
                <td>
                    <div id="lblQPopup_Bu_Cd" class="lblCondition">사업부</div>
                    <input id="edtQBu_Cd" class="edtKey" type="text" style="width: 60px;" />
                    <input id="btnQBu_Cd" class="btnIcoPopup" type="button" />
                    <input id="edtQBu_Nm" class="edtDisplay" type="text" disabled="disabled" style="width: 120px;" />
                    <input id="edtQCust_Cd" class="edtHidden" type="text" disabled="disabled" />
                </td>
                <td>
                    <div id="lblQItem_State" class="lblCondition">상품상태</div>
                    <select id="cboQItem_State" class="cboNormal" style="width: 150px;"></select>
                </td>
            </tr>
            <tr>
                <td>
                    <div id="lblQItem_Cd" class="lblCondition">상품코드</div>
                    <input id="edtQItem_Cd" class="edtNormal" type="text" style="width: 150px;" />
                </td>
                <td>
                    <div id="lblQItem_Bar_Cd" class="lblCondition">상품바코드</div>
                    <input id="edtQItem_Bar_Cd" class="edtNormal" type="text" style="width: 210px;" />
                </td>
                <td>
                    <div id="lblQItem_Nm" class="lblCondition">상품명</div>
                    <input id="edtQItem_Nm" class="edtNormal" type="text" style="width: 240px;" />
                </td>
            </tr>
            <tr>
                <td>
                    <div id="lblQZone_Cd" class="lblCondition">존구분</div>
                    <select id="cboQZone_Cd" class="cboNormal" style="width: 150px;"></select>
                </td>
                <td>
                    <div id="lblQBank_Cd" class="lblCondition">행</div>
                    <input id="edtQBank_Cd" class="edtNormal" type="text" maxlength="3" style="width: 60px;" />
                </td>
                <td id="tdQEmpty_Cell_Yn">
                    <div id="lblEmpty_Cell_Yn" class="lblCondition">빈셀포함여부</div>
                    <input id="chkEmpty_Cell_Yn" name="chkEmpty_Cell_Yn" class="chkNormal subData" type="checkbox" />
                </td>
            </tr>
        </table>
        <!-- 추가 조건 -->
        <table id="ctrAdditionalConditionView" class="ctrAdditionalCondition">
            <tr>
                <td id="tdQDsp_Brand_Cd">
                    <div id="lblQPopup_Brand_Cd" class="lblCondition">브랜드</div>
                    <input id="edtQBrand_Cd" class="edtNormal" type="text" style="width: 60px;" />
                    <input id="btnQBrand_Cd" class="btnIcoPopup" type="button" />
                    <input id="edtQBrand_Nm" class="edtDisplay" type="text" disabled="disabled" style="width: 120px;" />
                </td>
            </tr>
            <tr>
                <td>
                    <div id="lblQPopup_Dept_Cd" class="lblCondition">부서</div>
                    <input id="edtQDept_Cd" class="edtNormal" type="text" style="width: 60px;" />
                    <input id="btnQDept_Cd" class="btnIcoPopup" type="button" />
                    <input id="edtQDept_Nm" class="edtDisplay" type="text" disabled="disabled" style="width: 120px;" />
                </td>
            </tr>
            <tr>
                <td>
                    <div id="lblQPopup_Vendor_Cd" class="lblCondition">공급처</div>
                    <input id="edtQVendor_Cd" class="edtNormal" type="text" style="width: 60px;" />
                    <input id="btnQVendor_Cd" class="btnIcoPopup" type="button" />
                    <input id="edtQVendor_Nm" class="edtDisplay" type="text" disabled="disabled" style="width: 120px;" />
                </td>
            </tr>
            <tr>
                <td id="tdQDsp_Item_Lot">
                    <div id="lblQItem_Lot" class="lblCondition">LOT번호</div>
                    <input id="edtQItem_Lot" class="edtNormal" type="text" style="width: 210px;" />
                </td>
            </tr>
            <tr>
                <td id="tdQDsp_Depart_Cd">
                    <div id="lblQDepart_Cd" class="lblCondition">대분류</div>
                    <select id="cboQDepart_Cd" class="cboNormal" style="width: 210px;"></select>
                </td>
            </tr>
            <tr>
                <td id="tdQDsp_Line_Cd">
                    <div id="lblQLine_Cd" class="lblCondition">중분류</div>
                    <select id="cboQLine_Cd" class="cboNormal" style="width: 210px;"></select>
                </td>
            </tr>
            <tr>
                <td id="tdQDsp_Class_Cd">
                    <div id="lblQClass_Cd" class="lblCondition">소분류</div>
                    <select id="cboQClass_Cd" class="cboNormal" style="width: 210px;"></select>
                </td>
            </tr>
        </table>
    </div>
    <div id="divMasterView">
        <!-- Tab Handler -->
        <ul>
            <li id="divTab1"><a href="#divT1TabSheetView">상품별</a></li>
            <li id="divTab2"><a href="#divT2TabSheetView">로케이션별</a></li>
            <li id="divTab3"><a href="#divT3TabSheetView">전체</a></li>
            <li id="divTab4"><a href="#divT4TabSheetView">실재고현황</a></li>
        </ul>
        <!-- 상품별 입고내역 -->
        <div id="divT1TabSheetView">
            <div>
                <table class="ctrTitleBar">
                    <tr>
                        <td id="ctrTitle_grdT1Master" class="lblTitle">상품별 재고현황</td>
                        <td id="ctrRows_grdT1Master" class="lblRows">0/0 Rows</td>
                    </tr>
                </table>
                <div id="grdT1Master"></div>
            </div>
            <div>
                <table class="ctrTitleBar">
                    <tr>
                        <td id="ctrTitle_grdT1Detail" class="lblTitle">상품별 재고현황 상세내역</td>
                        <td id="ctrRows_grdT1Detail" class="lblRows">0/0 Rows</td>
                    </tr>
                </table>
                <div id="grdT1Detail"></div>
            </div>
        </div>
        <!-- 배송처별 입고내역 -->
        <div id="divT2TabSheetView">
            <div>
                <table class="ctrTitleBar">
                    <tr>
                        <td id="ctrTitle_grdT2Master" class="lblTitle">로케이션별 재고현황</td>
                        <td id="ctrRows_grdT2Master" class="lblRows">0/0 Rows</td>
                    </tr>
                </table>
                <div id="grdT2Master"></div>
            </div>
            <div>
                <table class="ctrTitleBar">
                    <tr>
                        <td id="ctrTitle_grdT2Detail" class="lblTitle">로케이션별 재고현황 상세내역</td>
                        <td id="ctrRows_grdT2Detail" class="lblRows">0/0 Rows</td>
                    </tr>
                </table>
                <div id="grdT2Detail"></div>
            </div>
        </div>
        <!-- 전체재고내역 -->
        <div id="divT3TabSheetView">
            <table class="ctrTitleBar">
                <tr>
                    <td id="ctrTitle_grdT3Master" class="lblTitle">전체 재고현황</td>
                    <td id="ctrRows_grdT3Master" class="lblRows">0/0 Rows</td>
                </tr>
            </table>
            <div id="grdT3Master"></div>
        </div>
        <!-- 실재고현황내역 -->
        <div id="divT4TabSheetView">
            <div>
                <table class="ctrTitleBar">
                    <tr>
                        <td id="ctrTitle_grdT4Master" class="lblTitle">실재고현황</td>
                        <td id="ctrRows_grdT4Master" class="lblRows">0/0 Rows</td>
                    </tr>
                </table>
                <div id="grdT4Master"></div>
            </div>
        </div>
    </div>
</body>
</html>
