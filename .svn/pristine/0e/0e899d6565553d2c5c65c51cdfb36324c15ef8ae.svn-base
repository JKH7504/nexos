<!DOCTYPE HTML>
<html lang="ko">
<head>
<!-- ${NEXOS_META} -->
<!-- ${NEXOS_LIBRARY} ${c} ${../../../} -->
<!-- PROGRAM CSS/JS -->
<!-- <link rel="stylesheet" href="css/LVC01010Q0.css" /> -->
<script src="js/LVC01010Q0.js"></script>
</head>
<body>
    <!-- 조회 조건 -->
    <div id="ctrConditionView" class="ctrCondition">
        <table class="ctrInner">
            <tr>
                <td>
                    <div id="lblQAgg_Date" class="lblCondition">조회일자</div>
                    <input id="dtpQAgg_Date1" class="dtpKey" type="text" style="width: 100px;" />
                    <span class="lblTerm">▶</span>
                    <input id="dtpQAgg_Date2" class="dtpKey" type="text" style="width: 100px;" />
                </td>
                <td>
                    <div id="lblQCenter_Cd" class="lblCondition">물류센터</div>
                    <select id="cboQCenter_Cd" class="cboNormal" style="width: 150px;"></select>
                </td>
                <td>&nbsp;</td>
            </tr>
            <tr>
                <td>
                    <div id="lblPopup_Cust_Cd" class="lblCondition">고객사</div>
                    <input id="edtQCust_Cd" class="edtNormal" type="text" style="width: 60px;" />
                    <input id="btnQCust_Cd" class="btnIcoPopup" type="button" />
                    <input id="edtQCust_Nm" class="edtDisplay" type="text" disabled="disabled" style="width: 130px;" />
                </td>
                <td>
                    <div id="lblQPopup_Bu_Cd" class="lblCondition">사업부</div>
                    <input id="edtQBu_Cd" class="edtNormal" type="text" style="width: 60px;" />
                    <input id="btnQBu_Cd" class="btnIcoPopup" type="button" />
                    <input id="edtQBu_Nm" class="edtDisplay" type="text" disabled="disabled" style="width: 130px;" />
                </td>
                <td id="tdQDsp_Brand_Cd">
                    <div id="lblQPopup_Brand_Cd" class="lblCondition">브랜드</div>
                    <input id="edtQBrand_Cd" class="edtNormal" type="text" style="width: 60px;" />
                    <input id="btnQBrand_Cd" class="btnIcoPopup" type="button" />
                    <input id="edtQBrand_Nm" class="edtDisplay" type="text" disabled="disabled" style="width: 130px;" />
                </td>
            </tr>
        </table>
    </div>
    <div id="divLeftView" class="ctrLeft">
        <div id="divOrderCntView">
            <table id="ctrTitle_chtOrderCnt" class="ctrTitleBar">
                <tr>
                    <td id="dspTitle_Order_Cnt" class="lblTitle">전표수</td>
                </tr>
            </table>
            <div id="ctrAdditional_chtOrderCnt" class="ctrAdditional bottom styRight">
                <div id="lblTotal_Order_Cnt" class="lblNormal">전체 전표수</div>
                <input id="edtTotal_Order_Cnt" class="edtNormal" type="text" readonly="readonly" style="width: 80px;" />
            </div>
            <div id="chtOrderCnt" class="ctrGroup"></div>
        </div>
        <div id="divLineCntView">
            <table id="ctrTitle_chtLineCnt" class="ctrTitleBar">
                <tr>
                    <td id="dspTitle_Line_Cnt" class="lblTitle">라인수</td>
                </tr>
            </table>
            <div id="ctrAdditional_chtLineCnt" class="ctrAdditional bottom styRight">
                <div id="lblTotal_Line_Cnt" class="lblNormal">전체 라인수</div>
                <input id="edtTotal_Line_Cnt" class="edtNormal" type="text" readonly="readonly" style="width: 80px;" />
            </div>
            <div id="chtLineCnt"></div>
        </div>
    </div>
    <div id="divRightView" class="ctrRight">
        <div id="divOrderQtyView">
            <table id="ctrTitle_chtOrderQty" class="ctrTitleBar">
                <tr>
                    <td id="dspTitle_Order_Qty" class="lblTitle">수량</td>
                </tr>
            </table>
            <div id="ctrAdditional_chtOrderQty" class="ctrAdditional bottom styRight">
                <div id="lblTotal_Order_Qty" class="lblNormal">전체 수량</div>
                <input id="edtTotal_Order_Qty" class="edtNormal" type="text" readonly="readonly" style="width: 80px;" />
            </div>
            <div id="chtOrderQty"></div>
        </div>
        <div id="divOrderAnalysisView">
            <table id="ctrTitle_chtOrderAnalysis" class="ctrTitleBar">
                <tr>
                    <td id="dspTitle_Order_Analysis" class="lblTitle">주문분석</td>
                </tr>
            </table>
            <div id="ctrAdditional_chtOrderAnalysis" class="ctrAdditional bottom styRight">
                <div id="lblTotal_Order_Analysis" class="lblNormal">전체 전표수</div>
                <input id="edtTotal_Order_Analysis" class="edtNormal formatterInteger" type="text" readonly="readonly" style="width: 80px;" />
            </div>
            <div id="chtOrderAnalysis"></div>
        </div>
    </div>
</body>
</html>
