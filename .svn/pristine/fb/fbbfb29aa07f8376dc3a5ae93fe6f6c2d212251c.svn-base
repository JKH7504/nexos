/**
 * 거래명세서 수정 버튼 클릭 처리
 */
function btnInoutDateDOnClick() {

    if (!$NC.getProgramPermission().canSave) {
        alert($NC.getDisplayMsg("JS.MAIN.001", "해당 프로그램의 저장권한이 없습니다."));
        return;
    }

    var rCount = G_GRDMASTERD.data.getLength();
    if (rCount == 0) {
        alert($NC.getDisplayMsg("JS.LOM02010E0.027", "조회 후 처리하십시오."));
        return;
    }

    if (!confirm($NC.getDisplayMsg("JS.LOM02010E0.095", "거래명세서일자를 수정하시겠습니까?"))) {
        return;
    }

    if (G_GRDMASTERD.view.getEditorLock().isActive()) {
        G_GRDMASTERD.view.getEditorLock().commitCurrentEdit();
    }

    if ($NC.isGridModified(G_GRDSUBD)) {
        alert($NC.getDisplayMsg("JS.LOM02010E0.096", "데이터가 수정되었습니다. 저장 후 거래명세서일자 수정 처리하십시오."));
        return;
    }

    var dsMaster = [];
    var PROCESS_STATE = $NC.G_VAR.stateFWBW[$NC.G_VAR.activeView.PROCESS_CD].CONFIRM;
    var INOUT_DATE = $NC.getValue("#dtpInout_DateD");
    var checkedCount = 0;
    var rowData;
    for (var rIndex = 0; rIndex < rCount; rIndex++) {
        rowData = G_GRDMASTERD.data.getItem(rIndex);
        if (rowData.CHECK_YN != $ND.C_YES) {
            continue;
        }
        checkedCount++;
        // 출고지시 상태인 전표만 대상
        if (rowData.OUTBOUND_STATE != PROCESS_STATE) {
            continue;
        }
        dsMaster.push({
            P_CENTER_CD: rowData.CENTER_CD,
            P_BU_CD: rowData.BU_CD,
            P_OUTBOUND_DATE: rowData.OUTBOUND_DATE,
            P_OUTBOUND_NO: rowData.OUTBOUND_NO,
            P_INOUT_DATE: INOUT_DATE,
            P_USER_ID: $NC.G_USERINFO.USER_ID
        });
    }

    if (checkedCount == 0) {
        alert($NC.getDisplayMsg("JS.LOM02010E0.097", "거래명세서일자를 수정할 데이터를 선택하십시오."));
        return;
    }
    if (dsMaster.length == 0) {
        alert($NC.getDisplayMsg("JS.LOM02010E0.098", "선택한 데이터 중 거래명세서일자 수정 가능한 데이터가 없습니다."));
        return;
    }

    $NC.serviceCall("/LOM02010E0/callInoutDateUpdate.do", {
        P_DS_MASTER: dsMaster
    }, onSaveD, onSaveErrorD);
}

function grdSubOnCalc(rowData, changedColumnId) {

    switch (changedColumnId) {
        case "CONFIRM_BOX":
        case "CONFIRM_EA":
            rowData.CONFIRM_QTY = $NC.getBQty(rowData.CONFIRM_BOX, rowData.CONFIRM_EA, rowData.QTY_IN_BOX);
            break;
    }

    rowData.CONFIRM_BOX = $NC.getBBox(rowData.CONFIRM_QTY, rowData.QTY_IN_BOX);
    rowData.CONFIRM_EA = $NC.getBEa(rowData.CONFIRM_QTY, rowData.QTY_IN_BOX);
    rowData.CONFIRM_WEIGHT = $NC.getWeight(rowData.CONFIRM_QTY, rowData.QTY_IN_BOX, rowData.BOX_WEIGHT);

    return rowData;
}