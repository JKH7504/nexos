<!DOCTYPE HTML>
<html lang="ko">
<head>
<!-- ${NEXOS_META} -->
<!-- ${NEXOS_LIBRARY} ${} ${../../../} -->
<!-- PROGRAM CSS/JS -->
<!-- <link rel="stylesheet" href="css/LSC02030Q1.css" /> -->
<script src="js/LSC02030Q1.js"></script>
</head>
<body>
    <div id="ctrConditionView" class="ctrCondition">
        <!-- 조회 조건 -->
        <table class="ctrInner">
            <tr>
                <td>
                    <div id="lblQCenter_Cd" class="lblCondition">물류센터</div>
                    <select id="cboQCenter_Cd" class="cboKey" style="width: 150px;"></select>
                </td>
                <td>
                    <div id="lblQPopup_Bu_Cd" class="lblCondition">사업부</div>
                    <input id="edtQBu_Cd" class="edtKey" type="text" style="width: 60px;" />
                    <input id="btnQBu_Cd" class="btnIcoPopup" type="button" />
                    <input id="edtQBu_Nm" class="edtDisplay" type="text" disabled="disabled" style="width: 120px;" />
                </td>
                <td id="tdInout_Date">
                    <div id="lblQInout_Date" class="lblCondition">수불일자</div>
                    <input id="dtpQInout_Date1" class="dtpKey" type="text" style="width: 100px;" />
                    <span class="lblTerm">▶</span>
                    <input id="dtpQInout_Date2" class="dtpKey" type="text" style="width: 100px;" />
                </td>
                <td id="tdInout_Month" style="display: none;">
                    <div id="lblQInout_Month" class="lblCondition">수불월</div>
                    <input id="mtpInout_Month" class="mtpKey" type="text" style="width: 100px;" />
                </td>
            </tr>
            <tr>
                <td>
                    <div id="lblQItem_State" class="lblCondition">상품상태</div>
                    <select id="cboQItem_State" class="cboNormal" style="width: 150px;"></select>
                </td>
                <td id="tdViewDiv">
                    <div id="lblQView_Div" class="lblCondition">데이터구분</div>
                    <select id="cboQView_Div" class="cboNormal" style="width: 120px;"></select>
                </td>
            </tr>
        </table>
        <!-- 추가 조건 -->
        <table id="ctrAdditionalConditionView" class="ctrAdditionalCondition">
            <tr>
                <td id="tdQDsp_Brand_Cd">
                    <div id="lblQPopup_Brand_Cd" class="lblCondition">브랜드</div>
                    <input id="edtQBrand_Cd" class="edtNormal" type="text" style="width: 60px;" />
                    <input id="btnQBrand_Cd" class="btnIcoPopup" type="button" />
                    <input id="edtQBrand_Nm" class="edtDisplay" type="text" disabled="disabled" style="width: 120px;" />
                </td>
            </tr>
            <tr>
                <td>
                    <div id="lblQStyle_Cd" class="lblCondition">스타일코드</div>
                    <input id="edtQStyle_Cd" class="edtNormal" type="text" style="width: 210px;" />
                </td>
            </tr>
            <tr>
                <td>
                    <div id="lblQItem_Nm" class="lblCondition">상품명</div>
                    <input id="edtQItem_Nm" class="edtNormal" type="text" style="width: 210px;" />
                </td>
            </tr>
            <tr>
                <td>
                    <div id="lblQYear_Div" class="lblCondition">연도구분</div>
                    <select id="cboQYear_Div" class="cboNormal" style="width: 210px;"></select>
                </td>
            </tr>
            <tr>
                <td>
                    <div id="lblQSeason_Div" class="lblCondition">시즌구분</div>
                    <select id="cboQSeason_Div" class="cboNormal" style="width: 210px;"></select>
                </td>
            </tr>
            <tr>
                <td>
                    <div id="lblQItem_Div" class="lblCondition">상품구분</div>
                    <select id="cboQItem_Div" class="cboNormal" style="width: 210px;"></select>
                </td>
            </tr>
        </table>
    </div>
    <div id="divMasterView">
        <!-- Tab Handler -->
        <ul>
            <li id="divTab1"><a href="#divT1TabSheetView">입출고 수불내역</a></li>
            <li id="divTab2"><a href="#divT2TabSheetView">일자별 수불내역</a></li>
            <li id="divTab3"><a href="#divT3TabSheetView">기간별 수불내역</a></li>
            <li id="divTab4"><a href="#divT4TabSheetView">월단위 수불내역</a></li>
        </ul>
        <!-- 입출고구분별 수불내역 -->
        <div id="divT1TabSheetView">
            <table class="ctrTitleBar">
                <tr>
                    <td id="ctrTitle_grdT1Master" class="lblTitle">입출고 수불내역</td>
                    <td id="ctrRows_grdT1Master" class="lblRows">0/0 Rows</td>
                </tr>
            </table>
            <div id="grdT1Master"></div>
        </div>
        <!-- 일자별 수불내역 -->
        <div id="divT2TabSheetView">
            <table class="ctrTitleBar">
                <tr>
                    <td id="ctrTitle_grdT2Master" class="lblTitle">일자별 수불내역</td>
                    <td id="ctrRows_grdT2Master" class="lblRows">0/0 Rows</td>
                </tr>
            </table>
            <div id="grdT2Master"></div>
        </div>
        <!-- 기간별 수불내역 -->
        <div id="divT3TabSheetView">
            <table class="ctrTitleBar">
                <tr>
                    <td id="ctrTitle_grdT3Master" class="lblTitle">기간별 수불내역</td>
                    <td id="ctrRows_grdT3Master" class="lblRows">0/0 Rows</td>
                </tr>
            </table>
            <div id="grdT3Master"></div>
        </div>
        <!-- 월단위 수불내역 -->
        <div id="divT4TabSheetView">
            <table class="ctrTitleBar">
                <tr>
                    <td id="ctrTitle_grdT4Master" class="lblTitle">월단위 수불내역</td>
                    <td id="ctrRows_grdT4Master" class="lblRows">0/0 Rows</td>
                </tr>
            </table>
            <div id="grdT4Master"></div>
        </div>
    </div>
</body>
</html>
