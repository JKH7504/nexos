<!DOCTYPE HTML>
<html lang="ko">
<head>
<!-- ${NEXOS_META} -->
<!-- ${NEXOS_LIBRARY} ${} ${../../../} -->
<!-- PROGRAM CSS/JS -->
<!-- <link rel="stylesheet" href="css/LCC02081P0.css" /> -->
<script src="js/LCC02081P0.js"></script>
</head>
<body>
    <div id="ctrPopupView" class="ctrPopup">
        <table class="ctrInner">
            <tr>
                <td>
                    <div id="lblCenter_Cd_F" class="lblNormal">물류센터</div>
                    <input id="edtCenter_Cd_F" class="edtDisplay" type="text" style="width: 170px;" />
                </td>
                <td class="styFillArea" rowspan="6">
                    <div id="ctrInboundCondition" class="ctrGroupBox styFillArea ctrSubCondition">
                        <table class="ctrInner">
                            <tr>
                                <td colspan="2">
                                    <div id="lblQInbound_Date" class="lblCondition">입고일자</div>
                                    <input id="dtpQInbound_Date1" class="dtpKey" type="text" style="width: 100px;" />
                                    <span class="lblTerm">▶</span>
                                    <input id="dtpQInbound_Date2" class="dtpKey" type="text" style="width: 100px;" />
                                </td>
                                <td class="styFillArea styRight">
                                    <input id="btnSearchInbound" class="btnImgInquiry" type="button" value="입고검색" style="margin-left: -80px; width: 80px;" />
                                </td>
                            </tr>
                            <tr>
                                <td colspan="3">
                                    <div id="lblQPopup_Vendor_Cd" class="lblCondition">공급처</div>
                                    <input id="edtQVendor_Cd" class="edtNormal" type="text" style="width: 60px;" />
                                    <input id="btnQVendor_Cd" class="btnIcoPopup" type="button" />
                                    <input id="edtQVendor_Nm" class="edtDisplay" type="text" disabled="disabled" style="width: 130px;" />
                                </td>
                            </tr>
                            <tr>
                                <td>
                                    <div id="lblQShipment_No" class="lblCondition">SHIPMENT번호</div>
                                    <input id="edtQShipment_No" class="edtNormal" type="text" style="width: 120px;" />
                                </td>
                                <td>
                                    <div id="lblQBu_No" class="lblCondition">전표번호</div>
                                    <input id="edtQBu_No" class="edtNormal" type="text" style="width: 120px;" />
                                </td>
                            </tr>
                            <tr>
                                <td id="tdQDsp_Brand_Cd" colspan="3">
                                    <div id="lblQPopup_Brand_Cd" class="lblCondition">브랜드</div>
                                    <input id="edtQBrand_Cd" class="edtNormal" type="text" style="width: 80px;" />
                                    <input id="btnQBrand_Cd" class="btnIcoPopup" type="button" />
                                    <input id="edtQBrand_Nm" class="edtDisplay" type="text" disabled="disabled" style="width: 120px;" />
                                    <input id="edtQCust_Cd" class="edtHidden" type="text" disabled="disabled" />
                                </td>
                            </tr>
                            <tr>
                                <td>
                                    <div id="lblQItem_Cd" class="lblCondition">상품코드</div>
                                    <input id="edtQItem_Cd" class="edtNormal" type="text" style="width: 120px;" />
                                </td>
                                <td>
                                    <div id="lblQItem_Nm" class="lblCondition">상품명</div>
                                    <input id="edtQItem_Nm" class="edtNormal" type="text" style="width: 120px;" />
                                </td>
                            </tr>
                            <tr>
                                <td>
                                    <div id="lblQItem_Lot" class="lblCondition">LOT번호</div>
                                    <input id="edtQItem_Lot" class="edtNormal" type="text" style="width: 120px;" />
                                </td>
                            </tr>
                        </table>
                    </div>
                </td>
            </tr>
            <tr>
                <td>
                    <div id="lblPopup_Bu_Cd" class="lblNormal">사업부</div>
                    <input id="edtBu_Cd" class="edtDisplay" type="text" style="width: 60px;" />
                    <input id="edtBu_Nm" class="edtDisplay" type="text" disabled="disabled" style="width: 104px;" />
                </td>
            </tr>
            <tr>
                <td>
                    <div id="lblEtc_Date" class="lblKey">입출고일자</div>
                    <input id="dtpEtc_Date" class="dtpKey" type="text" style="width: 100px;" />
                </td>
            </tr>
            <tr>
                <td>
                    <div id="lblEtc_No" class="lblNormal">입출고번호</div>
                    <input id="edtEtc_No" class="edtNormal" type="text" disabled="disabled" style="width: 100px;" />
                </td>
            </tr>
            <tr>
                <td>
                    <div id="lblInout_Cd" class="lblKey">입출고구분</div>
                    <select id="cboInout_Cd" class="cboKey" style="width: 170px;"></select>
                </td>
            </tr>
            <tr>
                <td>
                    <div id="lblRemark1" class="lblNormal">비고</div>
                    <input id="edtRemark1" class="edtNormal" type="text" maxlength="255" style="width: 170px;" />
                </td>
            </tr>
        </table>
    </div>
    <!-- 입고전표내역 -->
    <div id="ctrSub1View" class="ctrPopup">
        <table class="ctrTitleBar">
            <tr>
                <td id="ctrTitle_grdSub1" class="lblTitle">입고내역</td>
                <td id="ctrRows_grdSub1" class="lblRows">0/0 Rows</td>
            </tr>
        </table>
        <div id="grdSub1"></div>
    </div>
    <!-- 파렛트내역 -->
    <div id="ctrSub2View" class="ctrPopup">
        <div id="ctrAdditional_grdSub2" class="ctrAdditional top">
            <table class="ctrInner">
                <tr>
                    <td class="styFillArea">
                    </td>
                    <td>
                        <div id="lblConvert_Item_State" class="lblNormal">변환상태</div>
                        <select id="cboItem_State" class="edtNormal" style="width: 100px;"></select>
                        <input id="btnChange_Item_State" class="btnNormal" type="button" value="수정" style="width: 50px;">
                    </td>
                    <td>
                        <div id="lblLocation_Cd" class="lblNormal">이동로케이션</div>
                        <input id="edtLocation_Cd" class="edtNormal" type="text" maxlength="20" style="width: 90px;" />
                        <input id="btnLocation_Cd" class="btnIcoPopup" type="button" />
                        <input id="btnChange_Location_Cd" class="btnNormal" type="button" value="수정" style="width: 50px;">
                    </td>
                    <td>
                        <div id="lblEtc_Div_F" class="lblNormal">사유구분</div>
                        <select id="cboEtc_Div" class="edtNormal" style="width: 100px;"></select>
                        <input id="btnChange_Etc_Div" class="btnNormal" type="button" value="수정" style="width: 50px;">
                    </td>
                </tr>
            </table>
        </div>
        <table class="ctrTitleBar">
            <tr>
                <td id="ctrTitle_grdSub2" class="lblTitle">입고 파렛트내역</td>
                <td id="ctrRows_grdSub2" class="lblRows">0/0 Rows</td>
            </tr>
        </table>
        <div id="grdSub2"></div>
    </div>
    <div id="ctrPopupActionBar" class="ctrPopupActionBar">
        <input id="btnSave" class="btnNormal" type="button" value="저장" style="width: 80px;" />
        <input id="btnClose" class="btnNormal" type="button" value="닫기" style="width: 80px;" />
    </div>
</body>
</html>
