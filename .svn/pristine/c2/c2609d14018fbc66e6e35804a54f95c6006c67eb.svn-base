/**
 * 저장
 */
function _Save() {

    if (G_GRDDETAIL.data.getLength() == 0) {
        alert($NC.getDisplayMsg("JS.LCC04011P0.024", "저장할 데이터가 없습니다."));
        return;
    }

    // 마지막 선택 Row Validation 체크
    if (!$NC.isGridValidLastRow(G_GRDDETAIL)) {
        return;
    }

    var dsDetail = [];
    var dsTarget = G_GRDDETAIL.data.getItems();
    var rowData, GAP_DIV;
    for (var rIndex = 0, rCount = dsTarget.length; rIndex < rCount; rIndex++) {
        rowData = dsTarget[rIndex];
        if (rowData.CRUD == $ND.C_DV_CRUD_R) {
            continue;
        }
        GAP_DIV = rowData.GAP_DIV;
        if ($NC.isNull(GAP_DIV) && rowData.STOCK_QTY != rowData.INVEST_QTY) {
            GAP_DIV = $ND.C_REASON_ETC_DIV;
        }
        dsDetail.push({
            P_CENTER_CD: $NC.G_VAR.masterData.CENTER_CD,
            P_BU_CD: $NC.G_VAR.masterData.BU_CD,
            P_INVEST_DATE: $NC.G_VAR.masterData.INVEST_DATE,
            P_INVEST_NO: $NC.G_VAR.masterData.INVEST_NO,
            P_LINE_NO: rowData.LINE_NO,
            P_LOCATION_CD: rowData.LOCATION_CD,
            P_BRAND_CD: rowData.BRAND_CD,
            P_ITEM_CD: rowData.ITEM_CD,
            P_ITEM_STATE: rowData.ITEM_STATE,
            P_ITEM_LOT: rowData.ITEM_LOT,
            P_VALID_DATE: rowData.VALID_DATE,
            P_BATCH_NO: rowData.BATCH_NO,
            P_STOCK_QTY: rowData.STOCK_QTY,
            P_INVEST_QTY: rowData.INVEST_QTY,
            P_GAP_DIV: GAP_DIV,
            P_CRUD: rowData.CRUD
        });
    }

    if ($NC.G_VAR.masterData.CRUD == $ND.C_DV_CRUD_R && dsDetail.length == 0) {
        alert($NC.getDisplayMsg("JS.LCC04011P0.025", "수정 후 저장하십시오."));
        return;
    }

    $NC.serviceCall("/LCC04010E0/save.do", {
        P_DS_MASTER: {
            P_CENTER_CD: $NC.G_VAR.masterData.CENTER_CD,
