function btnUploadXLSOnClick() {

    if ($NC.isNull($NC.G_VAR.masterData.CENTER_CD)) {
        alert($NC.getDisplayMsg("JS.LCC04011P0.034", "물류센터를 입력하십시오."));
        return;
    }
    if ($NC.isNull($NC.G_VAR.masterData.BU_CD)) {
        alert($NC.getDisplayMsg("JS.LCC04011P0.035", "사업부를 입력하십시오."));
        return;
    }
    if ($NC.isNull($NC.G_VAR.masterData.INVEST_DATE)) {
        alert($NC.getDisplayMsg("JS.LCC04011P0.036", "실사일자를 입력하십시오."));
        $NC.setFocus("#dtpInvest_Date");
        return;
    }

    if ($NC.isNotNull($NC.G_VAR.masterData.INVEST_NO)) {
        alert($NC.getDisplayMsg("JS.LCC04011P0.037", "엑셀 업로드는 기존 데이터에 추가할 수 없습니다. 저장 후 신규로 처리하십시오."));
        return;
    }

    if (G_GRDDETAIL.data.getLength() > 0) {
        if (!confirm($NC.getDisplayMsg("JS.LCC04011P0.038", "엑셀 업로드시 기존 내역 삭제 후 추가됩니다. 엑셀 업로드하시겠습니까?"))) {
            return;
        }
    }

    var COLUMN_INFO = [
        {
            P_COLUMN_NM: "LOCATION_CD",
            P_XLS_COLUMN_NM: "A"
        },
        {
            P_COLUMN_NM: "BRAND_CD",
            P_XLS_COLUMN_NM: "B"
        },
        {
            P_COLUMN_NM: "ITEM_CD",
            P_XLS_COLUMN_NM: "C"
        },
        {
            P_COLUMN_NM: "ITEM_STATE",
            P_XLS_COLUMN_NM: "D"
        },
        {
            P_COLUMN_NM: "ITEM_LOT",
            P_XLS_COLUMN_NM: "E"
        }
    ];
    if ($NC.G_VAR.G_PARAMETER.P_POLICY_LS210 == "2" && $NC.G_VAR.masterData.DATA_CREATE_DIV == "2") {
        COLUMN_INFO.push({
            P_COLUMN_NM: "VALID_DATE",
            P_XLS_COLUMN_NM: "F"
        });
        COLUMN_INFO.push({
            P_COLUMN_NM: "BATCH_NO",
            P_XLS_COLUMN_NM: "G"
        });
        COLUMN_INFO.push({
            P_COLUMN_NM: "INVEST_QTY",
            P_XLS_COLUMN_NM: "H"
        });
    } else {
        COLUMN_INFO.push({
            P_COLUMN_NM: "INVEST_QTY",
            P_XLS_COLUMN_NM: "F"
        });
    }

    $NC.excelFileUpload({
        P_QUERY_ID: "LCC04010E0.RS_SUB2",
        P_QUERY_PARAMS: {
            P_CENTER_CD: $NC.G_VAR.masterData.CENTER_CD,
            P_BU_CD: $NC.G_VAR.masterData.BU_CD,
            P_DATA_CREATE_DIV: $NC.G_VAR.masterData.DATA_CREATE_DIV,
            P_POLICY_LC110: $NC.G_VAR.G_PARAMETER.P_POLICY_LC110,
            P_POLICY_LS210: $NC.G_VAR.G_PARAMETER.P_POLICY_LS210
        },
        P_SERVICE_PARAMS: {
            P_XLS_COL_CHECK_YN: $ND.C_YES,
            P_XLS_FIRST_ROW: 3,
            P_XLS_FILE_DIV: "INVEST"
        },
        P_COLUMN_INFO: COLUMN_INFO
    }, function(ajaxData, dsResultData) {

        if ($NC.isNull(dsResultData) || dsResultData.length == 0) {
            alert($NC.getDisplayMsg("JS.LCC04011P0.039", "업로드할 수 있는 대상 데이터가 없습니다. 엑셀 파일을 확인하십시오."));
            return;
        }

        for (var row = 0; row < dsResultData.length; row++) {
            dsResultData[row].LINE_NO = "";
            dsResultData[row].CRUD = $ND.C_DV_CRUD_C;
        }

        $NC.setInitGridData(G_GRDDETAIL, dsResultData);
        $NC.setGridSelectRow(G_GRDDETAIL, 0);
        $NC.setEnable("#cboInvest_Div", false);
    });
}

function grdDetailOnCalc(rowData, changedColumnId) {

    switch (changedColumnId) {
        case "INVEST_BOX":
        case "INVEST_EA":
            rowData.INVEST_QTY = $NC.getBQty(rowData.INVEST_BOX, rowData.INVEST_EA, rowData.QTY_IN_BOX);
            break;
    }

    rowData.INVEST_BOX = $NC.getBBox(rowData.INVEST_QTY, rowData.QTY_IN_BOX);
    rowData.INVEST_EA = $NC.getBEa(rowData.INVEST_QTY, rowData.QTY_IN_BOX);

    return rowData;
}
