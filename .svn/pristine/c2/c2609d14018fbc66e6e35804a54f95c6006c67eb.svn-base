/**
 * 화면 초기화 - 화면 로드시 자동 호출 됨
 */
function _Initialize() {

    // 단위화면에서 사용될 일반 전역 변수 정의
    $NC.setGlobalVar({
        autoResizeView: {
            container: "#divDetailView",
            grids: [
                "#grdDetail"
            ]
        },
        // 마스터 데이터
        masterData: null,
        DIV_LOC: "_____", // '_' 다섯개
        // 체크할 정책 값
        policyVal: {
            CM120: "", // 로케이션표시정책
            CM121: "", // 로케이션 존 길이
            CM122: "", // 로케이션 행 길이
            CM123: "", // 로케이션 열 길이
            CM124: "" // 로케이션 단 길이
        }
    });

    $NC.setInitDatePicker("#dtpInvest_Date"); // 실사일자
    $NC.setInitDateRangePicker("#dtpInvest_Start_Date", "#dtpInvest_End_Date"); // 실사시작일자, 실사종료일자
    $NC.setInitDateRangePicker("#dtpQInout_Date1", "#dtpQInout_Date2"); // 수불시작일자, 수불종료일자
    $NC.setInitDatePicker("#dtpValid_Date", null, "N"); // 유효기한

    $NC.setValue("#edtItem_Lot", $ND.C_BASE_ITEM_LOT);

    // 버튼 클릭 이벤트 연결
    $("#btnManager_Id").click(showUserPopup);
    $("#btnQItem_Cd").click(showQItemPopup);
    $("#btnKItem_Cd").click(showKItemPopup);
    $("#btnLocation").click(showLocationPopup); // 로케이션 검색 버튼
    $("#btnQLocation").click(showQLocationPopup); // 로케이션 검색 버튼
    $("#btnClose").click(onCancel);
    $("#btnSave").click(_Save);
    $("#btnAddData").click(_New); // 자료추가 버튼 클릭
    $("#btnDelete").click(_Delete); // 그리드 행 삭제버튼
    $("#btnQBrand_Cd").click(showQBuBrandPopup);
    $("#btnKBrand_Cd").click(showKBuBrandPopup);
    $("#btnDownloadXLSFormat").click(btnDownloadXLSFormatOnClick); // 엑셀포맷 다운로드
    $("#btnUploadXLS").click(btnUploadXLSOnClick); // 엑셀업로드

    $NC.setEnable("#edtCenter_Cd_F", false); // 물류센터 비활성화
    $NC.setEnable("#edtBu_Cd", false); // 사업부 비활성화
    $NC.setEnable("#edtInvest_No", false); // 실사번호 비활성화

    $("#tdData_Create_Div").css("visibility", "hidden"); // 숨김: 공간 사용
    $NC.setVisible("#grpKeyBrandInfo", false);
    $NC.setVisible("#grpConditionBrandInfo");
    $NC.setVisible("#grpDisplayBrandInfo", false);

    $NC.setVisible("#grpInoutInfo", false); // 입출고정보
    $NC.setVisible("#grpLocationInfo", false); // 로케이션정보
    $NC.setVisible("#grpItemGroupInfo", false); // 상품그룹정보
    $NC.setVisible("#grpItemInfo", false); // 상품&로케이션정보
    $NC.setVisible("#grpItemStateInfo"); // 상품&로케이션
    $NC.setVisible("#grpItemAddInfo", false); // 상품입력정보
    $NC.setVisible("#grpRandomInfo", false); // 랜덤정보

    // 자료추가 조회조건 사이즈 조정
    $("#divAddCondition").height($("#divAddCondition").parent().height() - $NC.getViewOffset("#divAddCondition").height);
    // $NC.setGlobalVar 시점에 제외 사이즈 계산 불가(조회 조건이 모두 보이는 상태이므로)로 조회 조건 조정 후 계산
    $NC.G_VAR.autoResizeView.exceptHeight = $NC.getViewHeight("#ctrPopupView");

    // 그리드 초기화
    grdDetailInitialize();

    // 콤보박스 초기화
    $NC.serviceCall("/WC/getMultiDataSet.do", {
        P_SERVICE_PARAMS: [
            {
                P_RESULT_ID: "O_WC_POP_CMCODE_ITEM_STATE_2",
                P_QUERY_ID: "WC.POP_CMCODE",
                P_QUERY_PARAMS: {
                    P_COMMON_GRP: "ITEM_STATE",
                    P_COMMON_CD: $ND.C_ALL,
                    P_ATTR01_CD: "1",
                    P_VIEW_DIV: "2" // 1:등록팝업, 2:조회팝업
                }
            },
            {
                P_RESULT_ID: "O_WC_POP_CMCODE_ITEM_STATE_1",
                P_QUERY_ID: "WC.POP_CMCODE",
                P_QUERY_PARAMS: {
                    P_COMMON_GRP: "ITEM_STATE",
                    P_COMMON_CD: $ND.C_ALL,
                    P_ATTR01_CD: "1",
                    P_VIEW_DIV: "1" // 1:등록팝업, 2:조회팝업
                }
            },
            {
                P_RESULT_ID: "O_WC_POP_CMCODE_KEEP_DIV",
                P_QUERY_ID: "WC.POP_CMCODE",
                P_QUERY_PARAMS: {
                    P_COMMON_GRP: "KEEP_DIV",
                    P_COMMON_CD: $ND.C_ALL,
                    P_VIEW_DIV: "1" // 1:등록팝업, 2:조회팝업
                }
            }
        ]
    }, function(ajaxData) {
        var multipleData = $NC.toObject(ajaxData);
        // 상품상태 콤보 세팅
        $NC.setInitComboData({
            selector: "#cboQItem_State",
            codeField: "COMMON_CD",
            nameField: "COMMON_NM",
            fullNameField: "COMMON_CD_F",
            data: $NC.toArray(multipleData.O_WC_POP_CMCODE_ITEM_STATE_2),
            addAll: true
        });
        $NC.setInitComboData({
            selector: "#cboItem_State",
            codeField: "COMMON_CD",
            nameField: "COMMON_NM",
            fullNameField: "COMMON_CD_F",
            data: $NC.toArray(multipleData.O_WC_POP_CMCODE_ITEM_STATE_1)
        });
        $NC.setInitComboData({
            selector: "#cboKeep_Div",
            codeField: "COMMON_CD",
            nameField: "COMMON_NM",
            fullNameField: "COMMON_CD_F",
            data: $NC.toArray(multipleData.O_WC_POP_CMCODE_KEEP_DIV),
            addAll: true
        });
    });

    setDepartCombo();

    // 로케이션 정책
    setPolicyValInfo();

    // 신규/삭제/저장 버튼 툴팁 세팅
    $NC.setTooltip("#btnDelete", $NC.getDisplayMsg("JS.LCC04011P0.001", "삭제"));
}

/**
 * 등록팝업 Open 시 호출 됨
 */
function _OnLoaded() {

    $NC.setEnable("#edtCenter_Cd_F", false);
    $NC.setEnable("#edtBu_Cd", false);
    $NC.setEnable("#edtInvest_No", false);

    $NC.setValue("#edtCenter_Cd_F", $NC.G_VAR.G_PARAMETER.P_CENTER_CD_F);
    $NC.setValue("#edtCenter_Cd", $NC.G_VAR.G_PARAMETER.P_CENTER_CD);
    $NC.setValue("#edtBu_Cd", $NC.G_VAR.G_PARAMETER.P_BU_CD);
    $NC.setValue("#edtBu_Nm", $NC.G_VAR.G_PARAMETER.P_BU_NM);
    $NC.setValue("#edtRandom_Rate", 10);

    // 신규 등록
    if ($NC.G_VAR.G_PARAMETER.P_PROCESS_CD == $ND.C_PROCESS_ENTRY_NEW) {

        $NC.setValue("#dtpInvest_Date", $NC.G_VAR.G_PARAMETER.P_INVEST_DATE);

        var INVEST_DATE = $NC.getValue("#dtpInvest_Date");
        $NC.setValue("#edtManager_Id", $NC.G_USERINFO.USER_ID);
        $NC.setValue("#edtManager_Nm", $NC.G_USERINFO.USER_NM);
        // 마스터 데이터 세팅
        $NC.G_VAR.masterData = {
            CENTER_CD: $NC.G_VAR.G_PARAMETER.P_CENTER_CD,
            BU_CD: $NC.G_VAR.G_PARAMETER.P_BU_CD,
            INVEST_DATE: INVEST_DATE,
            INVEST_NO: "",
            INVEST_DIV: "",
            INVEST_START_DATE: INVEST_DATE,
            INVEST_END_DATE: INVEST_DATE,
            DATA_CREATE_DIV: $NC.G_VAR.G_PARAMETER.P_POLICY_LS210 == "1" ? "1" : "2", // 1 - 상품별, 2 - 유통기한/제조배치별
            MANAGER_ID: $NC.G_USERINFO.USER_ID,
            MANAGER_NM: $NC.G_USERINFO.USER_NM,
            REMARK1: "",
            CRUD: $ND.C_DV_CRUD_C
        };

        $NC.setFocus("#edtRemark1");
    }
    // 예정 -> 등록, 등록 수정
    else {
        // 마스터 데이터 세팅
        var dsMaster = $NC.G_VAR.G_PARAMETER.P_MASTER_DS;
        $NC.setValue("#dtpInvest_Date", dsMaster.INVEST_DATE);
        $NC.setValue("#edtInvest_No", dsMaster.INVEST_NO);
        $NC.setValue("#dtpInvest_Start_Date", dsMaster.INVEST_START_DATE);
        $NC.setValue("#dtpInvest_End_Date", dsMaster.INVEST_END_DATE);
        $NC.setValue("#edtManager_Id", dsMaster.MANAGER_ID);
        $NC.setValue("#edtManager_Nm", dsMaster.MANAGER_NM);
        $NC.setValue("#edtRemark1", dsMaster.REMARK1);

        $NC.setVisible("#grpInoutInfo", false); // 입출고정보
        $NC.setVisible("#grpLocationInfo", false); // 로케이션정보
        $NC.setVisible("#grpItemGroupInfo", false); // 상품그룹정보
        $NC.setVisible("#grpItemInfo", false); // 상품&로케이션
        $NC.setVisible("#grpItemStateInfo", false); // 상품&로케이션
        $NC.setVisible("#grpItemAddInfo", false); // 상품입력정보
        $NC.setVisible("#grpRandomInfo", false); // 랜덤정보

        $NC.G_VAR.masterData = {
            CENTER_CD: dsMaster.CENTER_CD,
            BU_CD: dsMaster.BU_CD,
            INVEST_DATE: dsMaster.INVEST_DATE,
            INVEST_NO: dsMaster.INVEST_NO,
            INVEST_DIV: dsMaster.INVEST_DIV,
            INVEST_START_DATE: dsMaster.INVEST_START_DATE,
            INVEST_END_DATE: dsMaster.INVEST_END_DATE,
            DATA_CREATE_DIV: dsMaster.DATA_CREATE_DIV,
            MANAGER_ID: dsMaster.MANAGER_ID,
            MANAGER_NM: dsMaster.MANAGER_NM,
            REMARK1: dsMaster.REMARK1,
            CRUD: $ND.C_DV_CRUD_R
        };

        // 디테일 데이터 세팅
        var dsDetail = $NC.G_VAR.G_PARAMETER.P_DETAIL_DS;
        var rowData, newRowData;
        G_GRDDETAIL.data.beginUpdate();
        try {
            for (var rIndex = 0, rCount = dsDetail.length; rIndex < rCount; rIndex++) {
                rowData = dsDetail[rIndex];
                newRowData = {
                    CENTER_CD: rowData.CENTER_CD,
                    BU_CD: rowData.BU_CD,
                    INVEST_DATE: rowData.INVEST_DATE,
                    INVEST_NO: rowData.INVEST_NO,
                    LINE_NO: rowData.LINE_NO,
                    LOCATION_CD: rowData.LOCATION_CD,
                    BRAND_CD: rowData.BRAND_CD,
                    BRAND_NM: rowData.BRAND_NM,
                    ITEM_CD: rowData.ITEM_CD,
                    ITEM_BAR_CD: rowData.ITEM_BAR_CD,
                    ITEM_NM: rowData.ITEM_NM,
                    ITEM_SPEC: rowData.ITEM_SPEC,
                    ITEM_STATE: rowData.ITEM_STATE,
                    ITEM_STATE_F: rowData.ITEM_STATE_F,
                    ITEM_LOT: rowData.ITEM_LOT,
                    VALID_DATE: rowData.VALID_DATE,
                    BATCH_NO: rowData.BATCH_NO,
                    QTY_IN_BOX: rowData.QTY_IN_BOX,
                    IN_UNIT_DIV_F: rowData.IN_UNIT_DIV_F,
                    STOCK_QTY: rowData.STOCK_QTY,
                    STOCK_BOX: rowData.STOCK_BOX,
                    STOCK_EA: rowData.STOCK_EA,
                    INVEST_QTY: rowData.INVEST_QTY,
                    INVEST_BOX: rowData.INVEST_BOX,
                    INVEST_EA: rowData.INVEST_EA,
                    DRUG_DIV_D: rowData.DRUG_DIV_D,
                    MEDICATION_DIV_D: rowData.MEDICATION_DIV_D,
                    KEEP_DIV_D: rowData.KEEP_DIV_D,
                    DRUG_CD: rowData.DRUG_CD,
                    id: $NC.getGridNewRowId(),
                    CRUD: $ND.C_DV_CRUD_R
                };
                G_GRDDETAIL.data.addItem(newRowData);
            }
        } finally {
            G_GRDDETAIL.data.endUpdate();
        }

        // 수정일 경우 입력불가 항목 비활성화 처리
        $NC.setEnable("#dtpInvest_Date", false);
        $NC.setEnable("#cboInvest_Div", false);
        $NC.setEnable("#cboData_Create_Div", false);
        $NC.setEnable("#dtpInvest_Start_Date", false);
        $NC.setEnable("#dtpInvest_End_Date", false);
        $NC.setEnable("#edtManager_Id", false);
        $NC.setEnable("#edtManager_Nm", false);
        $NC.setEnable("#btnManager_Id", false);

        $NC.setEnable("#dtpValid_Date", $NC.G_VAR.masterData.DATA_CREATE_DIV == "2");
        $NC.setEnable("#edtBatch_No", $NC.G_VAR.masterData.DATA_CREATE_DIV == "2");

        setDisplayConditionView($NC.G_VAR.masterData.INVEST_DIV, $NC.getValueRadioGroup("rgbAdd_Div"));

        $NC.setGridSelectRow(G_GRDDETAIL, 0);
    }

    // 콤보박스 초기화
    $NC.serviceCall("/WC/getMultiDataSet.do", {
        P_SERVICE_PARAMS: [
            {
                P_RESULT_ID: "O_WC_POP_CMCODE_INVEST_DIV",
                P_QUERY_ID: "WC.POP_CMCODE",
                P_QUERY_PARAMS: {
                    P_COMMON_GRP: "INVEST_DIV",
                    P_COMMON_CD: $ND.C_ALL,
                    P_VIEW_DIV: "1" // 1:등록팝업, 2:조회팝업
                }
            },
            {
                P_RESULT_ID: "O_WC_POP_CMZONE",
                P_QUERY_ID: "WC.POP_CMZONE",
                P_QUERY_PARAMS: {
                    P_CENTER_CD: $NC.G_VAR.masterData.CENTER_CD,
                    P_ZONE_CD: $ND.C_ALL
                }
            }
        ]
    }, function(ajaxData) {
        var multipleData = $NC.toObject(ajaxData);
        // 실사구분 세팅
        $NC.setInitComboData({
            selector: "#cboInvest_Div",
            codeField: "COMMON_CD",
            nameField: "COMMON_NM",
            fullNameField: "COMMON_CD_F",
            data: $NC.toArray(multipleData.O_WC_POP_CMCODE_INVEST_DIV),
            onComplete: function() {
                if ($NC.G_VAR.G_PARAMETER.P_PROCESS_CD == $ND.C_PROCESS_ENTRY_NEW) {
                    $NC.G_VAR.masterData.INVEST_DIV = $NC.getValue("#cboInvest_Div");
                } else {
                    $NC.setValue("#cboInvest_Div", $NC.G_VAR.masterData.INVEST_DIV);
                }
            }
        });
        var dsZone = $NC.toArray(multipleData.O_WC_POP_CMZONE);
        // 존 세팅
        $NC.setInitComboData({
            selector: "#cboQ1_Zone_Cd",
            codeField: "ZONE_CD",
            fullNameField: "ZONE_CD_F",
            data: dsZone,
            addEmpty: true
        });
        $NC.setInitComboData({
            selector: "#cboQ2_Zone_Cd",
            codeField: "ZONE_CD",
            fullNameField: "ZONE_CD_F",
            data: dsZone,
            addEmpty: true
        });
    });

    // 데이터 생성 구분 콤보박스 세팅
    $NC.setInitComboData({
        selector: "#cboData_Create_Div",
        codeField: "COMMON_CD",
        nameField: "COMMON_NM",
        data: [
            {
                COMMON_CD: "1",
                COMMON_NM: $NC.getDisplayMsg("JS.LCC04011P0.040", "상품별")
            },
            {
                COMMON_CD: "2",
                COMMON_NM: $NC.getDisplayMsg("JS.LCC04011P0.041", "유통기한/제조배치별 ")
            }
        ],
        onComplete: function() {
            $NC.setValue("#cboData_Create_Div", $NC.G_VAR.masterData.DATA_CREATE_DIV);
        }
    });
}

