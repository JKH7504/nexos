/**
 * 그리드 초기값 설정
 */
function grdDetailInitialize() {

    var options = {
        editable: true,
        autoEdit: true,
        frozenColumn: 3
    };

    // Grid Object, DataView 생성 및 초기화
    $NC.setInitGridObject("#grdDetail", {
        columns: grdDetailOnGetColumns(),
        queryId: null,
        sortCol: "ITEM_CD",
        gridOptions: options,
        canExportExcel: false,
        onFilter: grdDetailOnFilter
    });

    G_GRDDETAIL.view.onSelectedRowsChanged.subscribe(grdDetailOnAfterScroll);
    G_GRDDETAIL.view.onCellChange.subscribe(grdDetailOnCellChange);
    G_GRDDETAIL.view.onHeaderClick.subscribe(grdDetailOnHeaderClick);
    G_GRDDETAIL.view.onClick.subscribe(grdDetailOnClick);

    $NC.setGridColumnHeaderCheckBox(G_GRDDETAIL, "CHECK_YN");

}

function grdDetailOnHeaderClick(e, args) {

    switch (args.column.id) {
        case "CHECK_YN":
            $NC.onGridColumnHeaderCheckBoxChange(G_GRDDETAIL, e, args);
            break;
    }
}

function grdDetailOnClick(e, args) {

    var columnId = G_GRDDETAIL.view.getColumnId(args.cell);
    if ($NC.isNull(columnId)) {
        return;
    }

    switch (columnId) {
        case "CHECK_YN":
            $NC.onGridCheckBoxEditorChange(G_GRDDETAIL, e, args);
            break;
    }
}

/**
 * 그리드 행 선택 변경 했을 경우
 * 
 * @param e
 * @param args
 */
function grdDetailOnAfterScroll(e, args) {

    if (!$NC.isGridValidLastRow(G_GRDDETAIL, args.rows, e)) {
        return;
    }
    var row = args.rows[0];

    // 상단 현재로우/총건수 업데이트
    $NC.setGridDisplayRows(G_GRDDETAIL, row + 1);
}

function grdDetailOnCellChange(e, args) {

    var rowData = args.item;
    var columnId = G_GRDDETAIL.view.getColumnId(args.cell);
    switch (G_GRDDETAIL.view.getColumnId(args.cell)) {
        case "ZONE_CD":
            if ($NC.isNotNull(rowData.INVEST_QTY)) {
                if (Number(rowData.INVEST_QTY) < 0) {
                    alert($NC.getDisplayMsg("JS.LCC04011P0.031", "실사수량은 0보다 작을 수 없습니다."));
                    rowData.INVEST_QTY = 0;
                    $NC.setFocusGrid(G_GRDDETAIL, args.row, args.cell, true);
                    return false;
                }
            }
            break;
        case "INVEST_QTY":
        case "INVEST_BOX":
        case "INVEST_EA":
            rowData = grdDetailOnCalc(rowData, columnId);
            break;
    }

    // 마지막 선택 Row 수정 데이터 반영 및 상태 변경
    $NC.setGridApplyChange(G_GRDDETAIL, rowData);
}

function grdDetailOnBeforePost(row) {

    // Validation 대상인지 체크, 아니면 True로 리턴
    if (!$NC.isGridValidPostRow(G_GRDDETAIL, row)) {
        return true;
    }

    var rowData = G_GRDDETAIL.data.getItem(row);

    // 신규 데이터 업데이트, N -> C
    $NC.setGridApplyPost(G_GRDDETAIL, rowData);
    return true;
}

/**
 * grdDetail 데이터 필터링 이벤트
 */
function grdDetailOnFilter(item) {

    return item.CRUD != $ND.C_DV_CRUD_D;
}

/**
 * 자료추가 클릭후 하단 그리드에 데이터 표시처리
 */
function onGetDetail(ajaxData) {

    var dsResult = $NC.toArray(ajaxData);
    var rCount = dsResult.length;
    if (rCount == 0) {
        alert($NC.getDisplayMsg("JS.LCC04011P0.032", "검색 조건에 해당하는 데이터가 없습니다."));
        return;
    }

    var searchKey = [
        "LOCATION_CD",
        "BRAND_CD",
        "ITEM_CD",
        "ITEM_STATE",
        "ITEM_LOT",
        "VALID_DATE",
        "BATCH_NO"
    ];

    var addedCount = 0;
    var newRowData;
    G_GRDDETAIL.data.beginUpdate();
    try {
        for (var rIndex = 0; rIndex < rCount; rIndex++) {
            newRowData = dsResult[rIndex];

            newRowData.CRUD = $ND.C_DV_CRUD_C;
            newRowData.LINE_NO = "";
            newRowData.INVEST_QTY = newRowData.STOCK_QTY;
            newRowData.INVEST_BOX = newRowData.STOCK_BOX;
            newRowData.INVEST_EA = newRowData.STOCK_EA;

            if ($NC.getGridSearchRow(G_GRDDETAIL, {
                searchKey: searchKey,
                searchVal: [
                    newRowData.LOCATION_CD,
                    newRowData.BRAND_CD,
                    newRowData.ITEM_CD,
                    newRowData.ITEM_STATE,
                    newRowData.ITEM_LOT,
                    newRowData.VALID_DATE,
                    newRowData.BATCH_NO
                ]
            }) > -1) {
                continue;
            }
            newRowData.id = $NC.getGridNewRowId();
            G_GRDDETAIL.data.addItem(newRowData);
            addedCount++;
        }
    } finally {
        G_GRDDETAIL.data.endUpdate();
    }

    // 선택한 행이 모두 이미 지시내역 그리드에 존재할 경우
    if (addedCount == 0) {
        alert($NC.getDisplayMsg("JS.LCC04011P0.033", "검색 조건에 해당하는 데이터는 이미 추가된 데이터입니다."));
        return;
    }
    $NC.setGridSelectRow(G_GRDDETAIL, G_GRDDETAIL.data.getLength() - 1);

    // 수정 상태로 변경
    G_GRDDETAIL.lastRowModified = true;

    $NC.setEnable("#cboInvest_Div", false);
    $NC.setEnable("#cboData_Create_Div", false);
}

/**
 * 저장후 처리
 * 
 * @param ajaxData
 */
function onSave(ajaxData) {

    var resultData = $NC.toObject(ajaxData);
    var oMsg = $NC.getOutMessage(resultData);
    if (oMsg != $ND.C_OK) {
        alert(oMsg);
        return;
    }

    onClose();
}

