/**
 * 삭제
 */
function _Delete() {

    if (G_GRDDETAIL.data.getLength() == 0 || $NC.isNull(G_GRDDETAIL.lastRow)) {
        alert($NC.getDisplayMsg("JS.LCC04011P0.026", "삭제할 데이터가 없습니다."));
        return;
    }

    // 선택 데이터 검색 -> Indexes
    var checkedRows = $NC.getGridSearchRows(G_GRDDETAIL, {
        searchKey: "CHECK_YN",
        searchVal: $ND.C_YES
    });
    if (checkedRows.length == 0) {
        alert($NC.getDisplayMsg("JS.LCC04011P0.027", "삭제할 데이터를 선택 하세요."));
        return;
    }

    var rowData;
    for (var rIndex = checkedRows.length - 1; rIndex >= 0; rIndex--) {
        // 선택 데이터
        rowData = G_GRDDETAIL.data.getItem(checkedRows[rIndex]);
        // 신규 데이터일 경우 Grid 데이터만 삭제, 그외 CURD를 "D"로 변경
        $NC.deleteGridRowData(G_GRDDETAIL, rowData, true);
    }

    if (G_GRDDETAIL.data.getLength() == 0) {
        $NC.setEnable("#cboInvest_Div", true);
        $NC.setEnable("#cboData_Create_Div", true);
    }

}

/**
 * 마스터 데이터 변경시 처리
 */
function masterDataOnChange(e, args) {

    switch (args.col) {
        case "MANAGER_ID":
            $NP.onUserChange(args.val, {
                P_USER_ID: args.val,
                P_CERTIFY_DIV: $ND.C_ALL
            }, onUserPopup);
            break;
        case "INVEST_DIV":
            $NC.G_VAR.masterData.INVEST_DIV = args.val;
            setDisplayConditionView(args.val, $NC.getValueRadioGroup("rgbAdd_Div"));
            break;
        case "INVEST_DATE":
            $NC.setValueDatePicker(args.view, args.val, $NC.getDisplayMsg("JS.LCC04011P0.028", "실사일자를 정확히 입력하십시오."));
            $NC.G_VAR.masterData.INVEST_DATE = $NC.getValue("#dtpInvest_Date");
            break;
        case "INVEST_START_DATE":
            $NC.setValueDatePicker(args.view, args.val, $NC.getDisplayMsg("JS.LCC04011P0.029", "실사기간 시작일자를 정확히 입력하십시오."));
            $NC.G_VAR.masterData.INVEST_START_DATE = $NC.getValue("#dtpInvest_Start_Date");
            break;
        case "INVEST_END_DATE":
            $NC.setValueDatePicker(args.view, args.val, $NC.getDisplayMsg("JS.LCC04011P0.030", "실사기간 종료일자를 정확히 입력하십시오."));
            $NC.G_VAR.masterData.INVEST_END_DATE = $NC.getValue("#dtpInvest_End_Date");
            break;
        case "REMARK1":
            $NC.G_VAR.masterData.REMARK1 = args.val;
            break;
        case "DATA_CREATE_DIV":
            $NC.G_VAR.masterData.DATA_CREATE_DIV = args.val;
            $NC.setEnable("#dtpValid_Date", $NC.G_VAR.masterData.DATA_CREATE_DIV == "2");
            $NC.setEnable("#edtBatch_No", $NC.G_VAR.masterData.DATA_CREATE_DIV == "2");
            break;
        case "ADD_DIV1":
        case "ADD_DIV2":
            setDisplayConditionView($NC.G_VAR.masterData.INVEST_DIV, args.val);
            break;

    }

    if ($NC.G_VAR.masterData.CRUD == $ND.C_DV_CRUD_R) {
        $NC.G_VAR.masterData.CRUD = $ND.C_DV_CRUD_U;
    }
}

