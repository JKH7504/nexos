 */
function _OnGridCheckBoxFormatterClick(e, view, args) {

    var grdObject = $NC.getGridObject(args.grid);
    if (!grdObject.isValid) {
        return;
    }

    var columnId = grdObject.view.getColumnId(args.cell);
    if ($NC.isNull(columnId)) {
        return;
    }

    switch (columnId) {
        case "CHECK_YN":
            $NC.onGridCheckBoxFormatterChange(grdObject, e, args, true);
            break;
    }
}

/**
 * 종결처리 Button Event
 */
function btnOrderClosingOnClick() {

    if (!$NC.getProgramPermission().canSave) {
        alert($NC.getDisplayMsg("JS.MAIN.001", "해당 프로그램의 저장권한이 없습니다."));
        return;
    }

    if (G_GRDMASTER.data.getLength() == 0 || $NC.isNull(G_GRDMASTER.lastRow)) {
        alert($NC.getDisplayMsg("JS.LIC01010E0.016", "종결처리할 데이터가 없습니다."));
        return;
    }

    if (!confirm($NC.getDisplayMsg("JS.LIC01010E0.017", "종결처리 하시겠습니까?"))) {
        return;
    }

    var rowData = G_GRDMASTER.data.getItem(G_GRDMASTER.lastRow);
    // 종결처리 호출
    $NC.serviceCall("/LIC01010E0/callLIOrderClosing.do", {
        P_CENTER_CD: rowData.CENTER_CD,
        P_BU_CD: rowData.BU_CD,
        P_ORDER_DATE: rowData.ORDER_DATE,
        P_ORDER_NO: rowData.ORDER_NO,
        P_USER_ID: $NC.G_USERINFO.USER_ID
    }, onSave);
}

/**
 * QC상태전환 Button Event - QC상태전환 버튼 클릭
 */
function btnChangeQCStateOnClick() {

    if (!$NC.getProgramPermission().canSave) {
        alert($NC.getDisplayMsg("JS.MAIN.001", "해당 프로그램의 저장권한이 없습니다."));
        return;
    }

    if (G_GRDMASTER.data.getLength() == 0 || $NC.isNull(G_GRDMASTER.lastRow)) {
        alert($NC.getDisplayMsg("JS.LIC01010E0.018", "QC상태전환할 데이터가 없습니다."));
        return;
    }

    if (G_GRDDETAIL.data.getLength() == 0 || $NC.isNull(G_GRDDETAIL.lastRow)) {
        alert($NC.getDisplayMsg("JS.LIC01010E0.018", "QC상태전환할 데이터가 없습니다."));
        return;
    }

    var refRowData = G_GRDMASTER.data.getItem(G_GRDMASTER.lastRow);
    if ($NC.G_VAR.stateFWBW.CANCEL != refRowData.INBOUND_STATE) {
        alert($NC.getDisplayMsg("JS.LIC01010E0.019", "삭제가 불가능한 전표입니다."));
        return;
    }

    if (!confirm($NC.getDisplayMsg("JS.LIC01010E0.020", "QC상태전환 처리 하시겠습니까?"))) {
        return;
    }

    var dsDetail = [];
    var rowData, ITEM_STATE;
    for (var rIndex = 0, rowCount = G_GRDDETAIL.data.getLength(); rIndex < rowCount; rIndex++) {
        rowData = G_GRDDETAIL.data.getItem(rIndex);
        if (rowData.CHECK_YN != $ND.C_YES || rowData.ITEM_STATE == "B") {
            continue;
        }
        ITEM_STATE = $NC.iif(rowData.ITEM_STATE == "Q", "A", "Q");
        dsDetail.push({
            P_CENTER_CD: rowData.CENTER_CD,
            P_BU_CD: rowData.BU_CD,
            P_ORDER_DATE: rowData.ORDER_DATE,
            P_ORDER_NO: rowData.ORDER_NO,
            P_LINE_NO: rowData.LINE_NO,
            P_INBOUND_STATE: rowData.INBOUND_STATE,
            P_ITEM_STATE: ITEM_STATE,
            P_CRUD: rowData.CRUD
        });
    }

    if (dsDetail.length == 0) {
        alert($NC.getDisplayMsg("JS.LIC01010E0.021", "QC상태전환 처리할 데이터를 선택하십시오."));
        return;
    }

    $NC.serviceCall("/LIC01010E0/changeItemStateToQCState.do", {
        P_DS_MASTER: {
            P_CENTER_CD: refRowData.CENTER_CD,
            P_BU_CD: refRowData.BU_CD,
            P_ORDER_DATE: refRowData.ORDER_DATE,
            P_ORDER_NO: refRowData.ORDER_NO
        },
        P_DS_DETAIL: dsDetail,
        P_USER_ID: $NC.G_USERINFO.USER_ID
    }, onSave, onSaveError);
}

/**
 * 프로그램 사용 권한 설정
 */
function setUserProgramPermission() {

    var permission = $NC.getProgramPermission();
    var enable = G_GRDMASTER.data.getLength() > 0;

    $NC.setEnableButton("#divMasterView", permission.canSave && enable);
}
