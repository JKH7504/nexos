<!DOCTYPE HTML>
<html lang="ko">
<head>
<!-- ${NEXOS_META} -->
<!-- ${NEXOS_LIBRARY} ${} ${../../../} -->
<!-- PROGRAM CSS/JS -->
<!-- <link rel="stylesheet" href="css/LOB05070Q0.css" /> -->
<script src="js/LOC04010Q0.js"></script>
</head>
<body>
    <!-- 조회 조건 -->
    <div id="ctrConditionView" class="ctrCondition">
        <table class="ctrInner">
            <tr>
                <td>
                    <div id="lblQCenter_Cd" class="lblCondition">물류센터</div>
                    <select id="cboQCenter_Cd" class="cboKey" style="width: 150px;"></select>
                </td>
                <td>
                    <div id="lblQPopup_Bu_Cd" class="lblCondition">사업부</div>
                    <input id="edtQBu_Cd" class="edtNormal" type="text" style="width: 60px;" />
                    <input id="btnQBu_Cd" class="btnIcoPopup" type="button" />
                    <input id="edtQBu_Nm" class="edtDisplay" type="text" disabled="disabled" style="width: 120px;" />
                    <input id="edtQCust_Cd" class="edtHidden" type="text" disabled="disabled" />
                </td>
                <td>
                    <div id="lblQOubbound_Date" class="lblCondition">출고일자</div>
                    <input id="dtpQOutbound_Date" class="dtpKey" type="text" style="width: 100px;" />
                </td>
            </tr>
            <tr>
                <td>
                    <div>
                        <div id="lblQOutbound_Batch" class="lblCondition">출고차수</div>
                        <select id="cboQOutbound_Batch" class="cboNormal" style="width: 151px;"></select>
                        <input id="btnQOutbound_Batch" class="btnIcoRefresh" type="button" />
                    </div>
                </td>
                <td>
                    <div id="lblQView_Div" class="lblCondition">검색구분</div>
                    <select id="cboQView_Div" class="cboNormal" style="width: 210px;"></select>
                </td>
                <td>
                    <div id="divViewDiv2">
                        <div id="lblQEquip_Div" class="lblCondition">장비구분</div>
                        <div id="divEquip_Div" class="ctrGroupBox" style="width: 210px;">
                            <input id="rgbQEquip_Div0" name="rgbQEquip_Div" class="rgbNormal" type="radio" value="0" />
                            전체
                            <input id="rgbQEquip_Div1" name="rgbQEquip_Div" class="rgbNormal" type="radio" value="1" />
                            DPC
                            <input id="rgbQEquip_Div2" name="rgbQEquip_Div" class="rgbNormal" type="radio" value="2" />
                            DAS
                            <input id="rgbQEquip_Div3" name="rgbQEquip_Div" class="rgbNormal" type="radio" value="3" />
                            ASS
                        </div>
                    </div>
                </td>
            </tr>
        </table>
    </div>
    <div id="divMasterView">
        <!-- WCS작업결과내역 -->
        <table class="ctrTitleBar">
            <tr>
                <td id="ctrTitle_grdMaster" class="lblTitle">WCS작업결과내역</td>
                <td id="ctrRows_grdMaster" class="lblRows">0/0 Rows</td>
            </tr>
        </table>
        <div id="grdMaster"></div>
    </div>
</body>
</html>
