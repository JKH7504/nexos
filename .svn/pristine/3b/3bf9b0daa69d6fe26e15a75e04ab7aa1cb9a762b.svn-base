    // 마지막 선택 Row 수정 데이터 반영 및 상태 변경
    $NC.setGridApplyChange(G_GRDDETAIL, rowData);
    grdDetailOnCheckPStock(G_GRDDETAIL.lastRow, null);

    $NC.setFocusGrid(G_GRDDETAIL, G_GRDDETAIL.lastRow, focusCol, true, true);
}

/**
 * [제고에서선택] 팝업의 검색항목 값 변경시 화면 클리어
 */
function onChangingCondition() {

    // 초기화
    $NC.clearGridData(G_GRDSTOCK);
}

/**
 * 프로그램 사용 권한 설정
 */
function setUserProgramPermission() {
    // 프로그램 사용 권한
    var permission = $NC.G_VAR.G_PARAMETER.P_PERMISSION;

    if ($NC.G_VAR.G_PARAMETER.P_PROCESS_CD == $ND.C_PROCESS_ORDER || $NC.G_VAR.ORDER_YN == $ND.C_YES) {
        // 예정으로 등록, 예정으로 등록한 데이터 수정일 경우
        // 예정으로 등록 시 추가/삭제 허용 기준
        // NN - 추가/삭제 불가능
        // NY - 삭제만 가능
        // YN - 추가만 가능
        // YY - 추가/삭제 가능
        var POLICY_LO231 = $NC.G_VAR.G_PARAMETER.P_POLICY_LO231.match(/N|Y/gi);
        if ($NC.isNull(POLICY_LO231)) {
            POLICY_LO231 = [
                $ND.C_NO,
                $ND.C_NO
            ];
        }
        // 예정으로 등록시 추가/삭제 허용 기준 정책 및 프로그램 사용 권한 체크
        $NC.setEnable("#btnEntryNew", POLICY_LO231[0] == $ND.C_YES && permission.canSave);
        $NC.setEnable("#btnStockPopup", POLICY_LO231[0] == $ND.C_YES && permission.canSave);
        $NC.setEnable("#btnEntryDelete", POLICY_LO231[1] == $ND.C_YES && permission.canDelete);
        $NC.setEnable("#btnEntrySave", permission.canSave);
    } else {
        // 신규 등록, 신규로 등록한 데이터 수정일 경우
        $NC.setEnable("#btnEntryNew", permission.canSave);
        $NC.setEnable("#btnEntryDelete", permission.canDelete);
        $NC.setEnable("#btnEntrySave", permission.canSave);
    }
}

function setPolicyValInfo() {
    $NC.setPolicyValInfo({
        P_CENTER_CD: $NC.G_VAR.G_PARAMETER.P_CENTER_CD,
        P_BU_CD: $NC.getValue("#edtBu_Cd")
    }, function() {
        $NC.G_VAR.G_PARAMETER.P_POLICY_LO190 = $NC.G_VAR.policyVal.LO190;
        $NC.G_VAR.G_PARAMETER.P_POLICY_LO231 = $NC.G_VAR.policyVal.LO231;
        $NC.G_VAR.G_PARAMETER.P_POLICY_LO250 = $NC.G_VAR.policyVal.LO250;
        $NC.G_VAR.G_PARAMETER.P_POLICY_LS210 = $NC.G_VAR.policyVal.LS210;
        // 검색구분에 초기값 설정
        $NC.setEnable("#rgbQView_Div2", $NC.G_VAR.policyVal.LS210 == "2" && $NC.G_VAR.policyVal.LO250 == "2");
        // 유통기한/제조배치번호 지정 정책에 따라 조건 표시 설정
        grdDetailOnSetColumns();

        setUserProgramPermission();
    });
}