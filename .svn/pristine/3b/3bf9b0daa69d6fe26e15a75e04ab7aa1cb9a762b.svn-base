        P_ITEM_CD: ITEM_CD,
        P_ITEM_STATE: ITEM_STATE,
        P_ITEM_LOT: ITEM_LOT
    };

    // 데이터 조회
    $NC.serviceCallAndWait("/LOF02010E0/getDataSet.do", $NC.getGridParams(G_GRDOUTWAIT), onGetOutwait);

    if (G_GRDOUTWAIT.data.getLength() == 0) {
        alert($NC.getDisplayMsg("JS.LOF02011P0.033", "출고대기 내역이 존재하지 않습니다."));
        return;
    }

    $NC.showOverlay("#divOutwaitOverlay", {
        title: $NC.getDisplayMsg("JS.LOF02011P0.034", "출고대기 내역"),
        fullscreen: false,
        width: 700,
        height: 300,
        onComplete: function() {
            $NC.onGlobalResize();
            G_GRDOUTWAIT.view.focus();
            G_GRDOUTWAIT.view.invalidate();
            $NC.setGridSelectRow(G_GRDOUTWAIT, 0);
        }
    });
