/**
 * 화면 초기화 - 화면 로드시 자동 호출 됨
 */
function _Initialize() {

    // 단위화면에서 사용될 일반 전역 변수 정의
    $NC.setGlobalVar({
        autoResizeView: {
            container: "#divDetailView",
            grids: [
                "#grdDetail"
            ],
            exceptHeight: $NC.getViewHeight("#ctrPopupView")
        },
        // 체크할 정책 값
        policyVal: {
            LO190: "", // 공급금액 계산 정책----------------------------------사업부정책
            LO231: "NN", // [B2C/B2CF]예정으로 등록 시 추가/삭제 허용 기준----사업부정책
            LO250: "", // 유통기한/제조배치번호 지정 정책---------------------사업부정책
            LS210: "" // 재고 관리 기준---------------------------------------사업부정책
        },
        // 예정이 존재하는 데이터인지...
        ORDER_YN: $ND.C_NO,
        // 마스터 데이터
        masterData: null
    });

    // ================================================================================================================
    // $NC.G_VAR.G_PARAMETER.P_PROCESS_CD
    // N: 신규 등록
    // BP: 등록 수정
    // A : 예정으로 등록
    // ================================================================================================================

    // 현재고 검색 조건.상품상태 콤보 세팅
    $NC.setInitCombo("/WC/getDataSet.do", {
        P_QUERY_ID: "WC.POP_CMCODE",
        P_QUERY_PARAMS: {
            P_COMMON_GRP: "ITEM_STATE",
            P_COMMON_CD: $ND.C_ALL,
            P_ATTR01_CD: "1",
            P_VIEW_DIV: "1" // 1:등록팝업, 2:조회팝업
        }
    }, {
        selector: "#cboQItem_State",
        codeField: "COMMON_CD",
        nameField: "COMMON_NM",
        fullNameField: "COMMON_CD_F",
        addAll: true
    });

    // 버튼 클릭 이벤트 연결
    $("#btnClose").click(onCancel); // 닫기버튼
    $("#btnQBu_Cd").click(showUserBuPopup); // 사업부 검색 버튼
    $("#btnDelivery_Cd").click(showDeliveryPopup); // 배송처 검색 버튼
    $("#btnShipper_Zip_Cd").click(showPostPopup);
    $("#btnShipper_Zip_Cd_Clear").click(btnShipperZipCdClearOnClick);

    $("#btnEntryNew").click(_New); // 그리드 행 추가 버튼
    $("#btnEntryDelete").click(_Delete); // 그리드 행 삭제버튼
    $("#btnEntrySave").click(_Save); // 저장 버튼

    $("#btnStockPopup").click(showStockOverlay);// 재고에서 선택버튼 클릭
    $("#btnStockAddItem").click(btnStockAddItemOnClick); // 재고팝업에서 확인버튼클릭
    $("#btnStockInquiry").click(_Inquiry); // 재고팝업에서 조회버튼클릭
    $("#btnStockOverlayClose").click(function() { // 재고에서 선택 창 닫기
        $("#divStockOverlay").hide();
    });
    $("#btnOutwaitOverlayClose").click(function() { // 출고대기에서 창 닫기
        $("#divOutwaitOverlay").hide();
    });

    $("#btnQBrand_Cd").click(showBuBrandStockPopup); // 브랜드검색 버튼 클릭
    $("#btnQItem_Cd").click(showItemStockPopup); // 상품검색 버튼 클릭
    $NC.setEnable("#edtCenter_Cd_F", false);
    $NC.setEnable("#edtOrder_No", false);
    $NC.setEnable("#edtBu_Date", false);
    $NC.setEnable("#edtBu_No", false);

    $NC.setInitDatePicker("#dtpOutbound_Date");

    // 운송차수 다시보기 이미지 클릭시 처리
    $("#btnDelivery_Batch").click(function() {
        if ($NC.G_VAR.G_PARAMETER.P_PROCESS_CD != $ND.C_PROCESS_ENTRY_POPUP) {
            setDeliveryBatchCombo();
        }
    });

    $("#btnOutwaitPopup").click(showOutwaitOverlay); // 출고대기량 조회 버튼 클릭

    // 그리드 초기화
    grdDetailInitialize();
    grdOutwaitInitialize();
    grdStockInitialize();

    // 신규/삭제/저장 버튼 툴팁 세팅
    $NC.setTooltip("#btnEntryNew", $NC.getDisplayMsg("JS.LOF02011P0.XXX", "신규"));
    $NC.setTooltip("#btnEntryDelete", $NC.getDisplayMsg("JS.LOF02011P0.XXX", "삭제"));
    $NC.setTooltip("#btnEntrySave", $NC.getDisplayMsg("JS.LOF02011P0.XXX", "저장"));

}

/**
 * 등록팝업 Open 시 호출 됨
 */
function _OnLoaded() {

    $NC.setValue("#edtCenter_Cd_F", $NC.G_VAR.G_PARAMETER.P_CENTER_CD_F);
    $NC.setValue("#edtCenter_Cd", $NC.G_VAR.G_PARAMETER.P_CENTER_CD);
    $NC.setValue("#edtBu_Cd", $NC.G_VAR.G_PARAMETER.P_BU_CD);
    $NC.setValue("#edtBu_Nm", $NC.G_VAR.G_PARAMETER.P_BU_NM);
    $NC.setValue("#edtCust_Cd", $NC.G_VAR.G_PARAMETER.P_CUST_CD);

    setPolicyValInfo();

    $NC.setVisible("#divDelivery_Batch1", $NC.G_VAR.G_PARAMETER.P_POLICY_LO729 == $ND.C_YES);
    $NC.setVisible("#divDelivery_Batch2", $NC.G_VAR.G_PARAMETER.P_POLICY_LO729 == $ND.C_YES);

    var OUTBOUND_DATE;
    // 신규 등록
    if ($NC.G_VAR.G_PARAMETER.P_PROCESS_CD == $ND.C_PROCESS_ENTRY_NEW) {

        OUTBOUND_DATE = $NC.getValue("#dtpOutbound_Date");
        // var INOUT_CD = $NC.getValue("#cboInout_Cd");
        // 마스터 데이터 세팅
        $NC.G_VAR.masterData = {
            CENTER_CD: $NC.G_VAR.G_PARAMETER.P_CENTER_CD,
            BU_CD: $NC.G_VAR.G_PARAMETER.P_BU_CD,
            OUTBOUND_DATE: OUTBOUND_DATE,
            OUTBOUND_NO: "",
            INOUT_CD: "", // INOUT_CD,
            OUTBOUND_STATE: $ND.C_STATE_ENTRY,
            CUST_CD: $NC.G_VAR.G_PARAMETER.P_CUST_CD,
            ORDER_DIV: "",
            ERP_BATCH: "",
            DELIVERY_CD: "",
            DELIVERY_NM: "",
            RDELIVERY_CD: "",
            RDELIVERY_NM: "",
            DELIVERY_BATCH: $ND.C_BASE_BATCH_NO,
            ORDER_DATE: "",
            ORDER_NO: "",
            INOUT_DATE: "",
            DIRECT_YN: "",
            BU_DATE: "",
            BU_NO: "",
            SO_DATE: "",
            SO_NO: "",
            ORG_BU_NO: "",
            PLANED_DATETIME: "",
            MALL_MSG: "",
            ORDERER_CD: "",
            ORDERER_NM: "",
            ORDERER_TEL: "",
            ORDERER_HP: "",
            ORDERER_EMAIL: "",
            ORDERER_MSG: "",
            SHIPPER_NM: "",
            SHIPPER_TEL: "",
            SHIPPER_HP: "",
            SHIPPER_ZIP_CD: "",
            SHIPPER_ADDR_BASIC: "",
            SHIPPER_ADDR_DETAIL: "",
            ACS_ERROR_CD: "N",
            ACS_ERROR_MSG: "",
            ACS_USER_ID: "",
            ACS_DATETIME: "",
            ACS_EXEC_TIME: "",
            HDC_CUST_CD: "",
            CARRIER_CD: "",
            HDC_DIV: "",
            WB_NO: "",
            DATA_DIV: "00",
            REMARK1: "",
            CRUD: $ND.C_DV_CRUD_C
        };

        $NC.setFocus("#edtBu_Cd");

    }
    // 예정 -> 등록, 등록 수정
    else {
        // 마스터 데이터 세팅
        var OUTBOUND_NO;
        var OUTBOUND_STATE;
        var CRUD;
        var dsMaster = $NC.G_VAR.G_PARAMETER.P_MASTER_DS;
        if ($NC.G_VAR.G_PARAMETER.P_PROCESS_CD == $ND.C_PROCESS_ORDER) {
            OUTBOUND_DATE = $NC.getValue("#dtpOutbound_Date");
            OUTBOUND_NO = "";
            OUTBOUND_STATE = $ND.C_STATE_ENTRY;
            CRUD = $ND.C_DV_CRUD_C;
        } else {
            OUTBOUND_DATE = dsMaster.OUTBOUND_DATE;
            OUTBOUND_NO = dsMaster.OUTBOUND_NO;
            OUTBOUND_STATE = null;// dsMaster.OUTBOUND_STATE;
            CRUD = $ND.C_DV_CRUD_R;
            $NC.setValue("#dtpOutbound_Date", OUTBOUND_DATE);
            $NC.setValue("#edtOutbound_No", OUTBOUND_NO);
        }
        $NC.setValue("#edtOrder_Date", dsMaster.ORDER_DATE);
        $NC.setValue("#edtOrder_No", dsMaster.ORDER_NO);
        $NC.setValue("#edtDelivery_Cd", dsMaster.DELIVERY_CD);
        $NC.setValue("#edtDelivery_Nm", dsMaster.DELIVERY_NM);
        $NC.setValue("#cboDelivery_Batch", dsMaster.DELIVERY_BATCH);
        // $NC.setValue("#cboInout_Cd", dsMaster.INOUT_CD);
        // $NC.setValue("#cboOrder_Div", dsMaster.ORDER_DIV);
        $NC.setValue("#edtBu_Date", dsMaster.BU_DATE);
        $NC.setValue("#edtBu_No", dsMaster.BU_NO);
        $NC.setValue("#edtRemark1", dsMaster.REMARK1);

        // 온라인 데이터 세팅
        $NC.setValue("#edtMall_Msg", dsMaster.MALL_MSG);
        $NC.setValue("#edtOrderer_Cd", dsMaster.ORDERER_CD);
        $NC.setValue("#edtOrderer_Nm", dsMaster.ORDERER_NM);
        $NC.setValue("#edtOrderer_Tel", dsMaster.ORDERER_TEL);
        $NC.setValue("#edtOrderer_Hp", dsMaster.ORDERER_HP);
        $NC.setValue("#edtOrderer_Email", dsMaster.ORDERER_EMAIL);
        $NC.setValue("#edtOrderer_Msg", dsMaster.ORDERER_MSG);
        $NC.setValue("#edtShipper_Nm", dsMaster.SHIPPER_NM);
        $NC.setValue("#edtShipper_Tel", dsMaster.SHIPPER_TEL);
        $NC.setValue("#edtShipper_Hp", dsMaster.SHIPPER_HP);
        $NC.setValue("#edtShipper_Zip_Cd", dsMaster.SHIPPER_ZIP_CD);
        $NC.setValue("#edtShipper_Addr_Basic", dsMaster.SHIPPER_ADDR_BASIC);
        $NC.setValue("#edtShipper_Addr_Detail", dsMaster.SHIPPER_ADDR_DETAIL);

        $NC.G_VAR.masterData = {
            CENTER_CD: dsMaster.CENTER_CD,
            BU_CD: dsMaster.BU_CD,
            OUTBOUND_DATE: OUTBOUND_DATE,
            OUTBOUND_NO: OUTBOUND_NO,
            INOUT_CD: dsMaster.INOUT_CD,
            OUTBOUND_STATE: OUTBOUND_STATE,
            CUST_CD: dsMaster.CUST_CD,
            ORDER_DIV: dsMaster.ORDER_DIV,
            ERP_BATCH: dsMaster.ERP_BATCH,
            DELIVERY_CD: dsMaster.DELIVERY_CD,
            DELIVERY_NM: dsMaster.DELIVERY_NM,
            RDELIVERY_CD: dsMaster.RDELIVERY_CD,
            RDELIVERY_NM: dsMaster.RDELIVERY_NM,
            DELIVERY_BATCH: dsMaster.DELIVERY_BATCH,
            ORDER_DATE: dsMaster.ORDER_DATE,
            ORDER_NO: dsMaster.ORDER_NO,
            INOUT_DATE: dsMaster.INOUT_DATE,
            DIRECT_YN: dsMaster.DIRECT_YN,
            BU_DATE: dsMaster.BU_DATE,
            BU_NO: dsMaster.BU_NO,
            BU_DATETIME: dsMaster.BU_DATETIME,
            SO_DATE: dsMaster.SO_DATE,
            SO_NO: dsMaster.SO_NO,
            ORG_BU_NO: dsMaster.ORG_BU_NO,
            PLANED_DATETIME: dsMaster.PLANED_DATETIME,
            MALL_MSG: dsMaster.MALL_MSG,
            ORDERER_CD: dsMaster.ORDERER_CD,
            ORDERER_NM: dsMaster.ORDERER_NM,
            ORDERER_TEL: dsMaster.ORDERER_TEL,
            ORDERER_HP: dsMaster.ORDERER_HP,
            ORDERER_EMAIL: dsMaster.ORDERER_EMAIL,
            ORDERER_MSG: dsMaster.ORDERER_MSG,
            SHIPPER_NM: dsMaster.SHIPPER_NM,
            SHIPPER_TEL: dsMaster.SHIPPER_TEL,
            SHIPPER_HP: dsMaster.SHIPPER_HP,
            SHIPPER_ZIP_CD: dsMaster.SHIPPER_ZIP_CD,
            SHIPPER_ADDR_BASIC: dsMaster.SHIPPER_ADDR_BASIC,
            SHIPPER_ADDR_DETAIL: dsMaster.SHIPPER_ADDR_DETAIL,
            ACS_ERROR_CD: dsMaster.ACS_ERROR_CD,
            ACS_ERROR_MSG: dsMaster.ACS_ERROR_MSG,
            ACS_USER_ID: dsMaster.ACS_USER_ID,
            ACS_DATETIME: dsMaster.ACS_DATETIME,
            ACS_EXEC_TIME: dsMaster.ACS_EXEC_TIME,
            HDC_CUST_CD: dsMaster.HDC_CUST_CD,
            CARRIER_CD: dsMaster.CARRIER_CD,
            HDC_DIV: dsMaster.HDC_DIV,
            WB_NO: dsMaster.WB_NO,
            DATA_DIV: dsMaster.DATA_DIV,
            REMARK1: dsMaster.REMARK1,
            CRUD: CRUD
        };

        // 디테일 데이터 세팅
        var dsDetail = $NC.G_VAR.G_PARAMETER.P_DETAIL_DS;
        var rowData, rIndex, rCount, newRowData, ORDER_QTY;
        G_GRDDETAIL.data.beginUpdate();
        try {
            for (rIndex = 0, rCount = dsDetail.length; rIndex < rCount; rIndex++) {
                rowData = $.extend({}, dsDetail[rIndex]);

                ORDER_QTY = Number(rowData.ORDER_QTY);
                // 예정과 등록에 맞게 rowData 수정
                if ($NC.G_VAR.G_PARAMETER.P_PROCESS_CD == $ND.C_PROCESS_ORDER) { // 예정
                    // 예정수량이 1보다 작은 데이터는 제외(분할입고로 인해 예정수량이 0인 데이터)
                    if (ORDER_QTY < 1) {
                        continue;
                    }
                    rowData.OUTBOUND_DATE = OUTBOUND_DATE;
                    rowData.OUTBOUND_NO = OUTBOUND_NO;
                    rowData.OUTBOUND_STATE = $ND.C_STATE_ENTRY;
                    rowData.ORG_LINE_NO = rowData.LINE_NO;
                    rowData.LINE_NO = "";
                    rowData.ENTRY_QTY = ORDER_QTY;
                    rowData.OLD_ENTRY_QTY = 0;
                    rowData.ENTRY_BOX = $NC.getBBox(ORDER_QTY, rowData.QTY_IN_BOX);
                    rowData.ENTRY_EA = $NC.getBEa(ORDER_QTY, rowData.QTY_IN_BOX);
                    rowData.ENTRY_WEIGHT = $NC.getWeight(ORDER_QTY, rowData.QTY_IN_BOX, rowData.BOX_WEIGHT);
                    rowData.CONFIRM_QTY = ORDER_QTY;
                    rowData.DELIVERY_QTY = ORDER_QTY;
                }
                // 등록
                else {
                    rowData.OUTBOUND_STATE = null;
                }

                newRowData = {
                    CENTER_CD: rowData.CENTER_CD,
                    BU_CD: rowData.BU_CD,
                    OUTBOUND_DATE: rowData.OUTBOUND_DATE,
                    OUTBOUND_NO: rowData.OUTBOUND_NO,
                    LINE_NO: rowData.LINE_NO,
                    OUTBOUND_STATE: rowData.OUTBOUND_STATE,
                    BRAND_CD: rowData.BRAND_CD,
                    BRAND_NM: rowData.BRAND_NM,
                    ITEM_CD: rowData.ITEM_CD,
                    ITEM_BAR_CD: rowData.ITEM_BAR_CD,
                    ITEM_NM: rowData.ITEM_NM,
                    ITEM_SPEC: rowData.ITEM_SPEC,
                    ITEM_STATE: rowData.ITEM_STATE,
                    ITEM_STATE_F: rowData.ITEM_STATE_F,
                    ITEM_LOT: rowData.ITEM_LOT,
                    QTY_IN_BOX: rowData.QTY_IN_BOX,
                    ORDER_QTY: rowData.ORDER_QTY,
                    ENTRY_QTY: rowData.ENTRY_QTY,
                    OLD_ENTRY_QTY: rowData.ENTRY_QTY,
                    ENTRY_BOX: rowData.ENTRY_BOX,
                    ENTRY_EA: rowData.ENTRY_EA,
                    CONFIRM_QTY: rowData.CONFIRM_QTY,
                    DELIVERY_QTY: rowData.DELIVERY_QTY,
                    BOX_WEIGHT: rowData.BOX_WEIGHT,
                    ENTRY_WEIGHT: rowData.ENTRY_WEIGHT,
                    VALID_DATE: rowData.VALID_DATE,
                    BATCH_NO: rowData.BATCH_NO,
                    SUPPLY_PRICE: rowData.SUPPLY_PRICE,
                    APPLY_PRICE: rowData.APPLY_PRICE,
                    DC_PRICE: rowData.DC_PRICE,
                    SUPPLY_AMT: rowData.SUPPLY_AMT,
                    VAT_AMT: rowData.VAT_AMT,
                    DC_AMT: rowData.DC_AMT,
                    TOTAL_AMT: rowData.TOTAL_AMT,
                    VAT_YN: rowData.VAT_YN,
                    ITEM_ORDER_DIV: rowData.ITEM_ORDER_DIV,
                    ITEM_ORDER_DIV_F: rowData.ITEM_ORDER_DIV_F,
                    MALL_ITEM_CD: rowData.MALL_ITEM_CD,
                    MALL_OPT_CD: rowData.MALL_OPT_CD,
                    MALL_ITEM_QTY: rowData.MALL_ITEM_QTY,
                    ORDER_DATE: rowData.ORDER_DATE,
                    ORDER_NO: rowData.ORDER_NO,
                    ORDER_LINE_NO: rowData.ORDER_LINE_NO,
                    BU_DATE: rowData.BU_DATE,
                    BU_NO: rowData.BU_NO,
                    BU_LINE_NO: rowData.BU_LINE_NO,
                    BU_KEY: rowData.BU_KEY,
                    BU_DATETIME: rowData.BU_DATETIME,
                    SO_DATE: rowData.SO_DATE,
                    SO_NO: rowData.SO_NO,
                    SO_LINE_NO: rowData.SO_LINE_NO,
                    SO_KEY: rowData.SO_KEY,
                    ORG_BU_NO: rowData.ORG_BU_NO,
                    ORG_LINE_NO: rowData.ORG_LINE_NO,
                    SUM_ENTRY_QTY: 0,
                    SHORTAGE_DIV: rowData.SHORTAGE_DIV,
                    SHORTAGE_DIV_F: rowData.SHORTAGE_DIV_F,
                    SHORTAGE_COMMENT: rowData.SHORTAGE_COMMENT,
                    SPARE1_NOTE: rowData.SPARE1_NOTE,
                    SPARE2_NOTE: rowData.SPARE2_NOTE,
                    SPARE3_NOTE: rowData.SPARE3_NOTE,
                    SPARE4_NOTE: rowData.SPARE4_NOTE,
                    SPARE5_NOTE: rowData.SPARE5_NOTE,
                    SPARE6_NOTE: rowData.SPARE6_NOTE,
                    SPARE7_NOTE: rowData.SPARE7_NOTE,
                    SPARE8_NOTE: rowData.SPARE8_NOTE,
                    SPARE9_NOTE: rowData.SPARE9_NOTE,
                    REMARK1: rowData.REMARK1,
                    id: $NC.getGridNewRowId(),
                    CRUD: CRUD
                };
                // 예정에만 SHORTAGE_YN 추가
                if ($NC.G_VAR.G_PARAMETER.P_PROCESS_CD == $ND.C_PROCESS_ORDER) {
                    newRowData.SHORTAGE_YN = $ND.NO;
                }
                G_GRDDETAIL.data.addItem(newRowData);
            }
        } finally {
            G_GRDDETAIL.data.endUpdate();
        }

        // 가용재고 체크
        for (rIndex = 0, rCount = G_GRDDETAIL.data.getLength(); rIndex < rCount; rIndex++) {
            grdDetailOnCheckPStock(rIndex, true);
        }

        // 예정으로 등록/ 등록수정
        $NC.setEnable("#edtBu_Cd", false);
        $NC.setEnable("#edtBu_Nm", false);
        $NC.setEnable("#btnQBu_Cd", false);
        $NC.setEnable("#cboInout_Cd", false);
        // $NC.setEnable("#cboOrder_Div", false);
        $NC.setEnable("#edtDelivery_Cd", false);
        $NC.setEnable("#btnDelivery_Cd", false);

        // 주문/수령자 정보 모두 Disabled
        $NC.setEnable("#ctrPopupView .subData", false);
        $NC.setEnable("#btnShipper_Zip_Cd", false); // 우편번호 팝업버튼 비활성화
        $NC.setEnable("#btnShipper_Zip_Cd_Clear", false); // 우편번호, 주소 초기화 버튼 비활성화

        // 예정으로 등록
        if ($NC.G_VAR.G_PARAMETER.P_PROCESS_CD == $ND.C_PROCESS_ORDER) {
        }
        // 등록수정
        else if ($NC.G_VAR.G_PARAMETER.P_PROCESS_CD == $ND.C_PROCESS_ENTRY_POPUP) {
            $NC.setEnable("#dtpOutbound_Date", false);
            $NC.setEnable("#cboDelivery_Batch", false);
            $NC.setEnable("#btnDelivery_Batch", false);
            $NC.setEnable("#edtDelivery_Batch_Nm", false);
        }

        $NC.G_VAR.ORDER_YN = $NC.isNotNull($NC.getValue("#edtOrder_No")) ? $ND.C_YES : $ND.C_NO;

        $NC.setFocus("#edtRemark1");

        $NC.setGridSelectRow(G_GRDDETAIL, 0);
    }

    // 조회조건 - 출고구분 세팅
    $NC.setInitCombo("/WC/getDataSet.do", {
        P_QUERY_ID: "WC.POP_CMCODE",
        P_QUERY_PARAMS: {
            P_COMMON_GRP: "INOUT_CD",
            P_COMMON_CD: $ND.C_ALL,
            P_ATTR01_CD: $ND.C_INOUT_GRP_DF,
            P_VIEW_DIV: "1" // 1:등록팝업, 2:조회팝업
        }
    }, {
        selector: "#cboInout_Cd",
        codeField: "COMMON_CD",
        nameField: "COMMON_NM",
        fullNameField: "COMMON_CD_F",
        selectOption: $NC.G_VAR.G_PARAMETER.P_PROCESS_CD == $ND.C_PROCESS_ENTRY_NEW ? "F" : null,
        onComplete: function() {
            if ($NC.G_VAR.G_PARAMETER.P_PROCESS_CD == $ND.C_PROCESS_ENTRY_NEW) {
                $NC.G_VAR.masterData.INOUT_CD = $NC.getValue("#cboInout_Cd");
            } else {
                $NC.setValue("#cboInout_Cd", $NC.G_VAR.masterData.INOUT_CD);
            }
        }
    });

    // 주문구분
    $NC.setInitCombo("/WC/getDataSet.do", {
        P_QUERY_ID: "WC.POP_CMCODE",
        P_QUERY_PARAMS: {
            P_COMMON_GRP: "ORDER_DIV",
            P_COMMON_CD: $ND.C_ALL,
            P_VIEW_DIV: "1" // 1:등록팝업, 2:조회팝업
        }
    }, {
        selector: "#cboOrder_Div",
        codeField: "COMMON_CD",
        nameField: "COMMON_NM",
        fullNameField: "COMMON_CD_F",
