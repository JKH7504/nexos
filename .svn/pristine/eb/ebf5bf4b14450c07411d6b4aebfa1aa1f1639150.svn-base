<!DOCTYPE HTML>
<html lang="ko">
<head>
<!-- ${NEXOS_META} -->
<!-- ${NEXOS_LIBRARY} ${} ${../../../} -->
<!-- PROGRAM CSS/JS -->
<!-- <link rel="stylesheet" href="css/CMC05040E1.css" /> -->
<script src="js/CMC05040E1.js"></script>
</head>
<body>
    <!-- 조회 조건 -->
    <div id="ctrConditionView" class="ctrCondition">
        <table class="ctrInner">
            <tr>
                <td>
                    <div id="lblQCenter_Cd" class="lblCondition">물류센터</div>
                    <select id="cboQCenter_Cd" class="cboKey" style="width: 150px;"></select>
                </td>
                <td>
                    <div id="lblQItem_State" class="lblCondition">상품상태</div>
                    <select id="cboQItem_State" class="cboNormal" style="width: 150px;"></select>
                </td>
                <td>
                    <div id="lblQZone_Cd" class="lblCondition">존구분</div>
                    <select id="cboQZone_Cd" class="cboNormal" style="width: 200px;"></select>
                </td>
            </tr>
        </table>
    </div>
    <!-- 고정로케이션 마스터뷰 -->
    <div id="divLeftView" class="ctrLeft">
        <table class="ctrTitleBar">
            <tr>
                <td id="ctrTitle_grdMaster" class="lblTitle">고정로케이션</td>
                <td id="ctrRows_grdMaster" class="lblRows">0/0 Rows</td>
            </tr>
        </table>
        <div id="grdMaster"></div>
    </div>
    <!-- 상품 지정 -->
    <div id="divRightView" class="ctrRight">
        <table class="ctrTitleBar">
            <tr>
                <td id="ctrTitle_grdDetail" class="lblTitle">상품 지정</td>
                <td>
                    <input id="btnInquiryItem" class="btnImgInquiry" type="button" value="조회" style="width: 55px;" />
                    <input id="btnAddItem" class="btnImgNormal" type="button" value="등록" style="width: 55px;" />
                </td>
                <td id="ctrRows_grdDetail" class="lblRows">0/0 Rows</td>
            </tr>
        </table>
        <!-- 상품 지정 조회조건 -->
        <div id="ctrAdditional_grdDetail" class="ctrAdditional bottom ctrSubCondition">
            <table class="ctrInner">
                <tr>
                    <td colspan="2">
                        <div id="lblQPopup_Bu_Cd" class="lblCondition">사업부</div>
                        <input id="edtQBu_Cd" class="edtKey" type="text" style="width: 60px;" />
                        <input id="btnQBu_Cd" class="btnIcoPopup" type="button" />
                        <input id="edtQBu_Nm" class="edtDisplay" type="text" disabled="disabled" style="width: 196px;" />
                    </td>
                </tr>
                <tr>
                    <td colspan="2">
                        <div id="lblQPopup_Brand_Cd" class="lblCondition">브랜드</div>
                        <input id="edtQBrand_Cd" class="edtNormal" type="text" style="width: 60px;" />
                        <input id="btnQBrand_Cd" class="btnIcoPopup" type="button" />
                        <input id="edtQBrand_Nm" class="edtDisplay" type="text" disabled="disabled" style="width: 196px;" />
                    </td>
                </tr>
                <tr>
                    <td colspan="2">
                        <div id="lblQItem_Cd" class="lblCondition">상품코드</div>
                        <input id="edtQItem_Cd" class="edtNormal" type="text" style="width: 286px;" />
                    </td>
                </tr>
                <tr>
                    <td colspan="2">
                        <div id="lblQItem_Nm" class="lblCondition">상품명</div>
                        <input id="edtQItem_Nm" class="edtNormal" type="text" style="width: 286px;" />
                    </td>
                </tr>
                <tr>
                    <td colspan="2">
                        <div id="lblQYear_Div" class="lblCondition">연도구분</div>
                        <select id="cboQYear_Div" class="cboNormal" style="width: 150px;"></select>
                    </td>
                </tr>
                <tr>
                    <td colspan="2">
                        <div id="lblQSeason_Div" class="lblCondition">시즌구분</div>
                        <select id="cboQSeason_Div" class="cboNormal" style="width: 150px;"></select>
                    </td>
                </tr>
                <tr>
                    <td>
                        <div id="lblItem_State" class="lblKey">상품상태</div>
                        <select id="cboItem_State" class="cboKey" style="width: 150px;"></select>
                    </td>
                    <td>
                        <input id="btnLocationFixReset" class="btnNormal" type="button" value="고정LOC 재설정" style="width: 106px;" />
                    </td>
                </tr>
                <tr>
                    <td colspan="2">
                        <div id="lblOperation_Div" class="lblKey">운영구분</div>
                        <div class="ctrGroupBox" style="width: 286px;">
                            <input id="rgbOperation_Div1" name="rgbOperation_Div" class="rgbNormal" type="radio" value="1" />
                            상품별
                            <input id="rgbOperation_Div2" name="rgbOperation_Div" class="rgbNormal" type="radio" value="2" />
                            스타일별
                            <input id="rgbOperation_Div3" name="rgbOperation_Div" class="rgbNormal" type="radio" value="3" />
                            스타일+컬러별
                        </div>
                    </td>
                </tr>
            </table>
        </div>
        <div id="grdDetail"></div>
    </div>
</body>
</html>
