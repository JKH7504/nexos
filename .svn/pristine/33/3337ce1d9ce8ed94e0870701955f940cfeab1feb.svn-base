<!DOCTYPE HTML>
<html lang="ko">
<head>
<!-- ${NEXOS_META} -->
<!-- ${NEXOS_LIBRARY} ${} ${../../../} -->
<!-- PROGRAM CSS/JS -->
<!-- <link rel="stylesheet" href="css/CMC05010E0.css" /> -->
<script src="js/CMC05010E0.js"></script>
</head>
<body>
    <!-- 조회 조건 -->
    <div id="ctrConditionView" class="ctrCondition">
        <table class="ctrInner">
            <tr>
                <td>
                    <div id="lblQCenter_Cd" class="lblCondition">물류센터</div>
                    <select id="cboQCenter_Cd" class="cboKey" style="width: 150px;"></select>
                </td>
                <td>
                    <div id="lblQPopup_Bu_Cd" class="lblCondition">사업부</div>
                    <input id="edtQBu_Cd" class="edtKey" type="text" style="width: 60px;" />
                    <input id="btnQBu_Cd" class="btnIcoPopup" type="button" />
                    <input id="edtQBu_Nm" class="edtDisplay" type="text" disabled="disabled" style="width: 150px;" />
                    <input id="edtQCust_Cd" class="edtHidden" type="text" disabled="disabled" />
                </td>
                <td>
                    <div id="lblQItem_State" class="lblCondition">상품상태</div>
                    <select id="cboQItem_State" class="cboKey" style="width: 120px;"></select>
                </td>
            </tr>
        </table>
    </div>
    <div id="divMasterView">
        <div>
            <!-- 권장로케이션 마스터 -->
            <table class="ctrTitleBar">
                <tr>
                    <td id="ctrTitle_grdMaster" class="lblTitle">권장로케이션</td>
                    <td id="ctrRows_grdMaster" class="lblRows">0/0 Rows</td>
                </tr>
            </table>
            <div id="grdMaster"></div>
        </div>
        <div>
            <!-- 권장로케이션 지정 -->
            <div id="divDetailView" class="ctrLeft" style="border-width: 0 1px 0 0">
                <table class="ctrTitleBar">
                    <tr>
                        <td>
                            <div id="dspRecommendGroup" class="lblTitle">권장로케이션 지정</div>
                        </td>
                    </tr>
                </table>
                <div id="divDetailInfoView" class="ctrAdditional bottom" style="margin-bottom: -1px;">
                    <div class="ctrGroupRow styLeft">
                        <div id="lblCreate_Div" class="lblKey">생성구분</div>
                        <select id="cboCreate_Div" class="cboKey" style="width: 150px;"></select>
                        <input id="btnEntryRecommendLoc" class="btnImgNormal" type="button" value="권장로케이션등록" style="width: 135px;" />
                    </div>
                </div>
                <div id="grdZone" class="ctrLeft" style="border-width: 0 1px 0 0;"></div>
                <div id="grdBank" class="ctrMiddle" style="border-width: 0 1px 0 1px;"></div>
                <div id="grdBay" class="ctrMiddle" style="border-width: 0 1px 0 1px;"></div>
                <div id="grdLev" class="ctrRight" style="border-width: 0 0 0 1px;"></div>
            </div>
            <!-- 보관구분 -->
            <div id="divKeepDivView" class="ctrMiddle" style="border-width: 0 0 0 1px; display: none;">
                <table class="ctrTitleBar">
                    <tr>
                        <td id="ctrTitle_grdSub" class="lblTitle">상품보관 지정</td>
                        <td id="ctrRows_grdSub" class="lblRows">0/0 Rows</td>
                    </tr>
                </table>
                <div id="grdSub"></div>
            </div>
            <!-- 상품그룹 지정 -->
            <div id="divItemGroupView" class="ctrMiddle" style="border-width: 0 0 0 1px; display: none;">
                <table class="ctrTitleBar">
                    <tr>
                        <td id="dspTitle_ItemGroup" class="lblTitle">상품그룹 지정</td>
                    </tr>
                </table>
                <div id="grdDepart" class="ctrLeft" style="border-width: 0 1px 0 0;"></div>
                <div id="grdLine" class="ctrMiddle" style="border-width: 0 1px 0 1px;"></div>
                <div id="grdClass" class="ctrRight" style="border-width: 0 0 0 1px;"></div>
            </div>
            <!-- 상품 지정 -->
            <div id="divItemView" class="ctrRight" style="border-width: 0 0 0 1px; display: none;">
                <table class="ctrTitleBar">
                    <tr>
                        <td id="ctrTitle_grdItem" class="lblTitle">상품 지정</td>
                        <td>
                            <input id="btnInquiryItem" class="btnImgInquiry" type="button" value="조회" style="width: 55px;" />
                        </td>
                        <td id="ctrRows_grdItem" class="lblRows">0/0 Rows</td>
                    </tr>
                </table>
                <div id="ctrAdditional_grdItem" class="ctrAdditional bottom ctrSubCondition">
                    <table class="ctrInner">
                        <tr>
                            <td>
                                <div id="lblQPopup_Brand_Cd" class="lblCondition">브랜드</div>
                                <input id="edtQBrand_Cd" class="edtNormal" type="text" style="width: 60px;" />
                                <input id="btnQBrand_Cd" class="btnIcoPopup" type="button" />
                                <input id="edtQBrand_Nm" class="edtDisplay" type="text" disabled="disabled" style="width: 150px;" />
                            </td>
                            <td>
                                <div id="lblQKeep_Div" class="lblCondition">보관구분</div>
                                <select id="cboQKeep_Div" class="cboNormal" style="width: 150px;"></select>
                            </td>
                        </tr>
                        <tr>
                            <td>
                                <div id="lblQItem_Cd" class="lblCondition">상품코드</div>
                                <input id="edtQItem_Cd" class="edtNormal" type="text" style="width: 150px;" />
                            </td>
                            <td>
                                <div id="lblQItem_Nm" class="lblCondition">상품명</div>
                                <input id="edtQItem_Nm" class="edtNormal" type="text" style="width: 150px;" />
                            </td>
                        </tr>
                    </table>
                </div>
                <div id="grdItem"></div>
            </div>
        </div>
    </div>
</body>
</html>
