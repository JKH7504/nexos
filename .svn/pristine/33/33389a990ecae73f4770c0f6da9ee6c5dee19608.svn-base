       SET LAST_USER_ID             = #{P_USER_ID}
          ,LAST_DATETIME            = SYSDATE
          <if test="P_INOUT_CD                != null">,INOUT_CD                 = #{P_INOUT_CD}               </if>
          <if test="P_ETC_STATE               != null">,ETC_STATE                = #{P_ETC_STATE}              </if>
