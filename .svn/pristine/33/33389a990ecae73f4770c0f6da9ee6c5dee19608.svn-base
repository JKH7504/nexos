          <if test="P_LINK_BU_CD              != null">,LINK_BU_CD               = #{P_LINK_BU_CD}             </if>
          <if test="P_LINK_ORDER_DATE         != null">,LINK_ORDER_DATE          = #{P_LINK_ORDER_DATE}        </if>
          <if test="P_LINK_ORDER_NO           != null">,LINK_ORDER_NO            = #{P_LINK_ORDER_NO}          </if>
