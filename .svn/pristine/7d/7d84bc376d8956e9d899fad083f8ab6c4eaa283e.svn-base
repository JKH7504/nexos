function onProc(ajaxData) {

    _Inquiry();
    G_GRDMASTER.lastKeyVal = $NC.getValue("#dtpOutbound_Date");
    alert($NC.getDisplayMsg("JS.LOB06010E0.007", "변경이 완료되었습니다."));
}

function btnChangeDateOnClick() {

    if (!$NC.getProgramPermission().canSave) {
        alert($NC.getDisplayMsg("JS.LOB06010E0.008", "해당 프로그램의 저장권한이 없습니다."));
        return;
    }

    if (G_GRDMASTER.data.getLength() == 0 || $NC.isNull(G_GRDMASTER.lastRow)) {
        alert($NC.getDisplayMsg("JS.LOB06010E0.009", "조회 후 처리하십시오."));
        return;
    }

    var OUTBOUND_DATE_NEW = $NC.getValue("#dtpOutbound_Date");
    if ($NC.isNull(OUTBOUND_DATE_NEW)) {
        alert($NC.getDisplayMsg("JS.LOB06010E0.010", "변경일자를 선택하십시오."));
        $NC.setFocus("#dtpOutbound_Date");
        return;
    }

    if (!confirm($NC.getDisplayMsg("JS.LOB06010E0.011", "선택한 일자로 출고일자를 변경 하시겠습니까?"))) {
        return;
    }

    var rowData = G_GRDMASTER.data.getItem(G_GRDMASTER.lastRow);
    // LI_CHANGE_OUTBOUND_DATE 호출
    $NC.serviceCall("/LOB06010E0/callLOChangeOutboundDate.do", {
        P_CENTER_CD: rowData.CENTER_CD,
        P_BU_CD: rowData.BU_CD,
        P_OUTBOUND_DATE: rowData.OUTBOUND_DATE,
        P_OUTBOUND_NO: rowData.OUTBOUND_NO,
        P_OUTBOUND_DATE_NEW: OUTBOUND_DATE_NEW,
        P_USER_ID: $NC.G_USERINFO.USER_ID
    }, onProc);
}
