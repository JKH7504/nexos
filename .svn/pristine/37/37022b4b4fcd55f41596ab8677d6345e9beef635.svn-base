<!DOCTYPE HTML>
<html lang="ko">
<head>
<!-- ${NEXOS_META} -->
<!-- ${NEXOS_LIBRARY} ${} ${../../../} -->
<!-- PROGRAM CSS/JS -->
<!-- <link rel="stylesheet" href="css/LCC02091P0.css" /> -->
<script src="js/LCC02091P0.js"></script>
</head>
<body>
    <div id="ctrPopupView" class="ctrPopup">
        <table class="ctrInner">
            <tr>
                <td>
                    <div id="lblCenter_Cd_F" class="lblNormal">물류센터</div>
                    <input id="edtCenter_Cd_F" class="edtKey" type="text" disabled="disabled" style="width: 170px;" />
                </td>
                <td id="tdQDsp_Brand_Cd">
                    <div id="lblPopup_Brand_Cd" class="lblNormal">브랜드</div>
                    <input id="edtBrand_Cd" class="edtKey" type="text" style="width: 60px;" />
                    <input id="edtBrand_Nm" class="edtDisplay" type="text" disabled="disabled" style="width: 104px;" />
                </td>
            </tr>
            <tr>
                <td>
                    <div id="lblPopup_Bu_Cd" class="lblNormal">사업부</div>
                    <input id="edtBu_Cd" class="edtKey" type="text" style="width: 60px;" />
                    <input id="edtBu_Nm" class="edtDisplay" type="text" disabled="disabled" style="width: 104px;" />
                    <input id="edtCust_Cd" class="edtHidden" type="text" disabled="disabled" />
                </td>
                <td>
                    <div id="lblPopup_Item_Cd" class="lblKey">상품</div>
                    <input id="edtItem_Cd" class="edtKey" type="text" tabindex="3" style="width: 60px;" />
                    <input id="btnItem_Cd" class="btnIcoPopup" type="button" tabindex="2" />
                    <input id="edtItem_Nm" class="edtDisplay" type="text" disabled="disabled" style="width: 155px;" />
                </td>
            </tr>
            <tr>
                <td>
                    <div id="lblProcessing_Date_Title" class="lblKey">작업일자</div>
                    <input id="dtpProcessing_Date" class="dtpKey" type="text" tabindex="1" style="width: 100px;" />
                </td>
                <td><div id="lblProcessing_Qty_N" class="lblKey">작업수량</div>
                    <input id="edtProcessing_Qty" class="edtKey" type="text" tabindex="4" style="width: 100px;" />
                </td>
            </tr>
            <tr>
                <td>
                    <div id="lblProcessing_No_Title" class="lblNormal">작업번호</div>
                    <input id="edtProcessing_No" class="edtNormal" type="text" disabled="disabled" style="width: 100px;" />
                </td>
                <td colspan="2">
                    <div id="lblRemark1" class="lblNormal">비고</div>
                    <input id="edtRemark1" class="edtNormal" type="text" maxlength="255" tabindex="5" style="width: 542px;" />
                </td>
            </tr>
        </table>
    </div>
    <!-- 가공작업 등록 디테일-->
    <div id="divDetailView" class="ctrPopup">
        <table class="ctrTitleBar">
            <tr>
                <td id="ctrTitle_grdDetail" class="lblTitle">가공작업등록 상세정보</td>
                <td>
                    <input id="btnEntryNew" class="btnIcoNew" type="button" />
                    <input id="btnEntryDelete" class="btnIcoDelete" type="button" />
                    <input id="btnEntrySave" class="btnIcoSave" type="button" />
                </td>
                <td id="ctrRows_grdDetail" class="lblRows">0/0 Rows</td>
            </tr>
        </table>
        <div id="grdDetail"></div>
    </div>
    <div id="ctrPopupActionBar" class="ctrPopupActionBar">
        <input id="btnClose" class="btnNormal" type="button" value="닫기" style="width: 80px;" />
    </div>
</body>
</html>
