<!DOCTYPE HTML>
<html lang="ko">
<head>
<!-- ${NEXOS_META} -->
<!-- ${NEXOS_LIBRARY} ${} ${../../../} -->
<!-- PROGRAM CSS/JS -->
<!-- <link rel="stylesheet" href="css/LDC01011P0.css" /> -->
<script src="js/LDC01011P0.js"></script>
</head>
<body>
    <div id="divLeftView" class="ctrLeft" style="border-bottom-width: 0;">
        <table class="ctrTitleBar">
            <tr>
                <td id="dspTitle_SplitOrder" class="lblTitle">전표분할 상품정보</td>
            </tr>
        </table>
        <div class="ctrInner">
            <div class="ctrGroupRow">
                <div id="lblOutbound_No" class="lblNormal">출고번호</div>
                <input id="edtOutbound_No" class="edtDisplay" type="text" disabled="disabled" style="width: 100px;" />
            </div>
            <div class="ctrGroupRow">
                <div id="lblLine_No" class="lblNormal">출고순번</div>
                <input id="edtLine_No" class="edtDisplay" type="text" disabled="disabled" style="width: 40px;" />
            </div>
            <div class="ctrGroupRow">
                <div id="lblItem_Cd" class="lblNormal">상품코드</div>
                <input id="edtItem_Cd" class="edtDisplay" type="text" disabled="disabled" style="width: 100px;" />
            </div>
            <div class="ctrGroupRow">
                <div id="lblItem_Nm" class="lblNormal">상품명</div>
                <input id="edtItem_Nm" class="edtDisplay" type="text" disabled="disabled" style="width: 204px;" />
            </div>
            <div class="ctrGroupRow">
                <div id="lblItem_Spec" class="lblNormal">규격</div>
                <input id="edtItem_Spec" class="edtDisplay" type="text" disabled="disabled" style="width: 100px;" />
            </div>
            <div class="ctrGroupRow">
                <div id="lblEntry_Qty" class="lblKey">등록수량</div>
                <input id="edtEntry_Qty" class="edtDisplay" type="text" disabled="disabled" style="width: 100px;" />
            </div>
            <div class="ctrGroupRow">
                <div id="lblTotal_Entry_Qty" class="lblKey">분할수량합계</div>
                <input id="edtTotal_Entry_Qty" class="edtDisplay" type="text" disabled="disabled" style="width: 100px;" />
            </div>
        </div>
    </div>
    <div id="divRightView" class="ctrRight" style="border-bottom-width: 0;">
        <table class="ctrTitleBar">
            <tr>
                <td id="ctrTitle_grdDetail" class="lblTitle">전표분할 정보</td>
                <td>
                    <input id="btnEntryNew" class="btnIcoNew" type="button" />
                    <input id="btnEntryDelete" class="btnIcoDelete" type="button" />
                    <input id="btnEntrySave" class="btnIcoSave" type="button" />
                </td>
                <td id="ctrRows_grdDetail" class="lblRows">0/0 Rows</td>
            </tr>
        </table>
        <div id="grdDetail"></div>
    </div>
    <div id="ctrPopupActionBar" class="ctrPopupActionBar">
        <input id="btnClose" class="btnNormal" type="button" value="닫기" style="width: 80px;" />
    </div>
</body>
</html>
