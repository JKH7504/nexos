<!DOCTYPE HTML>
<html lang="ko">
<head>
<!-- ${NEXOS_META} -->
<!-- ${NEXOS_LIBRARY} ${} ${../../../} -->
<!-- PROGRAM CSS/JS -->
<link rel="stylesheet" href="css/RIB05050E0.css" />
<script src="js/RIB05050E0.js"></script>
</head>
<body>
    <!-- 조회 조건 -->
    <div id="ctrConditionView" class="ctrCondition">
        <table class="ctrInner">
            <tr>
                <td>
                    <div id="lblQCenter_Cd" class="lblCondition">물류센터</div>
                    <select id="cboQCenter_Cd" class="cboKey" style="width: 150px;"></select>
                </td>
                <td>
                    <div id="lblQPopup_Bu_Cd" class="lblCondition">사업부</div>
                    <input id="edtQBu_Cd" class="edtKey" type="text" style="width: 60px;" />
                    <input id="btnQBu_Cd" class="btnIcoPopup" type="button" />
                    <input id="edtQBu_Nm" class="edtDisplay" type="text" disabled="disabled" style="width: 120px;" />
                    <input id="edtQCust_Cd" class="edtHidden" type="text" disabled="disabled" />
                </td>
                <td>
                    <div id="lblQInbound_Date" class="lblCondition">반입일자</div>
                    <div id="ctrQT1_Inbound_Date" class="ctrGroup" style="width: 220px;">
                        <input id="dtpQInbound_Date" class="dtpKey" type="text" style="width: 100px;" />
                    </div>
                    <div id="ctrQT2_Inbound_Date" class="ctrGroup">
                        <input id="dtpQInbound_Date1" class="dtpKey" type="text" style="width: 100px;" />
                        <span class="lblTerm">▶</span>
                        <input id="dtpQInbound_Date2" class="dtpKey" type="text" style="width: 100px;" />
                    </div>
                </td>
            </tr>
            <tr>
                <td>
                    <div id="lblQPopup_Delivery_Cd" class="lblCondition">배송처</div>
                    <input id="edtQDelivery_Cd" class="edtNormal" type="text" style="width: 60px;" />
                    <input id="btnQDelivery_Cd" class="btnIcoPopup" type="button" />
                    <input id="edtQDelivery_Nm" class="edtDisplay" type="text" disabled="disabled" style="width: 120px;" />
                </td>
                <td>
                    <div id="lblQBu_No" class="lblCondition">전표번호</div>
                    <input id="edtQBu_No" class="edtNormal" type="text" style="width: 210px;" />
                </td>
                <td id="ctrQItem_Serial">
                    <div id="lblQItem_Serial" class="lblCondition">시리얼번호</div>
                    <input id="edtQItem_Serial" class="edtNormal" type="text" style="width: 220px;" />
                </td>
            </tr>
        </table>
        <!-- 추가 조건 -->
        <table id="ctrAdditionalConditionView" class="ctrAdditionalCondition">
            <tr>
                <td id="ctrQDsp_Brand_Cd">
                    <div id="lblQPopup_Brand_Cd" class="lblCondition">브랜드</div>
                    <input id="edtQBrand_Cd" class="edtNormal" type="text" style="width: 60px;" />
                    <input id="btnQBrand_Cd" class="btnIcoPopup" type="button" />
                    <input id="edtQBrand_Nm" class="edtDisplay" type="text" disabled="disabled" style="width: 120px;" />
                </td>
            </tr>
            <tr>
                <td>
                    <div id="lblQItem_Cd" class="lblCondition">상품코드</div>
                    <input id="edtQItem_Cd" class="edtNormal" type="text" style="width: 210px;" />
                </td>
            </tr>
            <tr>
                <td>
                    <div id="lblQItem_Nm" class="lblCondition">상품명</div>
                    <input id="edtQItem_Nm" class="edtNormal" type="text" style="width: 210px;" />
                </td>
            </tr>
        </table>
    </div>
    <div id="ctrMasterView">
        <!-- Tab Handler -->
        <ul>
            <li><a href="#ctrT1TabSheetView">반입시리얼 등록</a></li>
            <li><a href="#ctrT2TabSheetView">반입시리얼 등록내역</a></li>
        </ul>
        <!-- 반입 등록 -->
        <div id="ctrT1TabSheetView">
            <div id="ctrLeftView" class="ctrLeft" style="border-width: 0 1px 0 0;">
                <div>
                    <table class="ctrTitleBar">
                        <tr>
                            <td id="ctrTitle_grdT1Master" class="lblTitle">반입정보</td>
                            <td id="ctrRows_grdT1Master" class="lblRows">0/0 Rows</td>
                        </tr>
                    </table>
                    <div id="grdT1Master"></div>
                </div>
                <div>
                    <table class="ctrTitleBar">
                        <tr>
                            <td id="ctrTitle_grdT1Detail" class="lblTitle">반입상세정보</td>
                            <td id="ctrRows_grdT1Detail" class="lblRows">0/0 Rows</td>
                        </tr>
                    </table>
                    <div id="grdT1Detail"></div>
                </div>
            </div>
            <div id="ctrRightView" class="ctrRight scanInspection" style="border-width: 0 0 0 1px;">
                <table class="ctrTitleBar">
                    <tr>
                        <td id="ctrTitle_grdT1Sub" class="lblTitle">반입시리얼 정보</td>
                        <td>
                            <input id="btnSerialHistory" class="btnImgPopup" type="button" value="이력조회" style="width: 80px;" />
                        </td>
                        <td id="ctrRows_grdT1Sub" class="lblRows">0/0 Rows</td>
                    </tr>
                </table>
                <div id="ctrAdditional_grdT1Sub" class="ctrAdditional bottom">
                    <table class="ctrInner">
                        <tr>
                            <td colspan="3">
                                <div id="lblItem_Bar_Cd" class="lblKey">시리얼번호</div>
                                <input id="edtItem_Bar_Cd" class="edtKey styScan" type="text" style="width: 380px;" />
                            </td>
                        </tr>
                        <tr>
                            <td colspan="3">
                                <div id="lblLine_No" class="lblNormal">순번</div>
                                <input id="edtLine_No" class="edtDisplay" type="text" disabled="disabled" style="width: 95px;" />
                            </td>
                        </tr>
                        <tr>
                            <td colspan="3">
                                <div id="lblItem_Cd" class="lblNormal">상품코드</div>
                                <input id="edtItem_Cd" class="edtDisplay" type="text" disabled="disabled" style="width: 95px;" />
                                <input id="edtItem_Nm" class="edtDisplay" type="text" disabled="disabled" style="width: 173px;" />
                                <input id="edtItem_Spec" class="edtDisplay" type="text" disabled="disabled" style="width: 100px;" />
                            </td>
                        </tr>
                        <tr>
                            <td colspan="3">
                                <div id="lblBrand_Cd" class="lblNormal">브랜드</div>
                                <input id="edtBrand_Cd" class="edtDisplay" type="text" disabled="disabled" style="width: 95px;" />
                                <input id="edtBrand_Nm" class="edtDisplay" type="text" disabled="disabled" style="width: 173px;" />
                            </td>
                        </tr>
                        <tr>
                            <td>
                                <div id="lblConfirm_Qty" class="lblNormal">확정수량</div>
                                <input id="edtConfirm_Qty" class="edtDisplay styLarge" type="text" disabled="disabled" style="width: 95px;" />
                            </td>
                            <td style="width: 27px;"></td>
                            <td>
                                <div id="lblSerial_Qty" class="lblSpecial">시리얼수량</div>
                                <input id="edtSerial_Qty" class="edtDisplay styLarge" type="text" disabled="disabled" style="width: 95px;" />
                            </td>
                        </tr>
                        <tr>
                            <td colspan="3">
                                <div id="lblItem_Serial" class="lblKey">시리얼번호</div>
                                <input id="edtItem_Serial" class="edtKey styScan" type="text" style="width: 380px;" />
                            </td>
                        </tr>
                    </table>
                </div>
                <div id="grdT1Sub"></div>
            </div>
        </div>
        <!-- 반입시리얼 내역 -->
        <div id="ctrT2TabSheetView" style="display: none;">
            <table class="ctrTitleBar">
                <tr>
                    <td id="ctrTitle_grdT2Master" class="lblTitle">반입시리얼 내역</td>
                    <td id="ctrRows_grdT2Master" class="lblRows">0/0 Rows</td>
                </tr>
            </table>
            <div id="grdT2Master"></div>
        </div>
    </div>
    <!-- 이력조회 Overlay -->
    <div id="ctrSerialHistoryOverlay" style="display: none;">
        <div id="ctrSerialHistoryView" class="ctrPopup scanInspection">
            <div id="ctrAdditional_grdSerialHistory" class="ctrAdditional top">
                <table class="ctrInner">
                    <tr>
                        <td>
                            <div id="lblL1_Item_Cd" class="lblNormal">상품코드</div>
                            <input id="edtL1_Item_Cd" class="edtDisplay" type="text" disabled="disabled" style="width: 100px;" />
                            <input id="edtL1_Item_Nm" class="edtDisplay" type="text" disabled="disabled" style="width: 220px;" />
                            <input id="edtL1_Item_Spec" class="edtDisplay" type="text" disabled="disabled" style="width: 100px;" />
                        </td>
                    </tr>
                    <tr>
                        <td>
                            <div id="lblL1_Brand_Cd" class="lblNormal">브랜드</div>
                            <input id="edtL1_Brand_Cd" class="edtDisplay" type="text" disabled="disabled" style="width: 100px;" />
                            <input id="edtL1_Brand_Nm" class="edtDisplay" type="text" disabled="disabled" style="width: 220px;" />
                        </td>
                    </tr>
                    <tr>
                        <td>
                            <div id="lblL1_Item_Serial" class="lblKey">시리얼번호</div>
                            <input id="edtL1_Item_Serial" class="edtKey styScan" type="text" style="width: 432px;" />
                        </td>
                    </tr>
                </table>
            </div>
            <table class="ctrTitleBar">
                <tr>
                    <td id="ctrTitle_grdSerialHistory" class="lblTitle">시리얼 입출고이력 정보</td>
                    <td id="ctrRows_grdSerialHistory" class="lblRows">0/0 Rows</td>
                </tr>
            </table>
            <div id="grdSerialHistory"></div>
        </div>
        <div class="ctrPopupActionBar">
            <input id="btnSerialHistoryClose" class="btnNormal" type="button" value="닫기" style="width: 80px;" />
        </div>
    </div>
</body>
</html>
