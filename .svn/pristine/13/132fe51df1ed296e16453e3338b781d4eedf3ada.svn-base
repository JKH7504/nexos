<!DOCTYPE HTML>
<html lang="ko">
<head>
<!-- ${NEXOS_META} -->
<!-- ${NEXOS_LIBRARY} ${} ${../../../} -->
<!-- PROGRAM CSS/JS -->
<!-- <link rel="stylesheet" href="css/CMC04070E0.css" /> -->
<script src="js/CMC04070E0.js"></script>
</head>
<body>
    <div id="ctrConditionView" class="ctrCondition">
        <table class="ctrInner">
            <tr>
                <td>
                    <div id="lblQPopup_Bu_Cd" class="lblCondition">사업부</div>
                    <input id="edtQBu_Cd" class="edtKey" type="text" style="width: 60px;" />
                    <input id="btnQBu_Cd" class="btnIcoPopup" type="button" />
                    <input id="edtQBu_Nm" class="edtDisplay" type="text" disabled="disabled" style="width: 120px;" />
                    <input id="edtQCust_Cd" class="edtHidden" type="text" disabled="disabled" />
                </td>
                <td>
                    <div id="lblQOpen_Date" class="lblCondition">거래일자</div>
                    <input id="dtpQOpen_Date1" class="dtpKey" type="text" style="width: 100px;" />
                    <span class="lblTerm">▶</span>
                    <input id="dtpQOpen_Date2" class="dtpKey" type="text" style="width: 100px;" />
                </td>
                <td>
                    <div id="lblQDeal_Div" class="lblCondition">거래구분</div>
                    <div class="ctrGroupBox">
                        <input id="chkQDeal_Div1" name="chkQDeal_Div" class="chkNormal" type="checkbox" value="1" />
                        거래진행
                        <input id="chkQDeal_Div2" name="chkQDeal_Div" class="chkNormal" type="checkbox" value="2" />
                        거래종료
                        <input id="chkQDeal_Div3" name="chkQDeal_Div" class="chkNormal" type="checkbox" value="3" />
                        조회종료
                    </div>
                </td>
            </tr>
            <tr>
                <td>
                    <div id="lblQPopup_Brand_Cd" class="lblCondition">브랜드</div>
                    <input id="edtQBrand_Cd" class="edtNormal" type="text" style="width: 60px;" />
                    <input id="btnQBrand_Cd" class="btnIcoPopup" type="button" />
                    <input id="edtQBrand_Nm" class="edtDisplay" type="text" disabled="disabled" style="width: 120px;" />
                </td>
                <td>
                    <div id="lblQPromotion_Div" class="lblCondition">프로모션구분</div>
                    <select id="cboQPromotion_Div" class="cboNormal" style="width: 220px;"></select>
                </td>
                <td>
                    <div id="lblQPromotion_Cd_" class="lblCondition">프로모션코드</div>
                    <input id="edtQPromotion_Cd" class="edtNormal" type="text" style="width: 230px;" />
                </td>
            </tr>
        </table>
        <!-- 추가 조건 -->
        <table id="ctrAdditionalConditionView" class="ctrAdditionalCondition">
            <tr>
                <td>
                    <div id="lblQPromotion_Nm" class="lblCondition" style="width: 100px;">프로모션명</div>
                    <input id="edtQPromotion_Nm" class="edtNormal" type="text" style="width: 210px;" />
                </td>
            </tr>
            <tr>
                <td>
                    <div id="lblQItem_Cd" class="lblCondition" style="width: 100px;">상품코드</div>
                    <input id="edtQItem_Cd" class="edtNormal" type="text" style="width: 210px;" />
                </td>
            </tr>
            <tr>
                <td>
                    <div id="lblQPromotion_Item_Cd" class="lblCondition" style="width: 100px;">프로모션상품코드</div>
                    <input id="edtQPromotion_Item_Cd" class="edtNormal" type="text" style="width: 210px;" />
                </td>
            </tr>
        </table>
    </div>
    <div id="divMasterView" class="ctrNormal">
        <!-- Tab Handler -->
        <ul>
            <li id="divTab1"><a href="#divT1TabSheetView">프로모션 등록</a></li>
            <li id="divTab2"><a href="#divT2TabSheetView">온라인몰 등록</a></li>
        </ul>
        <!-- 매뉴얼증정 등록 -->
        <div id="divT1TabSheetView">
            <div id="divT1Master">
                <table class="ctrTitleBar">
                    <tr>
                        <td id="ctrTitle_grdT1Master" class="lblTitle">프로모션등록 정보</td>
                        <td id="ctrRows_grdT1Master" class="lblRows">0/0 Rows</td>
                    </tr>
                </table>
                <div id="grdT1Master"></div>
            </div>
            <div>
                <div id="divT1BottomLeft" class="ctrLeft" style="border-width: 0 1px 0 0;">
                    <table class="ctrTitleBar">
                        <tr>
                            <td id="ctrTitle_grdT1Detail" class="lblTitle">프로모션상품</td>
                            <td id="ctrRows_grdT1Detail" class="lblRows">0/0 Rows</td>
                        </tr>
                    </table>
                    <div id="grdT1Detail"></div>
                </div>
                <div id="divT1BottomRight" class="ctrRight" style="border-width: 0 0 0 1px; display: none;">
                    <table class="ctrTitleBar">
                        <tr>
                            <td id="ctrTitle_grdT1Sub" class="lblTitle">프로모션제외상품</td>
                            <td id="ctrRows_grdT1Sub" class="lblRows">0/0 Rows</td>
                        </tr>
                    </table>
                    <div id="grdT1Sub"></div>
                </div>
            </div>
        </div>
        <!-- 온라인몰 등록 -->
        <div id="divT2TabSheetView" style="display: none;">
            <div id="divT2Master">
                <table class="ctrTitleBar">
                    <tr>
                        <td id="ctrTitle_grdT2Master" class="lblTitle">프로모션 정보</td>
                        <td id="ctrRows_grdT2Master" class="lblRows">0/0 Rows</td>
                    </tr>
                </table>
                <div id="grdT2Master"></div>
            </div>
            <div id="divT2BottomView">
                <div id="divT2BottomLeft" class="ctrLeft" style="border-width: 0 1px 0 0;">
                    <table class="ctrTitleBar">
                        <tr>
                            <td id="ctrTitle_grdT2Detail" class="lblTitle">온라인몰 등록 내역</td>
                            <td>
                                <input id="btnDeleteDelivery" class="btnImgCancel" type="button" value="삭제" style="width: 55px;" />
                            </td>
                            <td id="ctrRows_grdT2Detail" class="lblRows">0/0 Rows</td>
                        </tr>
                    </table>
                    <div id="grdT2Detail"></div>
                </div>
                <div id="divT2BottomRight" class="ctrRight" style="border-width: 0 0 0 1px;">
                    <table class="ctrTitleBar">
                        <tr>
                            <td id="ctrTitle_grdT2Sub" class="lblTitle">미등록 온라인몰</td>
                            <td>
                                <input id="btnAddDelivery" class="btnImgNormal" type="button" value="등록" style="width: 55px;" />
                            </td>
                            <td id="ctrRows_grdT2Sub" class="lblRows">0/0 Rows</td>
                        </tr>
                    </table>
                    <div id="grdT2Sub"></div>
                </div>
            </div>
        </div>
    </div>
</body>
</html>
