<!DOCTYPE HTML>
<html lang="ko">
<head>
<!-- ${NEXOS_META} -->
<!-- ${NEXOS_LIBRARY} ${} ${../../../} -->
<!-- PROGRAM CSS/JS -->
<link rel="stylesheet" href="css/LSC03030Q0.css" />
<script src="js/LSC03030Q0.js"></script>
</head>
<body>
    <div id="ctrConditionView" class="ctrCondition">
        <!-- 조회 조건 -->
        <table class="ctrInner">
            <tr>
                <td>
                    <div id="lblQCenter_Cd" class="lblCondition">물류센터</div>
                    <select id="cboQCenter_Cd" class="cboKey" style="width: 150px;"></select>
                </td>
                <td>
                    <div id="lblQPopup_Bu_Cd" class="lblCondition">사업부</div>
                    <input id="edtQBu_Cd" class="edtKey" type="text" style="width: 60px;" />
                    <input id="btnQBu_Cd" class="btnIcoPopup" type="button" />
                    <input id="edtQBu_Nm" class="edtDisplay" type="text" disabled="disabled" style="width: 120px;" />
                    <input id="edtQCust_Cd" class="edtHidden" type="text" disabled="disabled" />
                </td>
                <td>
                    <div id="lblQInout_Date" class="lblCondition">기준일자</div>
                    <input id="dtpQInout_Date" class="dtpKey" type="text" style="width: 100px;" />
                </td>
            </tr>
            <tr>
                <td>
                    <div id="lblQItem_Cd" class="lblCondition">상품코드</div>
                    <input id="edtQItem_Cd" class="edtNormal" type="text" style="width: 210px;" />
                </td>
                <td>
                    <div id="lblQItem_Nm" class="lblCondition">상품명</div>
                    <input id="edtQItem_Nm" class="edtNormal" type="text" style="width: 210px;" />
                </td>
                <td>
                    <div id="divQConditionItemState" class="ctrGroup">
                        <div id="lblQItem_State" class="lblCondition">상품상태</div>
                        <select id="cboQItem_State" class="cboNormal" style="width: 150px;"></select>
                    </div>
                </td>
            </tr>
            <tr>
                <td>
                    <div id="lblQPopup_Dept_Cd" class="lblCondition">부서</div>
                    <input id="edtQDept_Cd" class="edtNormal" type="text" style="width: 60px;" />
                    <input id="btnQDept_Cd" class="btnIcoPopup" type="button" />
                    <input id="edtQDept_Nm" class="edtDisplay" type="text" disabled="disabled" style="width: 120px;" />
                </td>
                <td>
                    <div id="lblQClass_Div" class="lblCondition">분류구분</div>
                    <select id="cboQClass_Div" class="cboNormal" style="width: 150px;"></select>
                </td>
                <td>
                    <div id="divQConditionValidRank">
                        <div id="lblQValid_Rank" class="lblCondition">임박구분</div>
                        <select id="cboQValid_Rank" class="cboNormal" style="width: 150px;"></select>
                    </div>
                </td>
            </tr>
        </table>
        <!-- 추가 조건 -->
        <table id="ctrAdditionalConditionView" class="ctrAdditionalCondition">
            <tr>
                <td id="tdQDsp_Depart_Cd">
                    <div id="lblQDepart_Cd" class="lblCondition">대분류</div>
                    <select id="cboQDepart_Cd" class="cboNormal" style="width: 210px;"></select>
                </td>
            </tr>
            <tr>
                <td id="tdQDsp_Line_Cd">
                    <div id="lblQLine_Cd" class="lblCondition">중분류</div>
                    <select id="cboQLine_Cd" class="cboNormal" style="width: 210px;"></select>
                </td>
            </tr>
            <tr>
                <td id="tdQDsp_Class_Cd">
                    <div id="lblQClass_Cd" class="lblCondition">소분류</div>
                    <select id="cboQClass_Cd" class="cboNormal" style="width: 210px;"></select>
                </td>
            </tr>
        </table>
    </div>
    <div id="divMasterView">
        <!-- Tab Handler -->
        <ul>
            <li id="divTab1"><a href="#divT1TabSheetView">유통기한임박 상품분석</a></li>
            <li id="divTab2"><a href="#divT2TabSheetView">평균재고회전율</a></li>
        </ul>
        <!-- 유효기간임박 상품분석 -->
        <div id="divT1TabSheetView">
            <table class="ctrTitleBar">
                <tr>
                    <td id="ctrTitle_grdT1Master" class="lblTitle">유효기간임박 상품분석</td>
                    <td id="ctrRows_grdT1Master" class="lblRows">0/0 Rows</td>
                </tr>
            </table>
            <div id="grdT1Master"></div>
        </div>
        <!-- 평균재고회전율 -->
        <div id="divT2TabSheetView">
            <div>
                <table class="ctrTitleBar">
                    <tr>
                        <td id="ctrTitle_grdT2Master" class="lblTitle">평균재고회전율</td>
                        <td id="ctrRows_grdT2Master" class="lblRows">0/0 Rows</td>
                    </tr>
                </table>
                <div id="grdT2Master"></div>
            </div>
        </div>
    </div>
</body>
</html>
