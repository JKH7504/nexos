<!DOCTYPE HTML>
<html lang="ko">
<head>
<!-- ${NEXOS_META} -->
<!-- ${NEXOS_LIBRARY} ${} ${../../../} -->
<!-- PROGRAM CSS/JS -->
<!-- <link rel="stylesheet" href="css/CMC09010E0.css" /> -->
<script src="js/CMC09010E0.js"></script>
</head>
<body>
    <div id="ctrConditionView" class="ctrCondition">
        <table class="ctrInner">
            <tr>
                <td style="width: 325px;">
                    <div id="lblQCenter_Cd" class="lblCondition">물류센터</div>
                    <select id="cboQCenter_Cd" class="cboKey" style="width: 150px;"></select>
                </td>
                <td>
                    <div id="lblQPopup_Bu_Cd" class="lblCondition">사업부</div>
                    <input id="edtQBu_Cd" class="edtKey" type="text" style="width: 60px;" />
                    <input id="btnQBu_Cd" class="btnIcoPopup" type="button" />
                    <input id="edtQBu_Nm" class="edtDisplay" type="text" disabled="disabled" style="width: 120px;" />
                </td>
                <td class="styFillArea styRight">
                    <input id="btnCreate" class="btnImgPopup" type="button" value="작업일자 생성" style="width: 115px;" />
                    <input id="btnCopy" class="btnImgPopup" type="button" value="작업일자 복사" style="width: 115px;" />
                </td>
            </tr>
            <tr>
                <td>
                    <div id="lblQInout_Month" class="lblCondition">작업월</div>
                    <input id="mtpQInout_Month" class="mtpKey" type="text" style="width: 100px;" />
                    <div class="ctrGroupBox">
                        <input id="rgbQYm_Div1" name="rgbQYm_Div" class="rgbNormal" type="radio" value="1" />
                        월별
                        <input id="rgbQYm_Div2" name="rgbQYm_Div" class="rgbNormal" type="radio" value="2" />
                        연도별
                    </div>
                </td>
            </tr>
        </table>
    </div>
    <div id="divMasterView" class="ctrNormal">
        <table class="ctrTitleBar">
            <tr>
                <td id="ctrTitle_grdMaster" class="lblTitle">작업일자</td>
                <td id="ctrRows_grdMaster" class="lblRows">0/0 Rows</td>
            </tr>
        </table>
        <div id="grdMaster"></div>
    </div>
</body>
</html>
