<!DOCTYPE HTML>
<html lang="ko">
<head>
<!-- ${NEXOS_META} -->
<!-- ${NEXOS_LIBRARY} ${} ${../../../} -->
<!-- PROGRAM CSS/JS -->
<!-- <link rel="stylesheet" href="css/PDA_LOC02380E0.css" /> -->
<script src="js/PDA_LOC02380E0.js"></script>
</head>
<body>
    <div id="ctrConditionView" class="ctrCondition">
        <table class="ctrInner">
            <tr>
                <td>
                    <div id="lblQExtra_Work_Div" class="lblCondition">작업구분</div>
                    <div class="ctrGroupBox">
                        <input id="rgbQExtra_Work_Div1" name="rgbQExtra_Work_Div" class="rgbNormal" type="radio" value="%" checked="checked" />
                        전체
                        <input id="rgbQExtra_Work_Div2" name="rgbQExtra_Work_Div" class="rgbNormal" type="radio" value="Y" />
                        작업
                        <input id="rgbQExtra_Work_Div3" name="rgbQExtra_Work_Div" class=" rgbNormal" type="radio" value="N" />
                        미작업
                    </div>
                </td>
                <td class="styFillArea"></td>
                <td>
                    <input id="btnInquiry" class="btnImgInquiry" type="button" value="조회" />
                </td>
            </tr>
        </table>
    </div>
    <!-- 상단그리드 -->
    <div id="ctrMasterView" class="ctrNormal">
        <table class="ctrTitleBar">
            <tr>
                <td id="ctrTitle_grdMaster" class="lblTitle">출고내역</td>
                <td id="ctrRows_grdMaster" class="lblRows">0/0 Rows</td>
            </tr>
        </table>
        <div id="grdMaster"></div>
    </div>
    <div id="ctrMasterInfoView" class="ctrLinearLayout">
        <table class="ctrInner">
            <tr>
                <td>
                    <div id="lblBill_Qty_Title" class="lblKey">정산수량</div>
                </td>
                <td style="width: 35%; padding-right: 25px;">
                    <input id="edtBill_Plt_Qty" class="edtKey" type="text" />
                    <div class="lblDisplay">PLT</div>
                </td>
                <td style="width: 30%; padding-right: 25px;">
                    <input id="edtBill_Box_Qty" class="edtKey" type="text" />
                    <div class="lblDisplay">BOX</div>
                </td>
                <td style="width: 35%; padding-right: 25px;">
                    <input id="edtBill_Ea_Qty" class="edtKey" type="text" />
                    <div class="lblDisplay">EA</div>
                </td>
            </tr>
        </table>
    </div>
    <!-- 하단그리드 -->
    <div id="ctrDetailView" class="ctrNormal">
        <table class="ctrTitleBar">
            <tr>
                <td id="ctrTitle_grdDetail" class="lblTitle">추가작업비 정보</td>
                <td id="ctrRows_grdDetail" class="lblRows">0/0 Rows</td>
            </tr>
        </table>
        <div id="grdDetail"></div>
    </div>
    <div id="ctrDetailInfoView" class="ctrLinearLayout">
        <table class="ctrInner">
            <tr>
                <td>
                    <div id="lblExtra_Work_Qty" class="lblKey">작업수량</div>
                </td>
                <td class="styFillArea" colspan="3">
                    <input id="edtExtra_Work_Qty" class="edtKey" type="text" />
                </td>
            </tr>
        </table>
    </div>
    <!-- 액션버튼 -->
    <table id="ctrActionBar" class="ctrBottomActionBar">
        <tr>
            <td>
                <div id="btnClose" class="btnAction">
                    <span>이전</span>
                </div>
            </td>
            <td>
                <div id="btnCancel" class="btnAction">
                    <span>취소</span>
                </div>
            </td>
            <td class="styFillArea"></td>
            <td>
                <div id="btnSave" class="btnAction">
                    <span>저장</span>
                </div>
            </td>
        </tr>
    </table>
</body>
</html>