<jasperReport xmlns="http://jasperreports.sourceforge.net/jasperreports" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://jasperreports.sourceforge.net/jasperreports http://jasperreports.sourceforge.net/xsd/jasperreport.xsd" name="PAPER_LO_SHORTAGE_EXPECT" pageWidth="842" pageHeight="595" orientation="Landscape" columnWidth="812" leftMargin="15" rightMargin="15" topMargin="15" bottomMargin="15" uuid="c6b45e19-517d-4e89-b49a-9b71a12ff633">
	<property name="ireport.scriptlethandling" value="0"/>
	<property name="ireport.encoding" value="UTF-8"/>
	<property name="ireport.zoom" value="1.5"/>
	<property name="ireport.x" value="39"/>
	<property name="ireport.y" value="0"/>
	<import value="net.sf.jasperreports.engine.*"/>
	<import value="java.util.*"/>
	<import value="net.sf.jasperreports.engine.data.*"/>
	<parameter name="P_SYSDATE" class="java.lang.String" isForPrompting="false">
		<defaultValueExpression><![CDATA["2008-01-01 01:01:01"]]></defaultValueExpression>
	</parameter>
	<parameter name="P_USER_ID" class="java.lang.String" isForPrompting="false"/>
	<parameter name="P_USER_NM" class="java.lang.String" isForPrompting="false"/>
	<parameter name="P_CENTER_CD" class="java.lang.String"/>
	<parameter name="P_CENTER_CD_F" class="java.lang.String" isForPrompting="false"/>
	<parameter name="P_BU_CD" class="java.lang.String"/>
	<parameter name="P_BU_CD_F" class="java.lang.String" isForPrompting="false"/>
	<parameter name="P_ORDER_DATE1" class="java.lang.String"/>
	<parameter name="P_ORDER_DATE2" class="java.lang.String"/>
	<parameter name="P_INOUT_CD" class="java.lang.String"/>
	<parameter name="P_INOUT_CD_F" class="java.lang.String" isForPrompting="false"/>
	<parameter name="P_DELIVERY_CD" class="java.lang.String"/>
	<parameter name="P_DELIVERY_CD_F" class="java.lang.String" isForPrompting="false"/>
	<parameter name="P_ITEM_CD" class="java.lang.String"/>
	<parameter name="P_ITEM_CD_F" class="java.lang.String" isForPrompting="false"/>
	<parameter name="P_ITEM_NM" class="java.lang.String"/>
	<parameter name="P_ITEM_NM_F" class="java.lang.String" isForPrompting="false"/>
	<parameter name="FSUBTOTAL" class="java.lang.String" isForPrompting="false"/>
	<parameter name="FTOTAL" class="java.lang.String" isForPrompting="false"/>
	<parameter name="HPRINTTIME" class="java.lang.String" isForPrompting="false"/>
	<parameter name="HPUSER" class="java.lang.String" isForPrompting="false"/>
	<parameter name="HPAGE" class="java.lang.String" isForPrompting="false"/>
	<parameter name="P_REPORT_TITLE_NM" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[WITH T_ORDER AS
         (--<1. 대상 예정전표 내역>-----------------------------------------------------------------
          SELECT M1.CENTER_CD
                ,M1.BU_CD
                ,M1.ORDER_DATE
                ,M1.ORDER_NO
                ,M1.BACKORDER_YN
                ,M1.INOUT_CD
                ,M1.CUST_CD
                ,M1.DELIVERY_CD
                ,M1.RDELIVERY_CD
                ,M1.BU_NO
                ,M2.LINE_NO
                ,M2.BRAND_CD
                ,M2.ITEM_CD
                ,M2.ITEM_STATE
                ,M2.ITEM_LOT
                ,C1.QTY_IN_BOX
                ,M2.ORDER_QTY
                ,WF.GET_B_BOX(M2.ORDER_QTY, C1.QTY_IN_BOX) AS ORDER_BOX
                ,WF.GET_B_EA (M2.ORDER_QTY, C1.QTY_IN_BOX) AS ORDER_EA
            FROM (
                  SELECT M1.CENTER_CD
                        ,M1.BU_CD
                        ,M1.ORDER_DATE
                        ,M1.ORDER_NO
                        ,M1.BACKORDER_YN
                        ,M1.INOUT_CD
                        ,M1.CUST_CD
                        ,M1.DELIVERY_CD
                        ,M1.RDELIVERY_CD
                        ,M1.BU_NO
                    FROM LO010NM M1
                         JOIN LO010ND M2 ON M2.CENTER_CD   = M1.CENTER_CD
                                        AND M2.BU_CD       = M1.BU_CD
                                        AND M2.ORDER_DATE  = M1.ORDER_DATE
                                        AND M2.ORDER_NO    = M1.ORDER_NO
                         JOIN CMCODE  C1 ON C1.COMMON_CD     = M1.INOUT_CD
                                        AND C1.COMMON_GRP    = WF.GET_COMMON_GRP('INOUT_CD')
                                        AND C1.ATTR01_CD    IN ('D1' ,'D2')
                         JOIN CMITEM  T1 ON T1.BRAND_CD    = M2.BRAND_CD
                                        AND T1.ITEM_CD     = M2.ITEM_CD
                   WHERE M2.OUTBOUND_STATE   = WD.C_STATE_ORDER
                     AND M1.HOLD_YN          = WD.C_NO
                     AND ROWNUM < 100
                   GROUP BY M1.CENTER_CD
                           ,M1.BU_CD
                           ,M1.ORDER_DATE
                           ,M1.ORDER_NO
                           ,M1.BACKORDER_YN
                           ,M1.INOUT_CD
                           ,M1.BU_NO
                           ,M1.CUST_CD
                           ,M1.DELIVERY_CD
                           ,M1.RDELIVERY_CD
                 ) M1
                 JOIN LO010ND M2 ON M2.CENTER_CD   = M1.CENTER_CD
                                AND M2.BU_CD       = M1.BU_CD
                                AND M2.ORDER_DATE  = M1.ORDER_DATE
                                AND M2.ORDER_NO    = M1.ORDER_NO
                 JOIN CMITEM  C1 ON C1.BRAND_CD    = M2.BRAND_CD
                                AND C1.ITEM_CD     = M2.ITEM_CD
         )
      ,T_STOCK AS
         (--<2. 상품별 재고 내역>-------------------------------------------------------------------
          SELECT --+ INDEX(LS010NM LS010NM_IDX01)
                 BRAND_CD
                ,ITEM_CD
                ,ITEM_STATE
                ,ITEM_LOT
                ,WF.GET_MINUS_VAL(SUM(STOCK_QTY), SUM(OUT_WAIT_QTY)) AS STOCK_QTY
            FROM LS010NM
           WHERE HOLD_YN     = WD.C_NO
             AND (BRAND_CD ,ITEM_CD ,ITEM_STATE ,ITEM_LOT)
              IN (SELECT BRAND_CD ,ITEM_CD ,ITEM_STATE ,ITEM_LOT
                    FROM T_ORDER
                   GROUP BY BRAND_CD
                           ,ITEM_CD
                           ,ITEM_STATE
                           ,ITEM_LOT
                 )
           GROUP BY BRAND_CD
                   ,ITEM_CD
                   ,ITEM_STATE
                   ,ITEM_LOT
         )
      ,T_WAIT AS
         (--<3. 상품별 입출고대기 내역>-------------------------------------------------------------
          SELECT L1.BRAND_CD
                ,L1.ITEM_CD
                ,L1.ITEM_STATE
                ,L1.ITEM_LOT
                ,SUM(L1.OUTWAIT_QTY) AS OUTWAIT_QTY
                ,SUM(L1.INORDER_QTY) AS INORDER_QTY
                ,SUM(L1.INWAIT_QTY)  AS INWAIT_QTY
            FROM (--<3.1. 출고대기 내역>------------------------------------------------------------
                  SELECT BRAND_CD
                        ,ITEM_CD
                        ,ITEM_STATE
                        ,ITEM_LOT
                        ,SUM(ENTRY_QTY) AS OUTWAIT_QTY
                        ,0              AS INORDER_QTY
                        ,0              AS INWAIT_QTY
                    FROM LO020ND
                   WHERE OUTBOUND_STATE  = WD.C_STATE_ENTRY
                     AND ROWNUM < 100
                   GROUP BY BRAND_CD
                           ,ITEM_CD
                           ,ITEM_STATE
                           ,ITEM_LOT
                   UNION ALL
                  --<3.4. 입고예정 내역>------------------------------------------------------------
                  SELECT BRAND_CD
                        ,ITEM_CD
                        ,ITEM_STATE
                        ,ITEM_LOT
                        ,0                                AS OUTWAIT_QTY
                        ,SUM(WF.GET_MINUS_VAL(ORDER_QTY
                                             ,ENTRY_QTY)) AS INORDER_QTY
                        ,0                                AS INWAIT_QTY
                    FROM LI010ND
                   WHERE ORDER_DATE    BETWEEN TRUNC(SYSDATE)
                                           AND TRUNC(SYSDATE) + TO_NUMBER(NVL('0'/*V_POLICY_LO320*/, '0'))      --입고예정일자 범위 지정
                     AND INBOUND_STATE       = WD.C_STATE_ORDER
                     AND '2'/*V_POLICY_LO310*/      = WD.C_POLICY_VAL_3                                    --가입고 적용기준 - 3  입고예정포함
                   GROUP BY BRAND_CD
                           ,ITEM_CD
                           ,ITEM_STATE
                           ,ITEM_LOT
                  UNION ALL
                  --<3.5. 입고등록 내역>------------------------------------------------------------
                  SELECT BRAND_CD
                        ,ITEM_CD
                        ,ITEM_STATE
                        ,ITEM_LOT
                        ,0              AS OUTWAIT_QTY
                        ,0              AS INORDER_QTY
                        ,SUM(ENTRY_QTY) AS INWAIT_QTY
                    FROM LI020ND
                   WHERE INBOUND_DATE   >= TRUNC(SYSDATE)
                     AND INBOUND_STATE  IN (WD.C_STATE_ENTRY
                                           ,WD.C_STATE_DIRECTIONS)
                     AND '2'/*V_POLICY_LO310*/ IN (WD.C_POLICY_VAL_2
                                           ,WD.C_POLICY_VAL_3)                                      --가입고 적용기준 - 2  입고미확정포함, 3  입고예정포함
                   GROUP BY BRAND_CD
                           ,ITEM_CD
                           ,ITEM_STATE
                           ,ITEM_LOT
                 ) L1
          GROUP BY L1.BRAND_CD
                  ,L1.ITEM_CD
                  ,L1.ITEM_STATE
                  ,L1.ITEM_LOT
         )
      ,T_SHORTAGEITEM AS
         (
          --<L3. 가용재고부족 제품만 추출>----------------------------------------------------------
          SELECT L2.*
                ,WF.GET_MINUS_VAL(L2.STOCK_QTY + L2.INORDER_QTY + L2.INWAIT_QTY
                                 ,L2.OUTWAIT_QTY) AS PSTOCK_QTY
            FROM (--<L2. 가용재고 계산용 수량 계산>-------------------------------------------------
                  SELECT L1.CENTER_CD
                        ,L1.BU_CD
                        ,L1.BRAND_CD
                        ,L1.ITEM_CD
                        ,L1.ITEM_STATE
                        ,L1.ITEM_LOT
                        ,L1.ORDER_QTY
                        ,NVL(M1.STOCK_QTY,   0) AS STOCK_QTY
                        ,NVL(M2.OUTWAIT_QTY, 0) AS OUTWAIT_QTY
                        ,NVL(M2.INORDER_QTY, 0) AS INORDER_QTY
                        ,NVL(M2.INWAIT_QTY,  0) AS INWAIT_QTY
                    FROM (--<L1. 상품별 출고예정수량 합계 검색>-------------------------------------
                          SELECT CENTER_CD
                                ,BU_CD
                                ,BRAND_CD
                                ,ITEM_CD
                                ,ITEM_STATE
                                ,ITEM_LOT
                                ,SUM(ORDER_QTY) AS ORDER_QTY
                            FROM T_ORDER
                           GROUP BY CENTER_CD
                                   ,BU_CD
                                   ,BRAND_CD
                                   ,ITEM_CD
                                   ,ITEM_STATE
                                   ,ITEM_LOT
                         ) L1
                    LEFT JOIN T_STOCK M1 ON M1.BRAND_CD   = L1.BRAND_CD
                                        AND M1.ITEM_CD    = L1.ITEM_CD
                                        AND M1.ITEM_STATE = L1.ITEM_STATE
                                        AND M1.ITEM_LOT   = L1.ITEM_LOT
                    LEFT JOIN T_WAIT  M2 ON M1.BRAND_CD   = L1.BRAND_CD
                                        AND M2.ITEM_CD    = L1.ITEM_CD
                                        AND M2.ITEM_STATE = L1.ITEM_STATE
                                        AND M2.ITEM_LOT   = L1.ITEM_LOT
                 ) L2
           WHERE L2.ORDER_QTY > (L2.STOCK_QTY - L2.OUTWAIT_QTY + L2.INORDER_QTY + L2.INWAIT_QTY)
         )
      ,T_ASSIGNEDBOX AS
         (--<L1. 상품/오더별 할당 출고박스 결정>----------------------------------------------------
          SELECT L1.BRAND_CD
                ,L1.ITEM_CD
                ,L1.ITEM_STATE
                ,L1.ITEM_LOT
                ,L1.QTY_IN_BOX
                ,L1.CENTER_CD
                ,L1.BU_CD
                ,L1.CUST_CD
                ,L1.DELIVERY_CD
                ,L1.RDELIVERY_CD
                ,L1.INOUT_CD
                ,L1.BU_NO
                ,L1.ORDER_DATE
                ,L1.ORDER_NO
                ,L1.BACKORDER_YN
                ,L1.LINE_NO
                ,L1.ORDER_QTY
                ,SUM(L1.QTY_IN_BOX) AS ASSIGNED_QTY
            FROM (--<L0. 상품/오더별로 출고박스 수량으로 데이터 복제 후 박스 수량 누계>-------------
                  SELECT M1.BRAND_CD
                        ,M1.ITEM_CD
                        ,M1.ITEM_STATE
                        ,M1.ITEM_LOT
                        ,M2.QTY_IN_BOX
                        ,WF.GET_B_BOX(M1.PSTOCK_QTY, M2.QTY_IN_BOX) AS PSTOCK_BOX
                        ,M2.CENTER_CD
                        ,M2.BU_CD
                        ,M2.CUST_CD
                        ,M2.DELIVERY_CD
                        ,M2.RDELIVERY_CD
                        ,M2.BU_NO
                        ,M2.INOUT_CD
                        ,M2.ORDER_DATE
                        ,M2.ORDER_NO
                        ,M2.BACKORDER_YN
                        ,M2.LINE_NO
                        ,M2.ORDER_QTY
                        ,SUM(1) OVER(PARTITION BY M1.BRAND_CD
                                                 ,M1.ITEM_CD
                                                 ,M1.ITEM_STATE
                                                 ,M1.ITEM_LOT
                                         ORDER BY C1.NO
                                                 ,M2.ORDER_QTY
                                                 ,M2.DELIVERY_CD
                                                 ,M2.ORDER_DATE
                                                 ,M2.ORDER_NO
                                                 ,M2.LINE_NO) AS ACC_ORDER_BOX
                    FROM T_SHORTAGEITEM M1
                         JOIN T_ORDER   M2 ON M2.CENTER_CD   = M1.CENTER_CD
                                          AND M2.BU_CD       = M1.BU_CD
                                          AND M2.BRAND_CD    = M1.BRAND_CD
                                          AND M2.ITEM_CD     = M1.ITEM_CD
                                          AND M2.ITEM_STATE  = M1.ITEM_STATE
                                          AND M2.ITEM_LOT    = M1.ITEM_LOT
                         JOIN CSNO      C1 ON C1.NO         <= M2.ORDER_BOX
                   WHERE '3'/*V_POLICY_LO220*/ = WD.C_POLICY_VAL_2
                 ) L1
           WHERE L1.ACC_ORDER_BOX <= L1.PSTOCK_BOX                                                  --가용재고 박스수량으로 제한
           GROUP BY L1.BRAND_CD
                   ,L1.ITEM_CD
                   ,L1.ITEM_STATE
                   ,L1.ITEM_LOT
                   ,L1.QTY_IN_BOX
                   ,L1.CENTER_CD
                   ,L1.BU_CD
                   ,L1.CUST_CD
                   ,L1.DELIVERY_CD
                   ,L1.RDELIVERY_CD
                   ,L1.INOUT_CD
                   ,L1.BU_NO
                   ,L1.ORDER_DATE
                   ,L1.ORDER_NO
                   ,L1.BACKORDER_YN
                   ,L1.LINE_NO
                   ,L1.ORDER_QTY
         )
  SELECT L6.ITEM_CD
        ,T1.ITEM_NM
        ,WF.DSP_COMBO(L6.ITEM_STATE ,C2.COMMON_NM) AS ITEM_STATE_F
        ,T1.ITEM_SPEC
        ,L6.ITEM_LOT
        ,L6.BRAND_CD
        ,B1.BRAND_NM
        ,L6.DELIVERY_CD
        ,D1.DELIVERY_NM
        ,L6.RDELIVERY_CD
        ,D2.DELIVERY_NM AS RDELIVERY_NM
        ,L6.BU_NO
        ,C1.COMMON_NM AS INOUT_NM
        ,L6.ORDER_DATE
        ,L6.ORDER_NO
        ,L6.BACKORDER_YN
        ,L6.LINE_NO
        ,T1.QTY_IN_BOX
        ,T1.BOX_WEIGHT
        ,L6.ORDER_QTY                                                                               --예정수량
        ,L6.PRE_ADJUST_QTY                                                                          --출고가능수량
        ,L6.SHORTAGE_QTY                                                                            --미출예상수량

        ,L6.CENTER_CD
        ,L6.BU_CD
        ,L6.CUST_CD
        ,L6.INOUT_CD
        ,L6.ITEM_STATE

        ,WF.GET_B_BOX (L6.ORDER_QTY      ,T1.QTY_IN_BOX)                AS ORDER_BOX
        ,WF.GET_B_EA  (L6.ORDER_QTY      ,T1.QTY_IN_BOX)                AS ORDER_EA
        ,WF.GET_B_BOX (L6.PRE_ADJUST_QTY ,T1.QTY_IN_BOX)                AS PRE_ADJUST_BOX
        ,WF.GET_B_EA  (L6.PRE_ADJUST_QTY ,T1.QTY_IN_BOX)                AS PRE_ADJUST_EA
        ,WF.GET_B_BOX (L6.SHORTAGE_QTY   ,T1.QTY_IN_BOX)                AS SHORTAGE_BOX
        ,WF.GET_B_EA  (L6.SHORTAGE_QTY   ,T1.QTY_IN_BOX)                AS SHORTAGE_EA

        ,WF.GET_WEIGHT(L6.ORDER_QTY      ,T1.QTY_IN_BOX ,T1.BOX_WEIGHT) AS ORDER_WEIGHT
        ,WF.GET_WEIGHT(L6.PRE_ADJUST_QTY ,T1.QTY_IN_BOX ,T1.BOX_WEIGHT) AS PRE_ADJUST_WEIGHT
        ,WF.GET_WEIGHT(L6.SHORTAGE_QTY   ,T1.QTY_IN_BOX ,T1.BOX_WEIGHT) AS SHORTAGE_WEIGHT
        ,WF.GET_LPAD(L6.BRAND_CD        ,10 ,'0')
      || WF.GET_LPAD(L6.ITEM_CD         ,30 ,'0')
      || L6.ITEM_STATE
      || WF.GET_LPAD(L6.ITEM_LOT        ,20 ,'0')                       AS ITEM_GRP
    FROM (
          --<L5. 부족재고 자동할당>-----------------------------------------------------------------
          SELECT L5.BRAND_CD
                ,L5.ITEM_CD
                ,L5.ITEM_STATE
                ,L5.ITEM_LOT
                ,L5.CENTER_CD
                ,L5.BU_CD
                ,L5.CUST_CD
                ,L5.DELIVERY_CD
                ,L5.RDELIVERY_CD
                ,L5.BU_NO
                ,L5.INOUT_CD
                ,L5.ORDER_DATE
                ,L5.ORDER_NO
                ,L5.BACKORDER_YN
                ,L5.LINE_NO
                ,L5.ORDER_QTY
                ,L5.ASSIGN_QTY AS PRE_ADJUST_QTY
                ,L5.ORDER_QTY - L5.ASSIGN_QTY AS SHORTAGE_QTY
            FROM (
                  SELECT L4.*
                         --<출고등록시 부족재고 조정기준(LO220)>------------------------------------
                        ,CASE '3'/*V_POLICY_LO220*/
                              --<1 - 일괄 균등분배 조정>--------------------------------------------
                              --  출고예정수량의 백분율 * 가용재고
                              WHEN WD.C_POLICY_VAL_1 THEN
                                   CASE WHEN L4.ORDER_QTY < ROUND(L4.ORDER_QTY / L4.TOT_ORDER_QTY * L4.PSTOCK_QTY) THEN
                                             L4.ORDER_QTY
                                        ELSE ROUND(L4.ORDER_QTY / L4.TOT_ORDER_QTY * L4.PSTOCK_QTY)
                                   END
                              --<3 - 전표등록순 우선조정(전표 전체미등록)>--------------------------
                              WHEN WD.C_POLICY_VAL_3 THEN
                                   CASE --A. 누적예정수량 <= 가용재고 ==> 예정수량
                                        WHEN L4.ACC_ORDER_QTY <= L4.PSTOCK_QTY THEN L4.ORDER_QTY
                                        ELSE --B. 이전 누적 예정수량 < 가용재고 AND 가용재고 < 누적 예정수량 ==> 가용재고 - 이전 누적 예정수량
                                             CASE WHEN (L4.ACC_ORDER_QTY - L4.ORDER_QTY) < L4.PSTOCK_QTY THEN L4.PSTOCK_QTY - (L4.ACC_ORDER_QTY - L4.ORDER_QTY)
                                                  ELSE 0
                                             END
                                   END
                         END AS ASSIGN_QTY
                    FROM (--<L4. 전표별 예정수량 검색(정렬) 및 누적 예정수량 검색>------------------
                          SELECT L3.BRAND_CD
                                ,L3.ITEM_CD
                                ,L3.ITEM_STATE
                                ,L3.ITEM_LOT
                                ,L3.ORDER_QTY  AS TOT_ORDER_QTY
                                ,L3.PSTOCK_QTY
                                ,M1.CENTER_CD
                                ,M1.BU_CD
                                ,M1.CUST_CD
                                ,M1.DELIVERY_CD
                                ,M1.RDELIVERY_CD
                                ,M1.BU_NO
                                ,M1.INOUT_CD
                                ,M1.ORDER_DATE
                                ,M1.ORDER_NO
                                ,M1.BACKORDER_YN
                                ,M1.LINE_NO
                                ,M1.ORDER_QTY
                                 --<출고등록시 부족재고 조정기준(LO220)>----------------------------
                                 --   1 - 일괄 균등분배 조정은 예정수량이 큰 순으로
                                 --   2 - 전표등록순 우선조정은 전표일자, 전표번호 순으로 정렬
                                ,SUM(M1.ORDER_QTY) OVER(PARTITION BY L3.BRAND_CD
                                                                    ,L3.ITEM_CD
                                                                    ,L3.ITEM_STATE
                                                                    ,L3.ITEM_LOT
                                                            ORDER BY DECODE('3'/*V_POLICY_LO220*/, WD.C_POLICY_VAL_1, M1.ORDER_QTY * -1, 1)
                                                                    ,M1.ORDER_DATE
                                                                    ,M1.ORDER_NO
                                                                    ,M1.LINE_NO) AS ACC_ORDER_QTY
                            FROM T_SHORTAGEITEM L3
                                 JOIN T_ORDER   M1 ON M1.CENTER_CD   = L3.CENTER_CD
                                                  AND M1.BU_CD       = L3.BU_CD
                                                  AND M1.BRAND_CD    = L3.BRAND_CD
                                                  AND M1.ITEM_CD     = L3.ITEM_CD
                                                  AND M1.ITEM_STATE  = L3.ITEM_STATE
                                                  AND M1.ITEM_LOT    = L3.ITEM_LOT
                           WHERE '3'/*V_POLICY_LO220*/ IN (WD.C_POLICY_VAL_1, WD.C_POLICY_VAL_3)
                           ORDER BY L3.BRAND_CD
                                   ,L3.ITEM_CD
                                   ,L3.ITEM_STATE
                                   ,L3.ITEM_LOT
                                   ,DECODE('3'/*V_POLICY_LO220*/, WD.C_POLICY_VAL_1, M1.ORDER_QTY * -1, 1)
                                   ,M1.ORDER_DATE
                                   ,M1.ORDER_NO
                                   ,M1.LINE_NO
                         ) L4
                 ) L5
          UNION ALL
          SELECT L4.BRAND_CD
                ,L4.ITEM_CD
                ,L4.ITEM_STATE
                ,L4.ITEM_LOT
                ,L4.CENTER_CD
                ,L4.BU_CD
                ,L4.CUST_CD
                ,L4.DELIVERY_CD
                ,L4.RDELIVERY_CD
                ,L4.BU_NO
                ,L4.INOUT_CD
                ,L4.ORDER_DATE
                ,L4.ORDER_NO
                ,L4.BACKORDER_YN
                ,L4.LINE_NO
                ,L4.ORDER_QTY
                ,L4.ASSIGN_QTY AS PRE_ADJUST_QTY
                ,L4.ORDER_QTY - L4.ASSIGN_QTY AS SHORTAGE_QTY
            FROM (
                  SELECT L3.BRAND_CD
                        ,L3.ITEM_CD
                        ,L3.ITEM_STATE
                        ,L3.ITEM_LOT
                        ,L3.CENTER_CD
                        ,L3.BU_CD
                        ,L3.CUST_CD
                        ,L3.DELIVERY_CD
                        ,L3.RDELIVERY_CD
                        ,L3.BU_NO
                        ,L3.INOUT_CD
                        ,L3.ORDER_DATE
                        ,L3.ORDER_NO
                        ,L3.BACKORDER_YN
                        ,L3.LINE_NO
                        ,L3.ORDER_QTY
                        ,SUM(L3.ASSIGNED_QTY) AS ASSIGN_QTY
                    FROM (
                          SELECT L2.BRAND_CD
                                ,L2.ITEM_CD
                                ,L2.ITEM_STATE
                                ,L2.ITEM_LOT
                                ,L2.CENTER_CD
                                ,L2.BU_CD
                                ,L2.CUST_CD
                                ,L2.DELIVERY_CD
                                ,L2.RDELIVERY_CD
                                ,L2.BU_NO
                                ,L2.INOUT_CD
                                ,L2.ORDER_DATE
                                ,L2.ORDER_NO
                                ,L2.BACKORDER_YN
                                ,L2.LINE_NO
                                ,L2.ORDER_QTY
                                ,L2.ASSIGNED_QTY
                            FROM T_ASSIGNEDBOX L2
                          UNION ALL
                          SELECT L2.BRAND_CD
                                ,L2.ITEM_CD
                                ,L2.ITEM_STATE
                                ,L2.ITEM_LOT
                                ,L2.CENTER_CD
                                ,L2.BU_CD
                                ,L2.CUST_CD
                                ,L2.DELIVERY_CD
                                ,L2.RDELIVERY_CD
                                ,L2.BU_NO
                                ,L2.INOUT_CD
                                ,L2.ORDER_DATE
                                ,L2.ORDER_NO
                                ,L2.BACKORDER_YN
                                ,L2.LINE_NO
                                ,L2.ORDER_QTY
                                ,SUM(1) AS ASSIGNED_QTY
                            FROM (
                                  SELECT L1.*
                                        ,SUM(1) OVER(PARTITION BY L1.BRAND_CD
                                                                 ,L1.ITEM_CD
                                                                 ,L1.ITEM_STATE
                                                                 ,L1.ITEM_LOT
                                                         ORDER BY C1.NO
                                                                 ,L1.ORDER_QTY
                                                                 ,L1.DELIVERY_CD
                                                                 ,L1.ORDER_DATE
                                                                 ,L1.ORDER_NO
                                                                 ,L1.LINE_NO) AS ACC_ORDER_QTY
                                    FROM (
                                          SELECT M1.BRAND_CD
                                                ,M1.ITEM_CD
                                                ,M1.ITEM_STATE
                                                ,M1.ITEM_LOT
                                                ,M1.PSTOCK_QTY
                                                - NVL(SUM(M3.ASSIGNED_QTY) OVER(PARTITION BY M1.BRAND_CD
                                                                                            ,M1.ITEM_CD
                                                                                            ,M1.ITEM_STATE
                                                                                            ,M1.ITEM_LOT)
                                                     ,0) AS REMAIN_PSTOCK_QTY
                                                ,M2.CENTER_CD
                                                ,M2.BU_CD
                                                ,M2.CUST_CD
                                                ,M2.DELIVERY_CD
                                                ,M2.RDELIVERY_CD
                                                ,M2.BU_NO
                                                ,M2.INOUT_CD
                                                ,M2.ORDER_DATE
                                                ,M2.ORDER_NO
                                                ,M2.BACKORDER_YN
                                                ,M2.LINE_NO
                                                ,M2.ORDER_QTY
                                                ,NVL(M3.ASSIGNED_QTY, 0)                AS ASSIGNED_QTY
                                                ,M2.ORDER_QTY - NVL(M3.ASSIGNED_QTY, 0) AS REMAIN_ORDER_QTY
                                            FROM T_SHORTAGEITEM M1
                                                 JOIN T_ORDER       M2 ON M2.CENTER_CD   = M1.CENTER_CD
                                                                      AND M2.BU_CD       = M1.BU_CD
                                                                      AND M2.BRAND_CD    = M1.BRAND_CD
                                                                      AND M2.ITEM_CD     = M1.ITEM_CD
                                                                      AND M2.ITEM_STATE  = M1.ITEM_STATE
                                                                      AND M2.ITEM_LOT    = M1.ITEM_LOT
                                            LEFT JOIN T_ASSIGNEDBOX M3 ON M3.CENTER_CD   = M2.CENTER_CD
                                                                      AND M3.BU_CD       = M2.BU_CD
                                                                      AND M3.ORDER_DATE  = M2.ORDER_DATE
                                                                      AND M3.ORDER_NO    = M2.ORDER_NO
                                                                      AND M3.LINE_NO     = M2.LINE_NO
                                           WHERE '3'/*V_POLICY_LO220*/ = WD.C_POLICY_VAL_2
                                         ) L1
                                         JOIN CSNO C1 ON C1.NO <= L1.REMAIN_ORDER_QTY
                                   WHERE L1.REMAIN_PSTOCK_QTY > 0
                                     AND L1.REMAIN_ORDER_QTY  > 0
                                ) L2
                           WHERE L2.ACC_ORDER_QTY <= L2.REMAIN_PSTOCK_QTY                           --가용재고수량 제한
                           GROUP BY L2.BRAND_CD
                                   ,L2.ITEM_CD
                                   ,L2.ITEM_STATE
                                   ,L2.ITEM_LOT
                                   ,L2.CENTER_CD
                                   ,L2.BU_CD
                                   ,L2.CUST_CD
                                   ,L2.DELIVERY_CD
                                   ,L2.RDELIVERY_CD
                                   ,L2.INOUT_CD
                                   ,L2.BU_NO
                                   ,L2.ORDER_DATE
                                   ,L2.ORDER_NO
                                   ,L2.BACKORDER_YN
                                   ,L2.LINE_NO
                                   ,L2.ORDER_QTY
                         ) L3
                   GROUP BY L3.BRAND_CD
                           ,L3.ITEM_CD
                           ,L3.ITEM_STATE
                           ,L3.ITEM_LOT
                           ,L3.CENTER_CD
                           ,L3.BU_CD
                           ,L3.CUST_CD
                           ,L3.DELIVERY_CD
                           ,L3.RDELIVERY_CD
                           ,L3.INOUT_CD
                           ,L3.BU_NO
                           ,L3.ORDER_DATE
                           ,L3.ORDER_NO
                           ,L3.BACKORDER_YN
                           ,L3.LINE_NO
                           ,L3.ORDER_QTY
                 ) L4
         ) L6
         JOIN CMCODE     C1 ON C1.COMMON_CD     = L6.INOUT_CD
                           AND C1.COMMON_GRP    = WF.GET_COMMON_GRP('INOUT_CD')
                           AND C1.ATTR01_CD    IN ('D1' ,'D2')
         JOIN CMCODE     C2 ON C2.COMMON_CD     = L6.ITEM_STATE
                           AND C2.COMMON_GRP    = WF.GET_COMMON_GRP('ITEM_STATE')
         JOIN CMDELIVERY D1 ON D1.CUST_CD     = L6.CUST_CD
                           AND D1.DELIVERY_CD = L6.DELIVERY_CD
         JOIN CMDELIVERY D2 ON D2.CUST_CD     = L6.CUST_CD
                           AND D2.DELIVERY_CD = L6.RDELIVERY_CD
         JOIN CMBRAND    B1 ON B1.BRAND_CD    = L6.BRAND_CD
         JOIN CMITEM     T1 ON T1.BRAND_CD    = L6.BRAND_CD
                           AND T1.ITEM_CD     = L6.ITEM_CD
   WHERE L6.SHORTAGE_QTY       > 0
     AND ROWNUM < 100
   ORDER BY ITEM_GRP
           ,DELIVERY_CD
           ,INOUT_CD
           ,ORDER_DATE
           ,ORDER_NO
           ,LINE_NO]]>
	</queryString>
